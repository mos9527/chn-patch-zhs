# THIS FILE IS AUTOMATICALLY GENERATED BY generate_po.py.
                     
msgid "0:衆議院議員、猪鼻康三と、天成神光会教祖、[ruby-base]倉持[ruby-text-start]くらもち[ruby-text-end][ruby-base]雄大[ruby-text-start]ゆうだい[ruby-text-end]は、目の前で不気味な唸りを上げる巨大な装置――ノアⅡを見て、満足げにうなずいた。[%p]"
msgstr "0:众议院议员猪鼻康三与天成神光会会长——[ruby-base]仓持。[ruby-text-start]仓持。[ruby-text-end][ruby-base]仓持。[ruby-text-start]雄大[ruby-text-end]看着眼前发出诡异低鸣的巨大装置——诺亚Ⅱ，她满意地点了点头。[%p]"

msgid "100:[name]猪鼻[line]「５０００人を殺すと言い出したときは、どうなることかと思ったが」[%p]"
msgstr "100:[name]猪鼻[line]「当你说要杀掉五千个人的时候，我还以为会怎么样呢。」[%p]"

msgid "200:[name]倉持[line]「調整にはずいぶんと骨が折れたよ。特に、３００人委員会に勘付かれないようにするのはね」[%p]"
msgstr "200:[name]仓持[line]「调整起来可是相当费力呢，尤其是要瞒过300人委员会的眼睛。」[%p]"

msgid "300:[name]野呂瀬[line]「ご協力に感謝します」[%p]"
msgstr "300:[name]野吕濑[line]「感谢您的协助。」[%p]"

msgid "400:野呂瀬が、わずかに頭を下げる。[%p]"
msgstr "400:野吕濑微微低头致意。[%p]"

msgid "500:[name]猪鼻[line]「だがこれが完成した今、５０００人などちっぽけな数字でしかなくなった」[%p]"
msgstr "500:[name]猪鼻[line]「不过，如今这东西已经完成了。五千这个数字变得微不足道。」[%p]"

msgid "600:[name]倉持[line]「早くも影響が目に見えて出てきているよ。人々の家畜化が始まっている」[%p]"
msgstr "600:[name]仓持[line]「影响很快就显现出来了，人们开始变成家畜了。」[%p]"

msgid "700:[name]猪鼻[line]「これで、いよいよ我らが世界を手に入れる日も現実のものとなったわけだ」[%p]"
msgstr "700:[name]仓持[line]「这么一来，我们得到世界的那一天终于成为现实了。」[%p]"

msgid "800:猪鼻と倉持は、互いに顔を見合わせて、ニヤリと不穏な笑みを浮かべた。[%p]"
msgstr "800:仓持与猪鼻彼此相视，咧嘴露出危险的笑容。[%p]"

msgid "900:それを、野呂瀬は眉ひとつ動かさずに見つめる。[%p]"
msgstr "900:野吕濑则是连眉头都不动一下地看着她。[%p]"

msgid "1000:[name]猪鼻[line]「しかし野呂瀬よ。貴様、なかなか興味深い趣味を持っているようじゃないか」[%p]"
msgstr "1000:[name]猪鼻[line]「不过，野吕濑啊，你似乎有着相当有趣嗜好呢。」[%p]"

msgid "1100:[name]野呂瀬[line]「なんのことでしょう」[%p]"
msgstr "1100:[name]猪鼻[line]「您在说什么？」[%p]"

msgid "1200:猪鼻は、卑しそうな含み笑いをして、[linebreak]ノアⅡの傍らに立てられている“オブジェ”へと顎をしゃくった。[%p]"
msgstr "1200:猪鼻露出猥亵的笑容。[linebreak]他用下巴指了指立在诺亚Ⅱ旁边的「装置」。[%p]"

msgid "1300:[name]猪鼻[line]「なんとも芸術性に溢れるオブジェだ。ぜひワシにもひとつ、用意してもらいたいものだな」[%p]"
msgstr "1300:[name]猪鼻[line]猪鼻[%p]"

msgid "1400:[name]倉持[line]「素材ならば、我が教団の信者の中から選りすぐりを提供してもよい」[%p]"
msgstr "1400:[name]仓持[line]「如果需要材料，可以从我们教团信徒当中挑选出最优秀的提供给你。」[%p]"

msgid "1500:[name]猪鼻[line]「くく、倉持先生は気前がいい」[%p]"
msgstr "1500:[name]猪鼻[line]「呵呵，仓持老师真是大方啊。」[%p]"

msgid "1600:[name]野呂瀬[line]「……あれは、餌ですよ」[%p]"
msgstr "1600:[name]野吕濑[line]「……那是诱饵哦。」[%p]"

msgid "1700:[name]猪鼻[line]「餌？」[%p]"
msgstr "1700:[name]野吕濑[line]「诱饵？」[%p]"

msgid "1800:[name]野呂瀬[line]「ノアⅡをより完璧なものにするための」[%p]"
msgstr "1800:[name]野吕濑[line]「为了使诺亚Ⅱ更臻完美。」[%p]"

msgid "1900:[name]猪鼻[line]「まあ、そういうことにしておこう」[%p]"
msgstr "1900:[name]野吕濑[line]「嗯，就当作是这样吧。」[%p]"

msgid "2000:[name]倉持[line]「それより、ノアⅡが具体的にどれほどのことができるのか、実演してもらいたいところだね」[%p]"
msgstr "2000:[name]仓持[line]「比起这个，诺亚Ⅱ具体来说能办到什么程度的事情呢？我倒希望你能实际演练一下。」[%p]"

msgid "2100:[name]猪鼻[line]「それはいい。早速やって見せたまえ」[%p]"
msgstr "2100:[name]野猪鼻子[line]「那就好，你马上示范给我看吧。」[%p]"

msgid "2200:野呂瀬は能面のように無表情を貫いたまま、小さく肩をすくめた。[%p]"
msgstr "2200:野吕濑维持着宛如面具般毫无情绪的表情，轻轻耸了耸肩。[%p]"

msgid "2300:[name]野呂瀬[line]「では、このようなものはどうですか？」[%p]"
msgstr "2300:[name]野吕濑[line]「那么，这种如何呢？」[%p]"

msgid "2400:野呂瀬がそう言い終えたときには、すでに“実演”は始まっていた。[%p]"
msgstr "2400:当野吕濑说完这句话时，『示范』已经开始了。[%p]"

msgid "2500:ふと気付くと、猪鼻も倉持も、その手に拳銃を持っている。[%p]"
msgstr "2500:回过神来，发现山猪鼻子跟仓持手上都拿着枪。[%p]"

msgid "2600:ノアⅡも、野呂瀬も、まったくなんの変化も動作もしないまま、その奇跡のような出来事は現実となっていた。[%p]"
msgstr "2600:诺亚Ⅱ跟野吕濑都完全没有任何变化或动作，这个奇迹般的事情就变成了现实。[%p]"

msgid "2700:[name]猪鼻[line]「ほう、本物そっくりだ」[%p]"
msgstr "2700:[name]山猪鼻子[line]「哦？跟真货一模一样呢。」[%p]"

msgid "2800:[name]倉持[line]「まるで手品のようだよ」[%p]"
msgstr "2800:[name]仓持[line]「简直就像变魔术似的。」[%p]"

msgid "2900:[name]野呂瀬[line]「手品と一緒にしてもらっては困ります」[%p]"
msgstr "2900:[name]野吕濑[line]「别把我的能力跟变魔术相提并论好吗？」[%p]"

msgid "3000:[name]野呂瀬[line]「その銃の存在は、現実」[%p]"
msgstr "3000:[name]「拜托不要拿我跟变魔术混为一谈。」[line]「那把枪的存在是现实。」[%p]"

msgid "3100:猪鼻と倉持は、銃をしげしげと眺めながら、再び満足そうにうなずく。[%p]"
msgstr "3100:猪鼻与仓持仔细端详手枪，再度满意地点头。[%p]"

msgid "3200:[name]猪鼻[line]「実に素晴らしい」[%p]"
msgstr "3200:[name]猪鼻[line]「真是太棒了。」[%p]"

msgid "3300:[name]倉持[line]「いかにも。神をも超越する力だ」[%p]"
msgstr "3300:[name]仓持[line]「正是，是连神都能超越的力量。」[%p]"

msgid "3400:[name]猪鼻[line]「この銃は記念にもらっておくとするか」[%p]"
msgstr "3400:[name]仓持[line]「这把枪就当作纪念收下吧。」[%p]"

msgid "3500:[name]野呂瀬[line]「……小物ですね」[%p]"
msgstr "3500:[name]野吕濑[line]「……真是个小人物呢。」[%p]"

msgid "3600:野呂瀬のそのかすかなつぶやきは、他の２人に聞こえることはなかった。[%p]"
msgstr "3600:野吕濑这句轻声细语，并没有传进其他两人耳里。[%p]"

msgid "3700:[name]猪鼻[line]「早速、今後の計画を検討しよう」[%p]"
msgstr "3700:[name]猪鼻[line]「事不宜迟，来讨论今后的计划吧。」[%p]"

msgid "3800:[name]倉持[line]「まずは３００人委員会の排除を」[%p]"
msgstr "3800:[name]仓持[line]「首先，要先除掉300人委员会。」[%p]"

msgid "3900:[name]猪鼻[line]「野呂瀬。ご苦労だったな」[%p]"
msgstr "3900:[name]仓持[line]「野吕濑，辛苦你了。」[%p]"

msgid "4000:[name]倉持[line]「今後のことは、追って連絡する」[%p]"
msgstr "4000:[name]仓持[line]「今后的事情我会再联络你们。」[%p]"

msgid "4100:２人はすっかり上機嫌で、足取りも軽くプラネタリウムから出ていこうとする。[linebreak]だが――[%p]"
msgstr "4100:两人的心情都好到不行，踏着轻快的脚步准备离开天象仪剧场。[linebreak]然而——[%p]"

msgid "4200:[name]野呂瀬[line]「その必要はありません」[%p]"
msgstr "4200:[name]野吕濑[line]「没有那个必要。」[%p]"

msgid "4300:[name]猪鼻[line]「……なに？」[%p]"
msgstr "4300:[name]猪鼻[line]「……什么？」[%p]"

msgid "4400:[name]倉持[line]「どういうことだ？」[%p]"
msgstr "4400:[name]仓持[line]「什么意思啊？」[%p]"

msgid "4500:そこで初めて、野呂瀬の表情に変化が起きた。[%p]"
msgstr "4500:这时，野吕濑的表情才终于有了变化。[%p]"

msgid "4600:目を細め、わずかに顎をそらして、その長身から２人を見据える。それはまるで見下しているかのような態度。[%p]"
msgstr "4600:她眯起眼睛，稍微抬起下巴，以高挑身材俯视两人，态度简直就像在鄙视她们一样。[%p]"

msgid "4700:いや、現実に、彼は２人を見下していた――[%p]"
msgstr "4700:不，在现实中，他确实是在藐视那两个人——[%p]"

msgid "4800:[name]野呂瀬[line]「計画は私が引き継ぎましょう。ただし、あなた方の考えているような低俗な目的ではなく、より崇高な理由のために、です」[%p]"
msgstr "4800:[name]野吕濑[line]「计划就由我来接手吧，只不过不是为了你们所想的那种低俗目的，而是为了更加崇高的理由。」[%p]"

msgid "4900:野呂瀬はあくまでも丁寧な口調を続ける。[linebreak]だがその言葉の意味に気付き、当然のように猪鼻も倉持も激昂した。[%p]"
msgstr "4900:野吕濑始终维持着客气的语气。[linebreak]不过察觉到这句话的意思后，山猪鼻与仓持都理所当然地激动起来。[%p]"

msgid "5000:[name]猪鼻[line]「貴様、裏切るのか！」[%p]"
msgstr "5000:[name]山猪鼻[line]山猪鼻[%p]"

msgid "5100:[name]倉持[line]「神にでもなったつもりかね？」[%p]"
msgstr "5100:[name]仓持[line]「你以为自己是神吗？」[%p]"

msgid "5200:[name]猪鼻[line]「３００人委員会が黙ってはおらんぞ！」[%p]"
msgstr "5200:[name]山猪鼻[line]「300人委员会可不会坐视不管哦！」[%p]"

msgid "5300:[name]野呂瀬[line]「おふたりも、３００人委員会のメンバーでありながら、反旗を翻そうとしていたではありませんか」[%p]"
msgstr "5300:[name]野吕濑[line]「两位不也是300人委员会的一员，却打算反抗组织吗？」[%p]"

msgid "5400:野呂瀬の、２人を捉える瞳。[linebreak]そこにある感情は――蔑み。[%p]"
msgstr "5400:野吕濑以双眼捕捉两人的身影。[linebreak]她眼中流露的感情是——轻蔑。[%p]"

msgid "5500:汚いものでも見るような冷たい視線が意味するもの。それは彼がこれまでずっと、猪鼻と倉持をただ利用してきただけだということだ。[%p]"
msgstr "5500:那道仿佛看着脏东西的冰冷视线，意味着他至今只是在利用猪鼻与仓持而已。[%p]"

msgid "5600:[name]野呂瀬[line]「これは、すべて予定通りの行動ですよ」[%p]"
msgstr "5600:[name]野吕濑[line]「这全都是按照预定计划进行的行动。」[%p]"

msgid "5700:[name]野呂瀬[line]「あなた方がここで死ぬことも含めて」[%p]"
msgstr "5700:[name]「这是，全部都照着预定计划进行的行动哦。」[line]「包含你们会死在这里在内。」[%p]"

msgid "5800:[name]猪鼻[line]「ふざけるな！」[%p]"
msgstr "5800:[name]野吕濑[line]「开什么玩笑啊！」[%p]"

msgid "5900:[name]倉持[line]「戯れ言を！」[%p]"
msgstr "5900:[name]仓持[line]「胡说八道！」[%p]"

msgid "6000:倉持と猪鼻は、揃って銃口を野呂瀬へと向ける。[linebreak]引き金に手をかけた。[%p]"
msgstr "6000:仓持与猪鼻同时把枪口对准了野吕濑。[linebreak]他们把手放在扳机上。[%p]"

msgid "6100:[name]野呂瀬[line]「戯れ言ではなく――」[%p]"
msgstr "6100:[name]野吕濑[line]「我不是在开玩笑——」[%p]"

msgid "6200:ふたつの銃が同時に火を噴き。[linebreak]ドーム内に、轟音が響き渡った。[%p]"
msgstr "6200:两把枪同时喷出火花。[linebreak]巨响在圆顶内部回荡着。[%p]"

msgid "6300:[name]野呂瀬[line]「予知、だったのですが」[%p]"
msgstr "6300:[name]野吕濑[line]「我原本是想预知……」[%p]"

msgid "6400:倒れたのは、野呂瀬ではなく、猪鼻と倉持。[%p]"
msgstr "6400:昏倒是猪鼻与仓持，不是野吕濑。[%p]"

msgid "6500:なぜか彼らは、引き金を引く直前で、銃の狙いを野呂瀬から外し、お互いを撃ち合ったのだ。[%p]"
msgstr "6500:不知为何，在扣下扳机之前，他们把枪口从野吕濑身上移开，互相射击彼此。[%p]"

msgid "6600:即死――。[%p]"
msgstr "6600:当场死亡——[%p]"

msgid "6700:互いの額に穴を開け仲良く倒れた２人にすでに息はなく、ただの肉の塊と化していた。[%p]"
msgstr "6700:额头被射穿，感情要好地一起倒下的两人已经没了呼吸，变成了一堆肉块。[%p]"

msgid "6800:[name]野呂瀬[line]「ノアⅡは、我が手にあります」[%p]"
msgstr "6800:[name]野吕濑[line]「诺亚Ⅱ，已经在我手中了。」[%p]"

msgid "6900:[name]野呂瀬[line]「俗物には、その意味がお分かりにはならなかったようだ」[%p]"
msgstr "6900:[name]野吕濑[line]「看来庸人是无法理解其中意义。」[%p]"

msgid "7000:野呂瀬が２人に見せた“実演”は、銃を出すだけではなかった。彼らに、お互いを野呂瀬だと勘違いさせ、殺し合わせる。[%p]"
msgstr "7000:野吕濑向两人「示范」的内容，不只是拿出枪支而已。她让两人误以为彼此是野吕濑，并且互相残杀。[%p]"

msgid "7100:そこまでが“実演”。[%p]"
msgstr "7100:到此为止都是「示范」。[%p]"

msgid "7200:ノアⅡの力をもってすれば、それはあまりにも造作のないことだった。[%p]"
msgstr "7200:以诺亚Ⅱ的力量，要办到这点小事根本是轻而易举。[%p]"

msgid "7300:野呂瀬は“オブジェ”へと向き直る。[%p]"
msgstr "7300:野吕濑重新面向「装置艺术」。[%p]"

msgid "7400:そこには、張り付けにされた咲畑梨深の姿があった。[%p]"
msgstr "7400:那里有着被贴在墙上的咲畑梨深的身影。[%p]"

msgid "7500:梨深は七海を解放するように懇願し、野呂瀬はそれを了承する代わりに彼女を人質とした。[%p]"
msgstr "7500:梨深恳求野吕濑放了七海，而野吕濑则以她为人质作为答应的条件。[%p]"

msgid "7600:七海に精神攻撃を受けたせいか、梨深からはほとんど生気が失われ、今も張り付けにされて力なくうなだれている。[%p]"
msgstr "7600:或许是因为受到来自七海的精神攻击，梨深几乎失去了所有活力，现在也无力地垂着头贴在墙上。[%p]"

msgid "7700:だが野呂瀬が近づくと、わずかに肩を震わせた。[%p]"
msgstr "7700:不过当野吕濑靠近时，她的肩膀微微颤抖了一下。[%p]"

msgid "7800:[name]梨深[line]「自分の……手を汚さずに……卑怯なことばっかり……」[%p]"
msgstr "7800:[name]梨深[line]「不弄脏自己的手……只会做些卑鄙的事情……」[%p]"

msgid "7900:[name]梨深[line]「あなたこそ……俗物だよ」[%p]"
msgstr "7900:[name]梨深[line]「你才是……庸人。」[%p]"

msgid "8000:[name]梨深[line]「……っ」[%p]"
msgstr "8000:[name]梨深[line]「……！」[%p]"

msgid "8100:野呂瀬の手の甲が、梨深の頬を打つ。[linebreak]それから、野呂瀬は彼女の顎をつかみ、無理矢理自分の方を向かせた。[%p]"
msgstr "8100:野吕濑的手掌，甩在了梨深的脸颊上。[linebreak]接着，野吕濑抓住她的下巴，硬是让她面向自己。[%p]"

msgid "8200:表情は死んでいると言うのに、梨深の薄く開かれた目だけは、今も爛々と輝き野呂瀬を射ている。[%p]"
msgstr "8200:明明表情已经死了，但唯有梨深那微微睁开的眼睛，现在也依然炯炯有神地盯着野吕濑不放。[%p]"

msgid "8300:[name]野呂瀬[line]「なぜ、ニシジョウタクミに助けを求めないのです？」[%p]"
msgstr "8300:[name]野吕濑[line]「为什么不向西上匠助求助呢？」[%p]"

msgid "8400:梨深の耳元で、野呂瀬はささやく。[%p]"
msgstr "8400:野吕濑在梨深耳边低语道。[%p]"

msgid "8500:[name]梨深[line]「彼を……巻き込まないで……」[%p]"
msgstr "8500:[name]梨深[line]「不要把他……牵扯进来……」[%p]"

msgid "8600:[name]野呂瀬[line]「どちらの“彼”のことを言っているのですか？」[%p]"
msgstr "8600:[name]野吕濑[line]「你口中的『他』是指谁呢？」[%p]"

msgid "8700:[name]梨深[line]「２人とも……だよ……」[%p]"
msgstr "8700:[name]野吕濑[line]「你们两个都一样啦……」[%p]"

msgid "8800:[name]野呂瀬[line]「私としては、どちらかひとりでじゅうぶんですがね」[%p]"
msgstr "8800:[name]野吕濑[line]「对我来说，只要其中一个人就够了。」[%p]"

msgid "8900:[name]野呂瀬[line]「Ｉｒ２を生み出した者と、その完全なるコピー」[%p]"
msgstr "8900:[name]野吕濑[line]「制造出Ir2的人，以及其完美的复制品。」[%p]"

msgid "9000:[name]野呂瀬[line]「２人とも、化け物じみた力を持っている」[%p]"
msgstr "9000:[name]野吕濑[line]「他们两人，都拥有怪物般的力量。」[%p]"

msgid "9100:[name]野呂瀬[line]「彼らどちらかのＣＯＤＥサンプルが手に入れば、ノアⅡはより完璧へと近づくでしょう」[%p]"
msgstr "9100:[name]野吕濑[line]「只要得到他们其中一方的CODE样本，诺亚Ⅱ就会更接近完美。」[%p]"

msgid "9200:[name]野呂瀬[line]「神をも超える力を得るのです」[%p]"
msgstr "9200:[name]野吕濑[line]「得到超越神的力量。」[%p]"

msgid "9300:[name]梨深[line]「…………」[%p]"
msgstr "9300:[name]梨深[line]「…………」[%p]"

msgid "9400:梨深が辛そうに目を伏せる。[%p]"
msgstr "9400:梨深难过地垂下眼帘。[%p]"

msgid "9500:野呂瀬は彼女の顎をつかんでいた手を乱暴に振り払うと、ノアⅡを見上げ、唇の端に笑みを浮かべた。[%p]"
msgstr "9500:野吕濑粗鲁地甩开抓着她下巴的手，抬头看着诺亚Ⅱ，在嘴角浮现笑意。[%p]"

msgid "9600:[name]野呂瀬[line]「さあ、呼びかけなさい」[%p]"
msgstr "9600:[name]野吕濑[line]「来，呼唤吧。」[%p]"

msgid "9700:[name]野呂瀬[line]「どちらでもいい」[%p]"
msgstr "9700:[name]野吕濑[line]「随便你。」[%p]"

msgid "9800:[name]野呂瀬[line]「私は、歓迎しますよ」[%p]"
msgstr "9800:[name]野吕濑[line]「我倒是很欢迎哦。」[%p]"

msgid "9900:むしろ、彼は心待ちにしていた。[linebreak]西條拓巳が、野呂瀬の出した“試験”を乗り越えて、この場にやって来るのを。[%p]"
msgstr "9900:不如说，他一直引颈期盼着这一刻的到来。[linebreak]期待西条拓巳能通过野吕濑提出的「考验」，来到这个地方。[%p]"

