msgid "0:気が付けば――[%p]"
msgstr "0:気が付けば――[%p]"

msgid "100:世界に鮮やかな光が戻っていた。[%p]"
msgstr "100:世界に鮮やかな光が戻っていた。[%p]"

msgid "200:空が、青い。[%p]"
msgstr "200:空が、青い。[%p]"

