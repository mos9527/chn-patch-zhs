# THIS FILE IS AUTOMATICALLY GENERATED BY generate_po.py.
                     
msgid "0:みんなが、僕に注目していた。[%p]"
msgstr "0:大家都在注视着我。[%p]"

msgid "100:また前みたいに罵倒され、嘲笑され、屈辱感に晒されるんだって、身構えてしまう。[%p]"
msgstr "100:我又会被像之前那样痛骂，被众人耻笑，承受屈辱——想到这里，我不禁绷紧了神经。[%p]"

msgid "200:あのときのトラウマで、足がすくむ。[%p]"
msgstr "200:当时的创伤让我双腿发软。[%p]"

msgid "300:でも――[%p]"
msgstr "300:但是——[%p]"

msgid "400:[name]祭参加者１[line]「頑張れ」[%p]"
msgstr "400:[name]祭典参加者1[line]「加油。」[%p]"

msgid "500:[name]祭参加者２[line]「応援してるね」[%p]"
msgstr "500:[name]「加油啊！」[line]「我会替你加油哦～」[%p]"

msgid "600:[name]祭参加者３[line]「頑張って、西條くん」[%p]"
msgstr "600:[name]「我支持你。」[line]「加油啊，西条同学～」[%p]"

msgid "700:[name]祭参加者４[line]「華々しく散ってこい」[%p]"
msgstr "700:[name]「加油哦，西条同学。」[line]「华丽地凋零吧！」[%p]"

msgid "800:[name]祭参加者５[line]「若いのに、勇気があるわね」[%p]"
msgstr "800:[name]「华丽地凋零吧！」[line]「年纪轻轻，却很有勇气呢。」[%p]"

msgid "900:[name]祭参加者６[line]「お前のこと忘れないからな」[%p]"
msgstr "900:[name]「明明还这么年轻，却很勇敢呢。」[line]「我不会忘记你的。」[%p]"

msgid "1000:[name]祭参加者７[line]「最後まで見守っててやるぞ」[%p]"
msgstr "1000:[name]「我不会忘记你。」[line]「我会守护你到最后哦。」[%p]"

msgid "1100:[name]祭参加者８[line]「漢を見せてくれ」[%p]"
msgstr "1100:[name]「我不会忘记你的。」[line]「让我见识一下男子汉气概吧！」[%p]"

msgid "1200:あのときとは、野次馬連中の反応がまるで違う。[linebreak]みんな、優しい言葉や励ましの言葉をかけてくる。[%p]"
msgstr "1200:围观群众的反应跟那时候截然不同。[linebreak]大家都用温柔的话语鼓励我。[%p]"

msgid "1300:だから僕は、狐につままれたような気分になって、何度も目を瞬かせた。[%p]"
msgstr "1300:所以我才会感到困惑，眨了好几次眼睛。[%p]"

msgid "1400:僕を、応援してくれてるの……？[%p]"
msgstr "1400:他们是在为我加油吗……？[%p]"

msgid "1500:[name]星来[line]「そのための祭りだもん」[%p]"
msgstr "1500:[name]星来[line]「所以才要办祭典啊。」[%p]"

msgid "1600:[name]星来[line]「みんなさ、タッキーを待っててくれたんだよ」[%p]"
msgstr "1600:[name]星来[line]「大家在等小泷哦。」[%p]"

msgid "1700:目の前に、人垣によって作られた通路。[linebreak]手でアーチまで作って、僕が通るのを待っている。[linebreak]まるで小学校の卒業式みたいだ。[%p]"
msgstr "1700:眼前是用人群围出的道路。[linebreak]他们用手做出拱门，等着我通过。[linebreak]简直就像小学毕业典礼一样。[%p]"

msgid "1800:その先は、Ｏ－ＦＲＯＮＴの入り口へと繋がっている。[linebreak]僕はおずおずとその道を進み出す。[%p]"
msgstr "1800:前方通往O—FROONT入口。[linebreak]我战战兢兢地踏上那条路。[%p]"

msgid "1900:なおも温かい言葉がたくさん投げかけられてくる。[%p]"
msgstr "1900:温暖的话语接二连三地传了过来。[%p]"

msgid "2000:さらに、Ｏ－ＦＲＯＮＴの中へと足を踏み入れると、ＴＡＴＳＵＹＡとスタバァの店員までが僕を拍手で出迎えた。[%p]"
msgstr "2000:而且，当我踏入O—FROONT里时，TATSUYA书店和星巴克的店员都用掌声迎接我。[%p]"

msgid "2100:だんだん勇気が出てきた。[linebreak]死ぬことへの、勇気が。[%p]"
msgstr "2100:勇气逐渐涌现出来。[linebreak]对死亡的勇气。[%p]"

msgid "2200:Ｏ－ＦＲＯＮＴの屋上は、相変わらず薄暗かった。[linebreak]ゴクリと息を呑む。[%p]"
msgstr "2200:O—FROONT的屋顶还是一样昏暗。[linebreak]我倒抽了一口气。[%p]"

msgid "2300:前にここに来たときの苦い経験がよみがえり、先に進むことを一瞬躊躇する。[%p]"
msgstr "2300:之前来这里时的痛苦经验重新浮现，让我一瞬间犹豫是否该前进。[%p]"

msgid "2400:せっかく勇気が出てきたのに。[linebreak]どこまでも自分がヘタレであることを自覚する。[%p]"
msgstr "2400:好不容易才鼓起勇气耶。[linebreak]我彻底体会到自己有多没用。[%p]"

msgid "2500:[name]？？？[line]「きっと戻ってくると信じてたよ」[%p]"
msgstr "2500:[name]？？？[line]「我一直相信你会回来哦！」[%p]"

msgid "2600:車椅子の、車輪が軋む音。[%p]"
msgstr "2600:轮椅车轮发出吱嘎声。[%p]"

msgid "2700:それとともに、前と同じように『将軍』が姿を見せた。今日もダーススパイダーのヘルメットをかぶっている。[%p]"
msgstr "2700:与此同时，「将军」跟之前一样现身了。今天也戴着一打蜘蛛头的安全帽。[%p]"

msgid "2800:しかも、この場にいたのは『将軍』だけじゃなかった。[%p]"
msgstr "2800:而且，在场的人不只「将军」而已。[%p]"

msgid "2900:[name]セナ[line]「よくもう一度挑戦する気になったな」[%p]"
msgstr "2900:[name]瑟娜[line]「真亏你还有勇气再次挑战呢。」[%p]"

msgid "3000:[name]梢[line]「拓巳しゃん、すごーいすごーいの！」[%p]"
msgstr "3000:[name]梢[line]「拓先生好厉害，好厉害哦～！」[%p]"

msgid "3100:[name]優愛[line]「西條くんのおかげで、わたしも、勇気を、もらいました」[%p]"
msgstr "3100:[name]优爱[line]「多亏有你，我也得到了勇气。」[%p]"

msgid "3200:[name]あやせ[line]「キミはとても崇高だわ、拓巳」[%p]"
msgstr "3200:[name]绫濑[line]「拓巳，你的志向非常伟大呢。」[%p]"

msgid "3300:セナや、こずぴぃや、優愛、それにあやせ。[linebreak]みんなが一列に並んで、僕に微笑みかけている。[%p]"
msgstr "3300:瑟娜、小九镖士、优爱还有绫濑。[linebreak]大家排成了一列，对我露出微笑。[%p]"

msgid "3400:[name]あやせ[line]「一緒に飛ぶわ、私たち、あなたと」[%p]"
msgstr "3400:[name]绫濑[line]绫濑[%p]"

msgid "3500:[name]梢[line]「だからーね、怖くないよ♪」[%p]"
msgstr "3500:[name]梢[line]「所以啊，我不会怕哦♪」[%p]"

msgid "3600:こずぴぃとあやせが、それぞれ左右の手を握りしめてきた。[linebreak]２人だけじゃない。セナも、優愛も一緒だ。５人で手を繋いで。[%p]"
msgstr "3600:小九和绫濑分别握住我的左手和右手。[linebreak]不只她们两个，瑟娜跟优爱也在一起。我们五个人牵着手。[%p]"

msgid "3700:これは『集団ダイブ』の再現？[%p]"
msgstr "3700:这是在重现「集体潜行」吗？[%p]"

msgid "3800:下で固唾を呑んで見守っていた群衆が、一斉に拍手した。僕らを祝福するかのように。僕らを称えるように。[%p]"
msgstr "3800:在下方屏息静观的群众，一起鼓掌了。像是要祝福我们一样；像是要称赞我们一样。[%p]"

msgid "3900:[name]将軍[line]「さようなら。君のことは、忘れないよ」[%p]"
msgstr "3900:[name]将军[line]「再见了，我不会忘记你的。」[%p]"

msgid "4000:将軍が、その細い手を振った。[linebreak]こずぴぃとあやせが、僕を引っ張ってくる。[%p]"
msgstr "4000:将军挥动他纤细的手臂。[linebreak]小九和绫濑把我拉了过去。[%p]"

msgid "4100:戯れるように。踊るように。[linebreak]優しい笑みを浮かべながら。[%p]"
msgstr "4100:像是在嬉戏，又像在跳舞似的——[linebreak]脸上带着温柔的笑容。[%p]"

msgid "4200:だから僕は、恐怖なんてこれっぽっちも感じることはなくて。[%p]"
msgstr "4200:所以我一点也没有感到恐惧——[%p]"

msgid "4300:すごくすんなりと、その身を空中へと踊らせることができた――[%p]"
msgstr "4300:我非常自然地，让自己的身体在空中舞动起来——[%p]"

msgid "4400:僕は思う。[linebreak]そんな風に、気持ちよく死ねたら、どんなによかっただろう。[%p]"
msgstr "4400:我想着。[linebreak]如果能像这样，痛快地死去的话，那该有多好啊？[%p]"

msgid "4500:でも、そんなものは所詮、妄想だった。[%p]"
msgstr "4500:但是，那种事终究只是我的幻想罢了。[%p]"

msgid "4600:人垣によって作られた通路は、確かに僕を導くように目の前に口を開けていた。[linebreak]でも、誰も手でアーチなんて作ってない。[%p]"
msgstr "4600:由人群围出的道路确实像在引导我一般，在眼前张开了一个开口。[linebreak]可是，根本没有人用手做出拱门的样子啊。[%p]"

msgid "4700:みんな、僕を見ている。でもその目は冷たく、鋭く。投げかけられるのは殺気に近い感情。[%p]"
msgstr "4700:大家都在看着我。但眼神冰冷而锐利，对我投来近乎杀意的感情。[%p]"

msgid "4800:誰もが、殺伐としている。[linebreak]誰も僕なんて待っていなかった。[linebreak]僕を祝福なんてしなかった。[%p]"
msgstr "4800:每个人都充满杀气。[linebreak]没有人等我。[linebreak]没有一个人祝福我。[%p]"

msgid "4900:前に僕がＯ－ＦＲＯＮＴで醜態をさらしたときの、あのノリは、今日はまったく見られない。[%p]"
msgstr "4900:之前我在O—FROONT丢脸时的那种气势，今天完全看不到。[%p]"

msgid "5000:気持ちよく死ぬことは、許されないらしい。[linebreak]どうせ、そんなことだろうと思ったよ。[linebreak]でも、いいんだ。[%p]"
msgstr "5000:看来我不能痛快地死去。[linebreak]我就知道会这样。[linebreak]不过，没关系。[%p]"

msgid "5100:殺してくれるなら、それで。[linebreak]僕は嗤って、その人垣の通路を進み始めた。[%p]"
msgstr "5100:如果能死在她手上，那就好。[linebreak]我笑着走进人群围起的道路中。[%p]"

msgid "5200:みんなが、僕に注目していた。[%p]"
msgstr "5200:大家都在注视着我。[%p]"

msgid "5300:また前みたいに罵倒され、嘲笑され、屈辱感に晒されるんだって、身構えてしまう。[%p]"
msgstr "5300:大家又会像之前那样痛骂我，嘲笑我，让我感到无比羞耻吧——我不禁紧张起来。[%p]"

msgid "5400:あのときのトラウマで、足がすくむ。[%p]"
msgstr "5400:当时的阴影让我的双腿不听使唤。[%p]"

msgid "5500:そんな僕の背中に、誰かがぶつかってきた。[linebreak]体当たり、と言う方が正しいかもしれない。[%p]"
msgstr "5500:就在这时，有人撞上了我的后背。[linebreak]说是用身体冲撞我或许比较正确。[%p]"

msgid "5600:不意を突かれた僕は、前のめりに体勢を崩し、倒れそうになるのをなんとか踏みとどまる。[%p]"
msgstr "5600:这出乎预料的一击让我失去平衡，身体往前倾，好不容易才站稳脚步避免跌倒。[%p]"

msgid "5700:振り返ると、２０代のサラリーマンらしき男だった。見たこともない顔だ。[%p]"
msgstr "5700:我转过头去一看，发现对方是个年约二十几岁，看似上班族的男人。是我没见过的人。[%p]"

msgid "5800:[name]祭参加者１[line]「おー、すげー。こんな感触なんだ」[%p]"
msgstr "5800:[name]祭典参加者一———[line]「哦～好厉害，原来是这种触感啊！」[%p]"

msgid "5900:自らの両手を見つめ、意味不明なことをつぶやいている。[linebreak]嫌がらせかな、と思っていたら、急に腰のあたりに熱を感じた。[%p]"
msgstr "5900:她看着自己的双手，嘴里喃喃说着莫名其妙的话。[linebreak]我心想「这是在找碴吗？」的时候，腰部附近突然感觉到一股热气。[%p]"

msgid "6000:そして、異物感。[linebreak]なにかが、腰のあたりに引っかかっているような。[%p]"
msgstr "6000:接着是异物感。[linebreak]好像有什么东西卡在腰部附近。[%p]"

msgid "6100:うずき出す。[linebreak]痛み。[linebreak]焼けるような。[%p]"
msgstr "6100:开始发痒了。[linebreak]疼痛感传来。[linebreak]灼烧般的痛楚。[%p]"

msgid "6200:顔をしかめながら、自分の腰を確かめてみる。[%p]"
msgstr "6200:我皱着脸，确认自己的腰部状况。[%p]"

msgid "6300:するとそこに、ナイフの柄のようなものが、突き出ていた。[%p]"
msgstr "6300:结果发现有类似刀柄的东西从那里突出。[%p]"

msgid "6400:違う、突き出ているんじゃない。[linebreak]ナイフが、刺さっているんだ。[%p]"
msgstr "6400:不对，那不是突出。[linebreak]是刀子插在那里。[%p]"

msgid "6500:[name]拓巳[line]「う……ぐ……っ」[%p]"
msgstr "6500:[name]拓巳[line]「呜……咕……！」[%p]"

msgid "6600:痛みが鋭くなる。[linebreak]それに、いつの間にか自分のズボンが濡れていた。[linebreak]それが自分の血によるものだと気付いて、愕然となった。[%p]"
msgstr "6600:痛楚变得越来越强烈。[linebreak]而且，不知不觉间自己的裤子也湿了。[linebreak]察觉到那是自己流出来的血时，他感到一阵错愕。[%p]"

msgid "6700:[name]祭参加者２[line]「次、私ねー」[%p]"
msgstr "6700:[name]祭典参加者2[line]「下一个换我～」[%p]"

msgid "6800:[name]拓巳[line]「え？」[%p]"
msgstr "6800:[name]拓巳[line]「咦？」[%p]"

msgid "6900:正面から、女――女子大生風のオシャレな若い女が、[linebreak]包丁を手に笑顔で駆け寄ってきた。避ける暇もなくて。[%p]"
msgstr "6900:从正前方，一名女性——看似女大学生般时髦的年轻女孩。[linebreak]她拿着菜刀面带笑容地冲了过来，我根本来不及闪躲。[%p]"

msgid "7000:その女の持つ包丁が、僕の腹に深々と突き刺さる。[%p]"
msgstr "7000:那名女性手上的菜刀深深插进了我的腹部。[%p]"

msgid "7100:[name]拓巳[line]「あ……あ……」[%p]"
msgstr "7100:[name]拓巳[line]「啊……啊……」[%p]"

msgid "7200:[name]祭参加者２[line]「きゃー、すっごーい、私、刺しちゃった♪」[%p]"
msgstr "7200:[name]祭典参加者2[line]「呀～好厉害，我捅下去了♪」[%p]"

msgid "7300:女ははしゃいだ様子で、すぐに僕から離れていく。[%p]"
msgstr "7300:女人兴奋地叫着，随即离开了我的身边。[%p]"

msgid "7400:気付けば、たくさんの群衆に僕は取り囲まれていた。[%p]"
msgstr "7400:回过神来时，我已经被大量的群众包围了。[%p]"

msgid "7500:学生も、会社員も、若い男も中年の女も、誰も彼も、手にナイフや包丁を握りしめて、ニヤニヤしながら僕を見つめている。[%p]"
msgstr "7500:有学生，也有上班族；有年轻男子，也有中年女子，每个人手上都紧握着刀子或菜刀，带着不怀好意的笑容注视我。[%p]"

msgid "7600:４つの街頭ビジョン。そこに、派手なキャプションが躍っていた。[%p]"
msgstr "7600:四个街头屏幕播放出华丽的标题画面。[%p]"

msgid "7700:“みんなで偽エスパー西條拓巳を刺し殺そう[color index=\"A0FF0000\"]オフ会[color index=\"800000\"]　開催中！”[%p]"
msgstr "7700:「大家一起把假灵能者西条拓巳捅死吧」[color index=\"A0FF0000\"]网聚[color index=\"800000\"]「举办中！」[%p]"

msgid "7800:僕の前に、順番待ちの行列ができている。[linebreak]整然と、みんな並び始める。[%p]"
msgstr "7800:我的面前，排着等待参加网聚的人龙。[linebreak]大家开始整齐地排队。[%p]"

msgid "7900:僕を、刺す、順番待ち――[%p]"
msgstr "7900:等着，轮到我被捅——[%p]"

msgid "8000:[name]祭参加者３[line]「えいっ」[%p]"
msgstr "8000:[name]参加祭典的人数是三。[line]「嘿！」[%p]"

msgid "8100:今度は太ったガングロ女子高生。[linebreak]僕の脇腹にナイフを突き刺した。[%p]"
msgstr "8100:这次换成胖胖的黑脸辣妹女高中生。[linebreak]她用刀子戳了我的侧腹部。[%p]"

msgid "8200:僕はあまりの事態に思考が付いていかず、痛みさえも感じる余裕はなく、立っているのが精一杯。[%p]"
msgstr "8200:我因为事情发生得太过突然，脑袋完全跟不上状况，连感到疼痛的时间都没有，光是站着就已经竭尽全力了。[%p]"

msgid "8300:[name]祭参加者３[line]「げえっ！　手に血が付いたぁ！　マジ最悪なんだけどっ」[%p]"
msgstr "8300:[name]祭典参加者3[line]「呃啊！手上沾到血了啦！真是有够糟！」[%p]"

msgid "8400:そのガングロ女子高生を押しのけるように、初老のサラリーマンのおっさんが包丁をきつく握って迫ってくる。[%p]"
msgstr "8400:一名中年上班族大叔推开那个黑皮辣妹，紧握菜刀逼近我。[%p]"

msgid "8500:[name]祭参加者４[line]「ストレス解消だ、おらっ！」[%p]"
msgstr "8500:[name]「呜哇！手上沾到血了啦！真是有够糟！」[line]「消除压力，看招！」[%p]"

msgid "8600:そのおっさんは、一度刺した包丁を抜いて、さらにもう一度、刺し直した。[%p]"
msgstr "8600:那个大叔拔出一次插在肉上的菜刀，又重新插了一次。[%p]"

msgid "8700:[name]拓巳[line]「ぐ、あ……」[%p]"
msgstr "8700:[name]拓巳[line]「咕、啊……」[%p]"

msgid "8800:[name]祭参加者４[line]「はは、こりゃいい」[%p]"
msgstr "8800:[name]祭典参加者4[line]「哈哈，这可真棒。」[%p]"

msgid "8900:すでに、背中に１本。腹に３本。[linebreak]間抜けな形で、刃物が僕の身体から突き出ている。[%p]"
msgstr "8900:背后已经有一根了，腹部有三根。[linebreak]刀子以一种很蠢的方式从我的体内刺了出来。[%p]"

msgid "9000:でもまだ終わらない。“刺し待ち”の行列は、どんどん数が増えている。みんな目を血走らせて、いまかいまかと僕を刺す機会を待っている。[%p]"
msgstr "9000:但事情还没结束，「等待被捅」的人龙还在不断增加。所有人都双眼充血地等着能一刀捅死我的机会到来。[%p]"

msgid "9100:僕は痛みが限界に達して、倒れそうになる。[linebreak]ところが、最初に刺した男が、背後から僕を羽交い締めにした。[%p]"
msgstr "9100:疼痛达到极限，我差点就要昏了过去。[linebreak]然而第一个捅我的男人却从背后架住了我。[%p]"

msgid "9200:[name]祭参加者１[line]「みんな並んで待ってるんだから、倒れちゃダメだろ」[%p]"
msgstr "9200:[name]祭典参加者1[line]「大家都在排队等着，你可不能倒下啊。」[%p]"

msgid "9300:そう言われた。直後に、またも背中に激痛。[linebreak]硬く冷たい刃が、僕の皮膚を、肉を抉ってくる。[%p]"
msgstr "9300:他这么对我说道。紧接着，我的背部又传来一阵剧痛。[linebreak]坚硬冰冷的刀刃划开我的皮肤与肌肉。[%p]"

msgid "9400:[name]拓巳[line]「うぐっ……やめ……」[%p]"
msgstr "9400:[name]拓巳[line]「呜咕……住手……」[%p]"

msgid "9500:やめろ、と言おうとして、必死で口をつぐんだ。[linebreak]これはある意味で僕が望んでいたことじゃないのか。[%p]"
msgstr "9500:我本来想叫她住手，但还是拼命忍住了。[linebreak]这不就是我一直想要的结果吗？[%p]"

msgid "9600:殺してもらいたかったんでしょ。[linebreak]それならこの展開は、願ったりかなったりじゃないか。[linebreak]だから、やめろなんて言うな……。[%p]"
msgstr "9600:你不是希望我杀了你吗？[linebreak]既然如此，这不就是如愿以偿了吗？[linebreak]所以你别叫我住手啊……[%p]"

msgid "9700:僕は男に身体を支えられ、倒れることもできず、[%p]"
msgstr "9700:我被男人撑着，连倒下都做不到——[%p]"

msgid "9800:刺される。[%p]"
msgstr "9800:会被捅死。[%p]"

msgid "9900:胸を。[%p]"
msgstr "9900:胸口。[%p]"

msgid "10000:腹を。[%p]"
msgstr "10000:腹部。[%p]"

msgid "10100:背中を。[%p]"
msgstr "10100:背部。[%p]"

msgid "10200:腕を。[%p]"
msgstr "10200:手臂。[%p]"

msgid "10300:太股を。[%p]"
msgstr "10300:大腿。[%p]"

msgid "10400:爪先を。[%p]"
msgstr "10400:脚尖。[%p]"

msgid "10500:[name]拓巳[line]「あああああ、ぎゃあああああ――！」[%p]"
msgstr "10500:[name]拓巳[line]「啊啊啊啊，呀啊啊啊——」[%p]"

msgid "10600:いつの間にか、絶叫していた。[%p]"
msgstr "10600:不知不觉间，我发出惨叫声。[%p]"

msgid "10700:死にたかったから、無様に泣き叫ぶのは自重しようと思ったのに。[%p]"
msgstr "10700:因为我想寻死，所以才想说要收敛一点，不要难看地哭喊。[%p]"

msgid "10800:それに耐えられないぐらい、痛い。[%p]"
msgstr "10800:我却痛到无法忍受的地步。[%p]"

msgid "10900:身体に刺さっている刃物は、今いったい何本ぐらいなのか。[%p]"
msgstr "10900:现在插在身上的刀子，究竟有几把呢？[%p]"

msgid "11000:すぐには数えられないぐらいの数。[%p]"
msgstr "11000:多到无法立刻计算出来的数量。[%p]"

msgid "11100:[name]拓巳[line]「あああ、ああああああ――！」[%p]"
msgstr "11100:[name]多到无法立刻计算出来的数量。[line]「啊啊啊，啊啊啊啊啊——！」[%p]"

msgid "11200:これだけ刺されて、痛みでもう身体をあまり動かすこともできないのに、感覚も、意識も、ちっとも麻痺してくれなくて。むしろ、やたらとクリアになっていて。[%p]"
msgstr "11200:被捅了这么多刀，明明痛到无法随意活动身体，感觉和意识却丝毫没有麻痹的迹象，反而变得更加清晰敏锐。[%p]"

msgid "11300:[name]拓巳[line]「いいいい、ぎぎぎいいいいい――！」[%p]"
msgstr "11300:[name]拓巳[line]「咿咿咿咿、叽叽叽咿咿咿——！」[%p]"

msgid "11400:いったい何度刺されればいいんだ。[linebreak]早く、死にたい……。[%p]"
msgstr "11400:到底要被捅多少次才好？[linebreak]真想早点死去……。[%p]"

msgid "11500:こんなに痛いのに、どうして僕はちっとも死ねないんだよ……。[%p]"
msgstr "11500:明明这么痛苦，为什么我却一点都死不了呢？[%p]"

msgid "11600:[name]星来[line]「そんなの、決まってるっしょ」[%p]"
msgstr "11600:[name]星来[line]「那还用说。」[%p]"

msgid "11700:星来の声が、頭の中に響いた。[%p]"
msgstr "11700:星来的声音在脑中响起。[%p]"

msgid "11800:[name]星来[line]「妄想だからだよ♪」[%p]"
msgstr "11800:[name]星来[line]「因为是妄想啊♪」[%p]"

msgid "11900:ため息をつきつつ、目を開ける。[linebreak]痛みも、異物感も消えていた。[%p]"
msgstr "11900:我叹着气睁开眼睛。[linebreak]疼痛和异物感都消失了。[%p]"

msgid "12000:僕の前には、何十人もの行列の代わりに、人垣によって作られた通路ができていた。[linebreak]僕を招き入れようとする、一筋の道。[%p]"
msgstr "12000:在我面前，代替几十个人排成一列的人墙形成了一条通道。[linebreak]一条邀请我进入其中的道路。[%p]"

msgid "12100:さっきの妄想が現実になるといいな、って思った。[linebreak]ただし、３回くらい刺されたところで死ねるなら、っていう条件付きだ。[%p]"
msgstr "12100:刚才的幻想如果能成为现实就好了，我这么想着。[linebreak]不过前提是我得先被捅个三次左右才能死掉。[%p]"

msgid "12200:あんな拷問みたいなのは嫌だ……。[%p]"
msgstr "12200:我可不想经历那种有如酷刑般的体验啊……[%p]"

msgid "12300:僕はぼんやりとした頭の中でそう考えながら、人垣の通路へと足を踏み出した。[%p]"
msgstr "12300:我在意识朦胧的大脑中这么想着，朝人群中的道路迈出步伐。[%p]"

msgid "12400:周囲の野次馬どもも、マスコミたちも、やけにピリピリした雰囲気を発している。[%p]"
msgstr "12400:周围的围观群众和媒体记者们，都散发出一股莫名紧张的气息。[%p]"

msgid "12500:ざわついてはいるけど、お祭り騒ぎじゃない。[%p]"
msgstr "12500:虽然气氛很吵闹，但并非庆典般的喧嚣。[%p]"

msgid "12600:前に僕がＯ－ＦＲＯＮＴで醜態をさらしたときの、あのノリは、今日はまったく見られない。[%p]"
msgstr "12600:之前我在O－FROONT出丑时的那种气氛，今天完全感受不到。[%p]"

msgid "12700:みんな、僕を見ている。その目は冷たく、鋭く。投げかけられるのは殺気に近い感情。[%p]"
msgstr "12700:大家都在看着我。眼神冰冷而锐利，投射过来的感情近乎杀意。[%p]"

msgid "12800:僕の期待はいやでも高まる。[%p]"
msgstr "12800:即使不愿意，我的期待也逐渐高涨。[%p]"

msgid "12900:殺して……。"
msgstr "12900:杀了我……"

msgid "13000:[linebreak]殺して。"
msgstr "13000:[linebreak]杀了我。"

msgid "13100:[linebreak]コロシテ。[%p]"
msgstr "13100:[linebreak]杀了我。[%p]"

msgid "13200:殺して……。殺して。コロシテ。"
msgstr "13200:杀了我，杀了我，杀了我。"

msgid "13300:と、すぐ前にあった人の壁が、聖書におけるモーゼの海割りのごとく、さあっと開いていった。[%p]"
msgstr "13300:这时，在她面前的人墙就像圣经里摩西分海一样，瞬间分开来。[%p]"

msgid "13400:僕を招き入れようとする、一筋の道。[%p]"
msgstr "13400:开出一条路让我进去。[%p]"

msgid "13500:彼らが待っていたのは……僕だ。[%p]"
msgstr "13500:他们等待的人是……我。[%p]"

msgid "13600:“第１回チキチキ　いまや日本一恥ずかしい男、自称エスパー西條拓巳を集団でリンチ死させて、渋谷の中心で『その目だれの目？』を叫ぶ[color index=\"A0FF0000\"]オフ会[color index=\"800000\"]”[%p]"
msgstr "13600:「第一届紧张刺激！现在全日本最丢脸的男人，自称超能力者西条拓巳集体围殴致死，在涩谷市中心大喊：『你的眼睛是谁的眼睛？』」[color index=\"A0FF0000\"]网聚。[color index=\"800000\"]网聚。[%p]"

msgid "13700:ふと頭の中にそんな長ったらしいタイトルが浮かんで、[linebreak]僕は――[%p]"
msgstr "13700:我脑中突然浮现这个冗长的标题——[linebreak]我——[%p]"

msgid "13800:嗤った。[%p]"
msgstr "13800:笑了出来。[%p]"

msgid "13900:[name]拓巳[line]「ふひ……ふひひ……」[%p]"
msgstr "13900:[name]拓巳[line]「呼嘻……呼嘻嘻……」[%p]"

msgid "14000:痛いのは、イヤだったけど。[linebreak]死ねるなら、それくらい我慢してやんよ。[%p]"
msgstr "14000:虽然我讨厌疼痛，但是——[linebreak]如果能一死了之的话，这点小事就忍耐一下吧。[%p]"

msgid "14100:[name]拓巳[line]「僕が死ぬには……ぴったりの、ま、祭り、じゃないか。ふひひ……」[%p]"
msgstr "14100:[name]痛觉是会让人讨厌没错啦。[line]「对我来说，这不就是个最适合我死去的……祭、祭典吗？呼嘻嘻……」[%p]"

msgid "14200:リベンジっていう星来たんの言葉の意味を把握した。あのとき、死ねなかったから、僕なんかのためにまた舞台を用意してくれたんだ。[%p]"
msgstr "14200:我明白星来所说「复仇」的意思了。因为那时候没死，所以又为了我这种人准备了舞台啊？[%p]"

msgid "14300:僕は口の端にだらしない笑みを浮かべて、人の壁でできた道を進み始めた。[%p]"
msgstr "14300:我的嘴角浮现不正经的笑容，开始在由人群形成的道路上前进。[%p]"

msgid "14400:行き着く先には、カメラとマイクをバカみたいに横並びで構えた、マスコミの姿があった。[%p]"
msgstr "14400:我抵达的地方是媒体记者的身影——摄像头和麦克风像傻瓜一样一字排开。[%p]"

msgid "14500:周囲の野次馬どもも、マスコミたちも、やけにピリピリした雰囲気を発している。[%p]"
msgstr "14500:周围的围观群众和媒体记者都散发出一股莫名紧张的气息。[%p]"

msgid "14600:ざわついてはいるけど、お祭り騒ぎじゃない。[%p]"
msgstr "14600:虽然嘈杂，但不是庆典般的喧嚣。[%p]"

msgid "14700:前に僕がＯ－ＦＲＯＮＴで醜態をさらしたときの、あのノリは、今日はまったく見られない。[%p]"
msgstr "14700:之前我在O－FROONT出丑时的那种气氛，今天完全看不到。[%p]"

msgid "14800:みんな、僕を見ている。その目は冷たく、鋭く。投げかけられるのは殺気に近い感情。[%p]"
msgstr "14800:大家都在看着我。眼神冰冷而锐利，投射过来的感情近乎杀意。[%p]"

msgid "14900:僕の期待はいやでも高まる。[%p]"
msgstr "14900:即使不愿意，我的期待也高涨起来。[%p]"

msgid "15000:殺して……。"
msgstr "15000:杀了我……"

msgid "15100:[linebreak]殺して。"
msgstr "15100:[linebreak]杀了我。"

msgid "15200:[linebreak]コロシテ。[%p]"
msgstr "15200:[linebreak]杀了我。[%p]"

msgid "15300:殺して……。殺して。コロシテ。"
msgstr "15300:杀了我，杀了我，杀了我。"

msgid "15400:見上げれば、巨大な４つの街頭ビジョンすべてが、人ごみの中を歩く僕の姿を捉えていた。[%p]"
msgstr "15400:抬头一看，四面巨大的街头屏幕都捕捉到我在人群中走动的身影。[%p]"

msgid "15500:きっとこれも、全国中継されているんだろう。[linebreak]視聴率が４０％なら、約４８００万人に見守られて、僕は死ぬ。[%p]"
msgstr "15500:这一定也正在全国转播吧？[linebreak]如果收视率达到40%，我就会在大约四千八百万双眼睛的关注下死去。[%p]"

msgid "15600:たかが妄想の存在である、この僕の死を。[linebreak]なんの価値もない、この僕の死を。[%p]"
msgstr "15600:我的死亡，只不过是妄想中的产物罢了。[linebreak]毫无价值可言的——我的死亡。[%p]"

msgid "15700:お前たちは見たいのか。[linebreak]なんておめでたいんだ。[%p]"
msgstr "15700:你们想看吗？[linebreak]真是天真的想法啊。[%p]"

msgid "15800:でも、別に構わない。[linebreak]僕を殺してくれるなら、どれだけでも見ちゃいなよ。[%p]"
msgstr "15800:不过，我并不介意。[linebreak]如果你们想看的话，不管多少次我都奉陪到底。[%p]"

msgid "15900:カメラとマイクの砲列の前に立つ。[%p]"
msgstr "15900:我站在摄像头和麦克风组成的炮阵前面。[%p]"

msgid "16000:一斉にフラッシュがたかれた。[%p]"
msgstr "16000:闪光灯同时亮起。[%p]"

msgid "16100:まばゆい光が明滅する。[%p]"
msgstr "16100:刺眼光芒闪烁不停。[%p]"

msgid "16200:僕は眼を細めて、そのまぶしさに耐えた。[%p]"
msgstr "16200:我眯起眼睛，忍受着那炫目的闪光。[%p]"

msgid "16300:テレビのワイドショーで見たことのある、初老のレポーターが口火を切った。[%p]"
msgstr "16300:在电视综艺节目上看过的中年记者率先开口了。[%p]"

msgid "16400:[name]芸能レポーターＡ[line]「西條くん。今回はどんなパフォーマンスを見せてくれるんだい？」[%p]"
msgstr "16400:[name]演艺新闻播报员A[line]「西条同学，这次你又会让我们见识到什么样的表演呢？」[%p]"

msgid "16500:[name]拓巳[line]「…………」[%p]"
msgstr "16500:[name]拓巳[line]「…………」[%p]"

msgid "16600:[name]芸能レポーターＡ[line]「ところで、実は今日の昼に逮捕された『張り付け』事件の容疑者は、実行犯に過ぎなくて、黒幕がいるっていう話があるんだけど、どう思うかな？」[%p]"
msgstr "16600:[name]演艺记者A[line]「对了，其实今天中午被逮捕的那个『贴身怪盗』案的嫌犯只是实行犯而已，听说背后还有幕后黑手存在呢。你们觉得如何？」[%p]"

msgid "16700:このおっさんはなんでこんなに馴れ馴れしいんだろう。[%p]"
msgstr "16700:这个大叔为什么这么自来熟啊？[%p]"

msgid "16800:それに“どう思うかな？”ってどういう意味だ？[linebreak]僕に意見を求めてるのか？[%p]"
msgstr "16800:而且，「你们觉得如何？」是什么意思？[linebreak]是在征求我的意见吗？[%p]"

msgid "16900:まあ、犯罪心理学の教授のよくあるコメントなんて、いつもテンプレ通りの面白みもへったくれもないものばかりだから、僕みたいな素人でも代わりはできそうだけど。[%p]"
msgstr "16900:不过，犯罪心理学教授常说的话都是些老套又无聊的内容，像我这样的外行人应该也能代替他们发言吧？[%p]"

msgid "17000:とは言え、今の僕にはそんなことどうでもよかった。[%p]"
msgstr "17000:话虽如此，现在这些事情对我来说都无所谓了。[%p]"

msgid "17100:ニュージェネへの興味なんて失っていた。[linebreak]僕がここに来たのは、殺してもらうためであって、マスコミの質問に答えるためじゃない。答える気力もない。[%p]"
msgstr "17100:我对新世代已经失去兴趣了。[linebreak]我来这里是为了被杀，不是为了回答媒体的问题。我没有力气回答他们。[%p]"

msgid "17200:[name]芸能レポーターＡ[line]「それでね、西條くん。ズバリ聞くけど」[%p]"
msgstr "17200:[name]娱乐记者A[line]「所以，西条同学，我就直接问了——」[%p]"

msgid "17300:[name]芸能レポーターＡ[line]「真犯人と目される人物は、通称『その目だれの目？』と言われていて」[%p]"
msgstr "17300:[name]娱乐记者A[line]「被怀疑是真正凶手的人，一般都叫作『谁的眼睛还是什么的眼睛？』。」[%p]"

msgid "17400:[name]拓巳[line]「…………」[%p]"
msgstr "17400:[name]拓巳[line]「…………」[%p]"

msgid "17500:[name]芸能レポーターＡ[line]「それは、君なんじゃないかっていう噂があるんだよね」[%p]"
msgstr "17500:[name]娱乐记者A[line]「有传闻说，那个人就是你。」[%p]"

msgid "17600:[name]拓巳[line]「そう……かもね……」[%p]"
msgstr "17600:[name]「有人在传，那会不会是你呢？」[line]「或许吧……」[%p]"

msgid "17700:僕のつぶやきは街頭ビジョンのスピーカーを通して、渋谷じゅうに発信された。[linebreak]どよめきがわき起こる。[%p]"
msgstr "17700:我的低语透过街头屏幕的喇叭，传遍涩谷的大街小巷。[linebreak]现场掀起一阵骚动。[%p]"

msgid "17800:この場に集まった人たちの動揺が、空気をますます殺伐としたものへと加速させていく。[%p]"
msgstr "17800:聚集在现场的人们开始动摇，让气氛变得更加杀气腾腾。[%p]"

msgid "17900:[name]拓巳[line]「どうでも、いい……」[%p]"
msgstr "17900:[name]拓巳[line]「怎样都好啦……」[%p]"

msgid "18000:早く殺して……。[linebreak]早く楽になりたいんだ。[linebreak]ウザいことをやらせないでよ。[%p]"
msgstr "18000:快点杀了我吧……[linebreak]我想早点解脱啊。[linebreak]别让我做这种烦人的事啦。[%p]"

msgid "18100:そう願うのに。何本ものマイクが、さらに僕に迫ってくる。口許に押しつけられそうな勢い。[%p]"
msgstr "18100:我明明这么希望，但好几支麦克风却继续逼近我，几乎要塞到我的嘴边了。[%p]"

msgid "18200:他のレポーターどもも堰を切ったように僕に質問をぶつけてくる。[%p]"
msgstr "18200:其他记者也像决堤般向我抛出问题。[%p]"

msgid "18300:[name]芸能レポーターＢ[line]「それは事件への関与を認める発言と考えていいんですか？」[%p]"
msgstr "18300:[name]演艺记者B[line]「我可以认为你这番发言，是承认自己参与了那起案件吗？」[%p]"

msgid "18400:[name]芸能レポーターＣ[line]「現実とゲームとの区別ができなくなるような経験はありますかぁ？」[%p]"
msgstr "18400:[name]演艺记者C[line]「你有无法区分现实与游戏的经验吗～？」[%p]"

msgid "18500:[name]芸能レポーターＢ[line]「逮捕された小前田容疑者との関係について聞かせてください！」[%p]"
msgstr "18500:[name]演艺记者B[line]「请告诉我们你与遭到逮捕的小前田嫌犯之间的关系～」[%p]"

msgid "18600:[name]芸能レポーターＣ[line]「人を殺したのはやはりゲーム感覚だったりします？」[%p]"
msgstr "18600:[name]演艺记者C[line]「杀人对你来说果然就像游戏一样吗～」[%p]"

msgid "18700:ある意味で僕はゲームキャラみたいなものだよ。[linebreak]誰か僕をリセットして。[linebreak]さっさとゲームオーバーにさせて……。[%p]"
msgstr "18700:就某种意义上，我确实很像游戏角色啊。[linebreak]拜托谁来重置一下我的记忆吧。[linebreak]快点让我GAME OVER啊……[%p]"

msgid "18800:[name]拓巳[line]「早く、殺してよ……」[%p]"
msgstr "18800:[name]拓巳[line]「你赶快杀了我吧……」[%p]"

msgid "18900:僕のその言葉に。[linebreak]緊張感が頂点に達していた群衆が、弾けた。[%p]"
msgstr "18900:听到我的话——[linebreak]紧张感达到顶点的群众，爆发了。[%p]"

msgid "19000:[name]祭参加者１[line]「死ね！」[%p]"
msgstr "19000:[name]祭典参加者1[line]「去死吧！」[%p]"

msgid "19100:[name]祭参加者２[line]「人でなし！」[%p]"
msgstr "19100:[name]祭典参加者2[line]「你不是人！」[%p]"

msgid "19200:[name]祭参加者３[line]「鬼畜！」[%p]"
msgstr "19200:[name]祭典参加者3[line]「太没人性了！」[%p]"

msgid "19300:[name]祭参加者４[line]「殺してやるよ！」[%p]"
msgstr "19300:[name]祭典参加者4[line]「我要杀了你！」[%p]"

msgid "19400:[name]祭参加者５[line]「地獄に堕ちろ！」[%p]"
msgstr "19400:[name]祭典参加者5[line]「下地狱吧！」[%p]"

msgid "19500:[name]祭参加者６[line]「死刑にしろ！」[%p]"
msgstr "19500:[name]祭典参加者6[line]「判他死刑！」[%p]"

msgid "19600:[name]祭参加者７[line]「ブチ殺せ！」[%p]"
msgstr "19600:[name]祭典参加者7[line]「宰了他！」[%p]"

msgid "19700:[name]祭参加者８[line]「殺人鬼！」[%p]"
msgstr "19700:[name]祭典参加者8[line]「杀人魔！」[%p]"

msgid "19800:人の波が、殺到する。[linebreak]うねりに呑み込まれ。[linebreak]怒号と悲鳴が交錯し。[%p]"
msgstr "19800:人群蜂拥而至。[linebreak]被人潮吞没。[linebreak]怒吼与惨叫交错响起。[%p]"

msgid "19900:僕も押し潰され、もみくちゃにされる。[%p]"
msgstr "19900:我也被挤扁，变得一团乱。[%p]"

msgid "20000:四方から人に押され、身体が浮いてしまう。[linebreak]足の裏が地面から離れ。[%p]"
msgstr "20000:从四面八方被人推挤，身体都快飘起来了。[linebreak]脚底离开了地面。[%p]"

msgid "20100:もがいても、どうすることもできない。[linebreak]頭を叩かれ、腕を殴られ。[%p]"
msgstr "20100:无论怎么挣扎，都无济于事。[linebreak]头被敲打，手臂也被痛揍了一顿。[%p]"

msgid "20200:でも死ぬほどの痛みじゃなくて。[linebreak]窒息しそうなほどの息苦しさにも、僕は抵抗せず。[%p]"
msgstr "20200:但不是会让人死去的疼痛。[linebreak]即使痛苦到快要窒息，我也没有反抗。[%p]"

msgid "20300:この混乱状態に身を任せる。[linebreak]すると圧迫感がふと消えた。[linebreak]その場に倒れ込む。[%p]"
msgstr "20300:委身于这种混乱的状态中。[linebreak]压迫感忽然消失了。[linebreak]我当场瘫倒在地。[%p]"

msgid "20400:星来たんが、僕の手からこぼれ落ちる。[linebreak]それはアスファルトの上を転がっていく。[%p]"
msgstr "20400:星星来来从我的手中滑落。[linebreak]在柏油路上滚动着。[%p]"

msgid "20500:いくつもの足が森の木々のように林立し、蠢いていて、その隙間を僕の嫁が転がっていく。[linebreak]必死に手を伸ばしても、届かない。[%p]"
msgstr "20500:好几只脚有如森林树木般林立，不停蠕动着，我的老婆就滚落在它们的缝隙间。[linebreak]即使拼命伸手也够不着她。[%p]"

msgid "20600:まぶしいほどの笑みを浮かべていた顔面が、[linebreak]僕を癒してくれた笑顔が、[linebreak]誰かの革靴に踏まれてしまう。[%p]"
msgstr "20600:原本浮现灿烂笑容的脸庞——[linebreak]治愈了我的那张笑脸——[linebreak]被某人的皮鞋踩到了。[%p]"

msgid "20700:[name]星来[line]「タッキー！　痛いよぅ、助けて！　タッキー！」[%p]"
msgstr "20700:[name]星来[line]「小高公子——好痛哦，救我啊！小高公子——」[%p]"

msgid "20800:さらにその小さな身体は蹴られて転がり、[linebreak]女の赤いヒールによって、[linebreak]胴体を、ぐしゃりと踏み潰された。[%p]"
msgstr "20800:娇小的身体被踢得在地上翻滚。[linebreak]在女人红色高跟鞋下——[linebreak]身体被狠狠踩烂了。[%p]"

msgid "20900:[name]星来[line]「タ……キー……ぼ……」[%p]"
msgstr "20900:[name]星来[line]「阿……哉……我……」[%p]"

msgid "21000:[name]拓巳[line]「星来……？」[%p]"
msgstr "21000:[name]拓巳[line]拓巳[%p]"

msgid "21100:呼びかけても、返事は返ってこない。[%p]"
msgstr "21100:我呼唤她，但没有得到回应。[%p]"

msgid "21200:僕の唯一の味方も、いなくなった。[%p]"
msgstr "21200:我唯一的同伴也消失了。[%p]"

msgid "21300:悲しさで、胸が潰れそうで。[linebreak]この世界に生きる意味はこれで完全に消失した。[%p]"
msgstr "21300:悲伤几乎要压垮我的胸口。[linebreak]活在这个世界的理由已经彻底消失了。[%p]"

msgid "21400:見上げれば。[linebreak]夜空さえも、人の壁に囲まれ、僕の視界からは覆い隠されて。[%p]"
msgstr "21400:抬头一看——[linebreak]连夜空也被人群包围，遮蔽了我的视野。[%p]"

msgid "21500:誰も、僕がここにしゃがみ込んでいることに気付かない。[%p]"
msgstr "21500:没有人发现我蹲在这里。[%p]"

msgid "21600:僕はここにいるのに。[linebreak]殺してくれって言ってるのに。[%p]"
msgstr "21600:明明我就在这里。[linebreak]明明我都说要你杀了我了。[%p]"

msgid "21700:どうしてみんな、僕を無視するんだ……！[%p]"
msgstr "21700:为什么大家都无视我的存在呢……！[%p]"

msgid "21800:[name]拓巳[line]「あ……」[%p]"
msgstr "21800:[name]拓巳[line]「啊……」[%p]"

msgid "21900:ふと。[%p]"
msgstr "21900:忽然间——[%p]"

msgid "22000:うなじのあたりに、感じる。[%p]"
msgstr "22000:后颈附近，有感觉了。[%p]"

msgid "22100:チリチリとした――[%p]"
msgstr "22100:刺痛的感觉——[%p]"

msgid "22200:“神の視線”の感覚。[%p]"
msgstr "22200:是「神之眼」的感觉。[%p]"

msgid "22300:[name]？？？[line]「君は――」[%p]"
msgstr "22300:[name]？？？[line]「你——」[%p]"

msgid "22400:僕の頭の中に、[%p]"
msgstr "22400:我的脑中……[%p]"

msgid "22500:[name]？？？[line]「ここで、死んじゃダメだ」[%p]"
msgstr "22500:[name]？？？[line]「不可以死在这里。」[%p]"

msgid "22600:声が、響いた。[%p]"
msgstr "22600:声音响起。[%p]"

msgid "22700:僕は息を呑み、[%p]"
msgstr "22700:我倒抽了一口气——[%p]"

msgid "22800:[name]？？？[line]「ディソードを、手に入れて」[%p]"
msgstr "22800:[name]？？？[line]「得到断罪者。」[%p]"

msgid "22900:奥歯をきつく噛みしめた。[%p]"
msgstr "22900:我咬紧牙关。[%p]"

msgid "23000:[name]拓巳[line]「……『将軍』……！」[%p]"
msgstr "23000:[name]拓巳[line]「……『将军』……！」[%p]"

msgid "23100:林立する足の群れの中に、あの車椅子を探す。ダーススパイダーのコスプレ野郎を探す。[%p]"
msgstr "23100:我在四处林立的大腿中寻找那台轮椅，寻找那个Cosplay成达斯蛛人的家伙。[%p]"

msgid "23200:この“祭り”の主催者は、やっぱり、君なのか。[linebreak]また悪趣味なコスプレをして、僕を脅すのか。[linebreak]前と同じことを再現しようとしてるのか。[%p]"
msgstr "23200:这场「庆典」的主办人果然是你吗？[linebreak]又要用这种低级的兴趣Cosplay来威胁我了吗？[linebreak]他想重现之前发生过的事吗？[%p]"

msgid "23300:僕が望むのは、死だ。[linebreak]生きたまま見せ物にされるのは、まっぴらだよ……。[%p]"
msgstr "23300:我所期望的是死亡。[linebreak]我才不要活生生地被当成展示品呢……[%p]"

msgid "23400:[name]将軍[line]「あのときの、自称『将軍』は、僕じゃない」[%p]"
msgstr "23400:[name]将军[line]「那时候，那个自称是『将军』的人，并不是我。」[%p]"

msgid "23500:[name]将軍[line]「僕は、コスプレはしないよ」[%p]"
msgstr "23500:[name]将军[line]「我不玩角色扮演哦。」[%p]"

msgid "23600:それこそ、どうでもいい……。[linebreak]いいから、早く、殺して……。[%p]"
msgstr "23600:那才真的不重要呢……[linebreak]别管了，快点杀了我吧……[%p]"

msgid "23700:[name]将軍[line]「あいつらが、動き出しているんだ」[%p]"
msgstr "23700:[name]别说了，快点杀掉我吧……[line]「那些家伙开始行动了。」[%p]"

msgid "23800:[name]将軍[line]「この場は、今以上の混乱になるよ」[%p]"
msgstr "23800:[name]「那些家伙开始行动了。」[line]「这里会变得比现在更乱哦。」[%p]"

msgid "23900:あいつら……？[%p]"
msgstr "23900:那些家伙是指谁啊……？[%p]"

msgid "24000:[name]将軍[line]「すべての黒幕。この混乱を引き起こした者たち。混乱を利用して、利己を追求しようとする者たち」[%p]"
msgstr "24000:[name]那些家伙是指谁啊……？[line]「一切的幕后黑手，引发这场混乱的人们，利用这场混乱谋求自身利益的人们。」[%p]"

msgid "24100:[name]将軍[line]「彼らを止められるのは……君だけなんだ」[%p]"
msgstr "24100:[name]「将军」[line]「能够阻止他们的……只有你而已。」[%p]"

msgid "24200:身体を起こす。[%p]"
msgstr "24200:我撑起身子。[%p]"

msgid "24300:怒声は相変わらず僕の頭上で渦巻いている。[%p]"
msgstr "24300:怒吼依然在我头上回荡着。[%p]"

msgid "24400:誰も僕に見向きもせず。[linebreak]野次馬も、マスコミも、入り乱れて。[linebreak]つかみ合い、罵り合い、殴り合っていた。[%p]"
msgstr "24400:没有人愿意看我一眼。[linebreak]围观群众和媒体都混在一起了。[linebreak]他们互相拉扯，互相叫骂，互相斗殴。[%p]"

msgid "24500:なんで、この人たちは――[%p]"
msgstr "24500:为什么这些人会——[%p]"

msgid "24600:[name]将軍[line]「沈黙の兵器……」[%p]"
msgstr "24600:[name]将军。[line]「沉默武器……」[%p]"

msgid "24700:[name]将軍[line]「『ノアⅡ』はすでに稼働してる」[%p]"
msgstr "24700:[name]将军。[line]「『诺亚Ⅱ』已经启动了。」[%p]"

msgid "24800:[name]将軍[line]「この混乱が企図された一番の目的は――」[%p]"
msgstr "24800:[name]将军。[line]「这场混乱最大的目的是——」[%p]"

msgid "24900:[name]将軍[line]「無差別人体実験」[%p]"
msgstr "24900:[name]将军。[line]「无差别人体实验。」[%p]"

msgid "25000:[name]拓巳[line]「……っ」[%p]"
msgstr "25000:[name]拓巳[line]「……！」[%p]"

msgid "25100:頭に、なにか硬いものが当たった。[%p]"
msgstr "25100:我的头被某种坚硬的东西打中了。[%p]"

msgid "25200:激痛が走る。頭蓋骨が割れたかと思うほどの痛み。たまらず頭を抱える。[%p]"
msgstr "25200:一阵剧痛窜过，疼痛剧烈到让人以为是头盖骨裂开了。我忍不住抱住自己的脑袋。[%p]"

msgid "25300:涙目になりながら見ると、近くにこぶし大の石が転がっていた。血がわずかにこびりついている。[%p]"
msgstr "25300:我眼眶泛泪地看向石头，发现附近有颗拳头大小的石头掉在地上，上面还沾着一点血迹。[%p]"

msgid "25400:誰かが投げたあの石が、直撃したらしい。[%p]"
msgstr "25400:看来是有人丢出那块石头砸中了我。[%p]"

msgid "25500:ズキズキとした痛み。手をやると、ぬるりとしたものが指に触れた。[linebreak]指先に、血がついていた。[linebreak]僕の、血。[%p]"
msgstr "25500:一阵刺痛传来，我把手伸过去，手指摸到了某种粘稠的东西。[linebreak]是血沾在了指尖上。[linebreak]是我的，血。[%p]"

msgid "25600:[name]祭参加者２[line]「きゃああああ！」[%p]"
msgstr "25600:[name]祭典参加者2[line]「呀啊啊啊啊！」[%p]"

msgid "25700:遠くで、乾いた炸裂音。[linebreak]直後に女性の金切り声。[%p]"
msgstr "25700:远方传来清脆的爆炸声。[linebreak]紧接着是女人尖锐的声音。[%p]"

msgid "25800:銃声……？[linebreak]銃を持ってる人間が、この場所にいた……？[%p]"
msgstr "25800:枪声……？[linebreak]有带枪的人在这里吗……？[%p]"

msgid "25900:どうしてみんな、こんなに興奮してるんだ……。[%p]"
msgstr "25900:为什么大家会这么兴奋啊……[%p]"

msgid "26000:どうしてみんな、まったく関係ない人たち同士で諍いを起こしてるんだ……。[%p]"
msgstr "26000:为什么毫无关系的人们要互相争执呢……[%p]"

msgid "26100:どうして、こんなに混沌とした状態になっているんだ……。[%p]"
msgstr "26100:为什么会变成这么混乱的状态啊……[%p]"

msgid "26200:みんなが待っていた、今日の“悪者”“生け贄”は、僕じゃないの？[%p]"
msgstr "26200:大家等待着今天的“坏人”，“祭品”，不是我吗？[%p]"

msgid "26300:[name]将軍[line]「渋谷のＧＥレート……バイオリズムが、あり得ないくらいに上昇してるんだよ」[%p]"
msgstr "26300:[name]将军。[line]「涩谷的GE汇率……生物节律上升到不可思议的地步了哦！」[%p]"

msgid "26400:[name]将軍[line]「言ったでしょ？　無差別人体実験だって」[%p]"
msgstr "26400:[name]将军。[line]「我说过了吧？这是无差别的人体实验啊！」[%p]"

msgid "26500:[name]将軍[line]「この場にいる人たちは今、見えない“攻撃”を受けているんだ」[%p]"
msgstr "26500:[name]将军。[line]「现在，在场的所有人都受到了看不见的『攻击』。」[%p]"

msgid "26600:あの、音がした。[linebreak]軋むような、[linebreak]錆び付くような、[%p]"
msgstr "26600:那个声音又响起了。[linebreak]像是在嘎吱作响——[linebreak]仿佛生锈了一般——[%p]"

msgid "26700:息も絶え絶えの、死にかけた女性の吐息のような、[%p]"
msgstr "26700:像是奄奄一息，即将死去的女人所发出的气息一般——[%p]"

msgid "26800:車椅子の車輪の音。[%p]"
msgstr "26800:轮椅车轮转动的声音。[%p]"

msgid "26900:これだけの人の数。[linebreak]無秩序にうねっている人の壁。[%p]"
msgstr "26900:这么多人。[linebreak]杂乱无章的人墙。[%p]"

msgid "27000:にもかかわらず、車椅子に乗った『将軍』は、誰にもぶつかることなく、誰にも絡まれることなく、[linebreak]僕のところへ、まっすぐに進んできた。[%p]"
msgstr "27000:尽管如此，坐在轮椅上的「将军」却没撞到任何人，也没被任何人缠上。[linebreak]笔直地朝我这里前进。[%p]"

msgid "27100:とても小さくて、ガリガリにやせ細った身体。[linebreak]皺だらけで、触れれば折れてしまいそうなくらい弱々しい。[%p]"
msgstr "27100:非常瘦小，骨瘦如柴的身体。[linebreak]满是皱纹，仿佛一碰就会断掉般脆弱不堪。[%p]"

msgid "27200:その瞳は、白濁していて。[linebreak]なにを見ているのか、なにが見えているのか、分からない。[%p]"
msgstr "27200:那双眼睛混浊泛白。[linebreak]不知道它在看什么，也不知道它看见了什么。[%p]"

msgid "27300:僕は彼から目を離せなくなる。[linebreak]周囲の喧噪が、急速に遠ざかっていく。[%p]"
msgstr "27300:我的视线无法从他身上移开。[linebreak]周围的嘈杂声迅速远去。[%p]"

msgid "27400:[name]将軍[line]「ずっと、君を見ていたよ」[%p]"
msgstr "27400:[name]将军[line]「我一直看着你哦。」[%p]"

msgid "27500:初めて会ったときと同じ声音だった。[linebreak]Ｏ－ＦＲＯＮＴのときのような、変な声じゃない。[%p]"
msgstr "27500:和初次见面时一样的嗓音。[linebreak]不是像O - FRONT那时一样奇怪的声音。[%p]"

msgid "27600:[name]拓巳[line]「君は、誰なの？」[%p]"
msgstr "27600:[name]拓巳[line]「你是谁？」[%p]"

msgid "27700:[name]将軍[line]「僕は、君だよ」[%p]"
msgstr "27700:[name]将军[line]「我就是你。」[%p]"

msgid "27800:その一言で、僕は悟った。[%p]"
msgstr "27800:这句话让我恍然大悟。[%p]"

msgid "27900:[name]拓巳[line]「僕を……妄想して……生み出したのも――」[%p]"
msgstr "27900:[name]拓巳[line]「把我……幻想出来的人——」[%p]"

msgid "28000:[name]拓巳[line]「“神の視線”の正体も――」[%p]"
msgstr "28000:[name]拓巳[line]拓巳[%p]"

msgid "28100:[name]拓巳[line]「君、だったんだね……」[%p]"
msgstr "28100:[name]拓巳[line]拓巳[%p]"

msgid "28200:[name]将軍[line]「うん」[%p]"
msgstr "28200:[name]将军[line]「嗯。」[%p]"

msgid "28300:こいつが、僕の生みの親。[linebreak]僕を妄想した存在。[linebreak]僕にとっては、どうしてもかなわない相手。[%p]"
msgstr "28300:这家伙就是我的亲生父母。[linebreak]是把我创造出来的存在。[linebreak]对我来说，是个无论如何都赢不了的对象。[%p]"

msgid "28400:[name]拓巳[line]「君の、名前は……？」[%p]"
msgstr "28400:[name]拓巳[line]「你的名字是……？」[%p]"

msgid "28500:[name]タクミ[line]「……西條、拓巳」[%p]"
msgstr "28500:[name]拓美[line]「……西条，拓美。」[%p]"

msgid "28600:本物の、僕。[linebreak]僕の、オリジナル。[%p]"
msgstr "28600:真正的我。[linebreak]我的原版。[%p]"

msgid "28700:[name]拓巳[line]「君は……いくつ、なの？」[%p]"
msgstr "28700:[name]拓美[line]「你……几岁了？」[%p]"

msgid "28800:[name]タクミ[line]「君と同い年だよ。君の“設定”とね」[%p]"
msgstr "28800:[name]拓美[line]「和你一样哦，就你的『设定』来说的话。」[%p]"

msgid "28900:[name]拓巳[line]「…………」[%p]"
msgstr "28900:[name]「我和你同年哦，以你的『设定』而言的话。」[line]「…………」[%p]"

msgid "29000:１７歳……。[linebreak]こんな、小さくて、皺だらけなのに……。[%p]"
msgstr "29000:十七岁……[linebreak]明明这么小，布满皱纹……[%p]"

msgid "29100:[name]タクミ[line]「このままだと、世界が死んじゃう」[%p]"
msgstr "29100:[name]拓美——[line]「再这样下去的话，这个世界会毁灭。」[%p]"

msgid "29200:[name]タクミ[line]「ノアⅡに、あらゆる人の心は支配されちゃう」[%p]"
msgstr "29200:[name]拓美——[line]「所有人类的心灵，都会被诺亚Ⅱ所支配。」[%p]"

msgid "29300:[name]タクミ[line]「だから、君に託したんだ」[%p]"
msgstr "29300:[name]拓美——[line]拓美——[%p]"

msgid "29400:託した……って、なにを……。[%p]"
msgstr "29400:托付了什么……？[%p]"

msgid "29500:[name]拓巳[line]「意味が、分かんないよ……。そんなの……僕には……か、関係、ない」[%p]"
msgstr "29500:[name]托付了什么……？[line]「我不懂你的意思啦……那种事跟我一点关系也没有啊！」[%p]"

msgid "29600:[name]拓巳[line]「頼むよ……僕を、消して。今すぐに、消して……ほしい」[%p]"
msgstr "29600:[name]拓巳[line]「拜托你了……请你，让我消失吧。我希望你现在就立刻消灭我……」[%p]"

msgid "29700:『将軍』の皺が、蠢動する。[linebreak]表情を変えているらしいけど、ただ皺が動いているようにしか見えない。[%p]"
msgstr "29700:「将军」脸上的皱纹开始蠕动起来。[linebreak]虽然它似乎改变了表情，但看起来只像是皱纹在蠕动而已。[%p]"

msgid "29800:[name]タクミ[line]「全部、僕のせいなんだ」[%p]"
msgstr "29800:[name]拓美[line]「这一切都是我的错。」[%p]"

msgid "29900:[name]タクミ[line]「僕が『Ｉｒ２』を妄想しちゃったせいで」[%p]"
msgstr "29900:[name]拓美[line]「都是因为我妄想了『Ir2』的关系。」[%p]"

msgid "30000:[name]タクミ[line]「なんとか、止めようとしたけど……僕はこんな身体だから、満足に動くこともできない」[%p]"
msgstr "30000:[name]拓美[line]「我有试着阻止她，可是……因为我的身体的关系，没办法自由行动。」[%p]"

msgid "30100:[name]タクミ[line]「それで、君を生み出した」[%p]"
msgstr "30100:[name]拓美[line]「然后就创造了你。」[%p]"

msgid "30200:[name]タクミ[line]「君に、止めてもらうために」[%p]"
msgstr "30200:[name]「所以才创造出你来。」[line]「为了让你阻止我。」[%p]"

msgid "30300:[name]拓巳[line]「そんなの、知らない……。[linebreak]僕は、君のコピーだけど、僕は、やりたくない……」[%p]"
msgstr "30300:[name]「为了让你阻止我。」[line]「那种事，我不知道……[linebreak]我虽然是你的复制品，但我不想做这种事……」[%p]"

msgid "30400:[name]拓巳[line]「コピーだけが、苦しまなくちゃいけないなんて……そんなの、イヤだ」[%p]"
msgstr "30400:[name]「虽然我是你的复制品，但我还是不想这么做。」[line]「只有复制品非得受苦不可……我讨厌这样。」[%p]"

msgid "30500:[name]拓巳[line]「死なせてよ……」[%p]"
msgstr "30500:[name]「我不希望只有复制品必须承受痛苦。」[line]「让我去死吧……」[%p]"

msgid "30600:[name]タクミ[line]「君はコピーじゃない。もうひとりの、僕だ」[%p]"
msgstr "30600:[name]拓美[line]「你不是复制品，而是另一个我。」[%p]"

msgid "30700:[name]拓巳[line]「つまり、コピーじゃないか」[%p]"
msgstr "30700:[name]拓美[line]拓美[%p]"

msgid "30800:[name]拓巳[line]「君のしてることは、すごく、残酷だ……」[%p]"
msgstr "30800:[name]拓美[line]「你所做的事，非常地残忍啊……」[%p]"

msgid "30900:[name]タクミ[line]「悪かったって、思ってるよ」[%p]"
msgstr "30900:[name]拓美[line]「我觉得很抱歉。」[%p]"

msgid "31000:[name]タクミ[line]「でも、他に方法はなかった」[%p]"
msgstr "31000:[name]拓美[line]「可是，也没有其他办法了啊。」[%p]"

msgid "31100:[name]タクミ[line]「今、世界の命運は、君にかかっていると言っても過言じゃないんだ」[%p]"
msgstr "31100:[name]拓美[line]「现在，就算说世界的命运掌握在你的手上也不为过。」[%p]"

msgid "31200:[name]タクミ[line]「だから――」[%p]"
msgstr "31200:[name]拓美[line]「所以——」[%p]"

msgid "31300:[name]拓巳[line]「そんなの……」[%p]"
msgstr "31300:[name]拓美[line]「那种事……」[%p]"

msgid "31400:[name]拓巳[line]「そんなの、梨深に、頼めばいいじゃないか……」[%p]"
msgstr "31400:[name]拓美[line]「那种事情，拜托梨深不就好了吗……」[%p]"

msgid "31500:[name]拓巳[line]「知り合い、なんでしょ……」[%p]"
msgstr "31500:[name]拓美[line]「是熟人吧……」[%p]"

msgid "31600:“知り合い”[%p]"
msgstr "31600:“熟人”[%p]"

msgid "31700:意識的に、そう言ってしまった。[linebreak]恋人かもしれないとか、仲がいいとか、そんなの考えたくなかった。[%p]"
msgstr "31700:我刻意这么说了出来。[linebreak]我不愿去想我们可能是情侣，或是关系很好之类的事。[%p]"

msgid "31800:そして、自分の気持ちに、今さら気付いてしまった。[%p]"
msgstr "31800:然后我事到如今才察觉自己的心意。[%p]"

msgid "31900:僕は、僕のオリジナルに、嫉妬してる。[linebreak]妄想の存在のくせに。[%p]"
msgstr "31900:我在嫉妒我的原创曲。[linebreak]明明只是幻想中的存在。[%p]"

msgid "32000:なんて……醜怪で、見苦しいんだ。[%p]"
msgstr "32000:多么丑陋，不堪入目啊。[%p]"

msgid "32100:身の程知らずもいいところだよ。[%p]"
msgstr "32100:不知天高地厚也该有个限度吧。[%p]"

msgid "32200:自分の感情に反吐が出る。[%p]"
msgstr "32200:我对自己的情感感到作呕。[%p]"

msgid "32300:ますます鬱積した気分になった。[%p]"
msgstr "32300:心情越来越郁闷了。[%p]"

msgid "32400:死にたい……。[%p]"
msgstr "32400:好想死……。[%p]"

msgid "32500:[name]タクミ[line]「梨深は、七海を助けに行って……逆に、捕まっちゃったんだ」[%p]"
msgstr "32500:[name]拓美——[line]「梨深她去救你的时候……反而被抓住了。」[%p]"

msgid "32600:[name]拓巳[line]「え……」[%p]"
msgstr "32600:[name]拓美——[line]「咦……？」[%p]"

msgid "32700:[name]タクミ[line]「梨深はね、僕を、救おうとしてくれていた」[%p]"
msgstr "32700:[name]拓美——[line]「梨深她啊，是想要拯救我。」[%p]"

msgid "32800:[name]タクミ[line]「その理由は――」[%p]"
msgstr "32800:[name]拓美——[line]「那个理由就是——」[%p]"

msgid "32900:[name]タクミ[line]「君も“知っている”よね？」[%p]"
msgstr "32900:[name]拓美——[line]「你也『知道』吧？」[%p]"

