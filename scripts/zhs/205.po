# THIS FILE IS AUTOMATICALLY GENERATED BY generate_po.py.
                     
msgid "0:ゲロカエルんの専門ショップは、『カエル家（いえ）』という名前だった。[%p]"
msgstr "0:呕吐物专卖店的名字叫做「青蛙家」。[%p]"

msgid "100:ちょっとしたダジャレで客の印象を良くしようという狙いなのだろうが、はっきり言ってオヤジギャグレベルだ、と判はため息をついた。[%p]"
msgstr "100:虽然店长应该是想用一点小冷笑话来给客人留下好印象，但老实说这只能算是大叔等级的笑话而已——判叹了口气这么想着。[%p]"

msgid "200:『カエル家』は現在、首都圏に２０店舗ほどが展開しており、なおも拡大中だと言う。[%p]"
msgstr "200:「青蛙家」现在在首都圈内开了大约二十间分店，而且还在继续扩张当中。[%p]"

msgid "300:中でも１号店であり、ゲロカエルんファンからは聖地と呼ばれている原宿のショップを、判は訪れていた。[%p]"
msgstr "300:其中一号店位于原宿区，呕吐物粉丝们称之为圣地。而判现在就来到了这家分店。[%p]"

msgid "400:だが、彼は早くも来たことを後悔していた。[%p]"
msgstr "400:然而他很快就后悔来到这里了。[%p]"

msgid "500:[name]判[line]「うーん。なあ優愛ちゃん。か、帰っていいか……？」[%p]"
msgstr "500:[name]判[line]「嗯～小优爱，我可不可以回去了啊……？」[%p]"

msgid "600:同行し、店まで案内してくれた優愛に、小声でそうつぶやく。[%p]"
msgstr "600:判低声对带他来到这间店的小优爱这么说道。[%p]"

msgid "700:[name]優愛[line]「え、でも、百瀬さんからゲロカエルんを買ってくるように、[linebreak]頼まれてるんじゃ……？」[%p]"
msgstr "700:[name]优爱[line]「咦，可是你不是说要买呕吐袋给百濑同学吗？[linebreak]她不是这么拜托你的吗……？」[%p]"

msgid "800:[name]判[line]「だってよぉ、おじさんだけ明らかに浮いてるからさあ」[%p]"
msgstr "800:[name]判[line]「因为啊，只有叔叔明显格格不入嘛。」[%p]"

msgid "900:竹下通り沿いにあり、目の前の道を若者たちが楽しそうに歩いているが、ほとんどの人間がこのショップに目もくれずに通り過ぎていく。[%p]"
msgstr "900:这家店位于竹下通，眼前有许多年轻人开心地走在路上，但几乎没有人会看一眼这间店就直接走过去。[%p]"

msgid "1000:狭い店内にはあまり人はいない。[%p]"
msgstr "1000:狭窄的店里没什么客人。[%p]"

msgid "1100:今も客は判、優愛を含めて５人ほどで、判以外の全員が女子高生だった。[%p]"
msgstr "1100:现在包含判在内，店里的客人只有五个人左右，除了判之外都是女高中生。[%p]"

msgid "1200:判のような冴えない風体で、優愛と一緒に竹下通りを歩けば、淫行目的の男だと周囲には映ってしまうだろう。[%p]"
msgstr "1200:像判这种不起眼的人，要是和优爱一起走在竹下通，周围的人应该会以为他是想对优爱乱来的男人吧？[%p]"

msgid "1300:そして流行りのマスコットを買ってやる……。なんとステレオタイプなエロオヤジなのだろう。判は憂鬱になった。[%p]"
msgstr "1300:然后买个流行的吉祥物给她……多么典型的色老头啊，判忧郁了起来。[%p]"

msgid "1400:しかも――[%p]"
msgstr "1400:而且——[%p]"

msgid "1500:[name]判[line]「な〜んか、誰かに見られてるような気がするんだよなあ」[%p]"
msgstr "1500:[name]而且——[line]「总～觉得好像有人在看我耶。」[%p]"

msgid "1600:[name]判[line]「いい年して自意識過剰かもしんねえが」[%p]"
msgstr "1600:[name]「总～觉得好像有人在看我耶。」[line]「虽然都一把年纪了，这样讲可能有点自我意识过剩……」[%p]"

msgid "1700:[name]優愛[line]「あ、それ、わたしも、です」[%p]"
msgstr "1700:[name]优爱[line]「啊，那个，我也是。」[%p]"

msgid "1800:[name]優愛[line]「最近、誰かの視線を感じて……たまに、すごく、怖くなるんですよ……」[%p]"
msgstr "1800:[name]优爱[line]「我最近，会感觉到某人的视线……偶尔会觉得非常恐怖呢……」[%p]"

msgid "1900:[name]判[line]「なんだ、俺だけじゃなかったか」[%p]"
msgstr "1900:[name]判[line]「什么嘛，原来不是只有我啊。」[%p]"

msgid "2000:[name]判[line]「優愛ちゃんの場合は心配だな。[linebreak]ストーカーに付け狙われてたりしたら……」[%p]"
msgstr "2000:[name]「怎么？原来不只我一个人吗？」[line]「优爱的话，倒是挺让人担心呢。[linebreak]要是被跟踪狂盯上的话……」[%p]"

msgid "2100:[name]優愛[line]「そ、そんな……！　怖いこと言わないでください……」[%p]"
msgstr "2100:[name]优爱[line]「怎、怎么会这样……！请不要说这么可怕的话啦……」[%p]"

msgid "2200:[name]判[line]「安心しな。そんなヤツがいたら俺が逮捕してやるから」[%p]"
msgstr "2200:[name]判[line]「放心吧，要是有那种家伙的话我会去把他抓起来的。」[%p]"

msgid "2300:ニヤリと笑って話を切り上げ、判は棚に置かれている様々なカエルのマスコットをしげしげと眺めた。[%p]"
msgstr "2300:我咧嘴一笑，结束这个话题后，便仔细地盯着放在架子上的各种青蛙吉祥物。[%p]"

msgid "2400:[name]判[line]「ところで、優愛ちゃんはここに来るのは何度目だい？」[%p]"
msgstr "2400:[name]判[line]「话说回来，优爱是第几次来这里了呢？」[%p]"

msgid "2500:[name]優愛[line]「わ、わたしも初めてです……」[%p]"
msgstr "2500:[name]优爱[line]「我、我也一样第一次来……」[%p]"

msgid "2600:[name]判[line]「あんまり客がいねえようだが……」[%p]"
msgstr "2600:[name]「我、我也是第一次来……」[line]「看起来没什么客人呢……」[%p]"

msgid "2700:[name]判[line]「静かなブームってヤツなのか？」[%p]"
msgstr "2700:[name]「好像没什么客人耶……」[line]「是那种不愠不火的流行吗？」[%p]"

msgid "2800:[name]優愛[line]「えっと、金曜日から日曜日にかけては、すごく、混んでるらしいです」[%p]"
msgstr "2800:[name]优爱[line]「呃，从星期五到星期天好像会非常拥挤。」[%p]"

msgid "2900:[name]優愛[line]「あ、つまりその、新作が発売されて、週末の間だけ、ですね」[%p]"
msgstr "2900:[name]优爱[line]「啊，也就是说那个……只有在新刊发售的那个周末会很拥挤吧。」[%p]"

msgid "3000:[name]優愛[line]「特に金曜日は、ものすごく並ぶらしくて……」[%p]"
msgstr "3000:[name]优爱[line]「特别是星期五，听说会排很长的队伍……」[%p]"

msgid "3100:[name]優愛[line]「学生のために、１８時に販売開始なんですけど、噂だと、その行列だけで竹下通りが埋め尽くされる、とか……」[%p]"
msgstr "3100:[name]优爱[line]「为了学生，所以从下午六点开始贩售，不过听说光是排队的人潮就会把竹下通给塞满之类的……」[%p]"

msgid "3200:[name]判[line]「週末だけ、ねぇ。ますます怪しいな」[%p]"
msgstr "3200:[name]判[line]「只有周末啊，愈来愈可疑了呢。」[%p]"

msgid "3300:判はとりあえず、先週発売されたという“涙目ゲロカエルん”をふたつ持って、レジへ向かった。[%p]"
msgstr "3300:总之，判拿了两本上周发售的《泪眼汪汪快昏倒》，走向收银台。[%p]"

msgid "3400:優愛は買わないらしく、判の後ろをついてくるだけだ。[%p]"
msgstr "3400:优爱似乎不打算买，只是跟在判后面走着。[%p]"

msgid "3500:[name]ショップ店員[line]「しゃぁせぇ」[%p]"
msgstr "3500:[name]店员[line]「来人啊～」[%p]"

msgid "3600:ショップ店員は、髪を緑色に染めた、頭の悪そうな２０代の男だった。耳にピアスまでしていて、レジにいなければとても店員には見えない。[%p]"
msgstr "3600:店员是个头发染绿，看起来脑袋不太好的二十几岁男子。耳朵上还戴着耳环，要不是站在收银台前，实在看不出是店员。[%p]"

msgid "3700:少なくとも接客する格好ではなかった。[%p]"
msgstr "3700:至少那身打扮不适合接待客人。[%p]"

msgid "3800:その店員が、判と優愛とを無遠慮にジロジロと見つめてくる。[%p]"
msgstr "3800:那个店员毫不客气地盯着判和优爱打量了一番。[%p]"

msgid "3900:[name]ショップ店員[line]「娘さんへのプレゼントってヤツっすか？」[%p]"
msgstr "3900:[name]店员[line]「是送给令嫒的礼物吗？」[%p]"

msgid "4000:[name]判[line]「いやあ、そうなんだよねえ」[%p]"
msgstr "4000:[name]判[line]「哎呀，就是说啊。」[%p]"

msgid "4100:[name]判[line]「この子がどうしてもって言うから」[%p]"
msgstr "4100:[name]「哎呀，就是说啊。」[line]「是这孩子坚持要来的。」[%p]"

msgid "4200:[name]判[line]「まあ、たまには父親らしいことしてやらんとねえ。はっは」[%p]"
msgstr "4200:[name]「因为这孩子无论如何都要来。」[line]「哎呀，偶尔也要做点父亲该做的事嘛。哈哈！」[%p]"

msgid "4300:判は愛想笑いを浮かべ、自分より年下の店員にヘコヘコして見せた。[%p]"
msgstr "4300:判露出讨好的笑容，对年纪比自己小的店员频频点头哈腰。[%p]"

msgid "4400:優愛が驚いたような顔をしてうつむいてしまったが、構わずその“父親という設定”を続ける。[%p]"
msgstr "4400:优爱一脸惊讶地低下头，不过判还是继续扮演着『父亲』的角色。[%p]"

msgid "4500:[name]判[line]「それより、このカエルに興味あるんだけど、ちょっと話聞かせてもらっていいかな？」[%p]"
msgstr "4500:[name]判[line]「比起那个，我对这只青蛙很有兴趣。可以稍微跟我聊聊吗？」[%p]"

msgid "4600:[name]判[line]「ゲコカエルんて、そんなに人気あるの？」[%p]"
msgstr "4600:[name]判[line]「呱呱蛙有那么受欢迎吗？」[%p]"

msgid "4700:[name]ショップ店員[line]「っていうか、ゲロカエルんすよ。バンバン人気あってぇ、ヤバいっス」[%p]"
msgstr "4700:[name]店员[line]「是呱呱蛙啦，超受欢迎的，太猛了！」[%p]"

msgid "4800:[name]判[line]「ああ、ゲプカエルんね。いつも週末に、新作が出るんでしょう？」[%p]"
msgstr "4800:[name]店员[line]「啊，是呱呱蛙啦！他们周末都会出新作品对吧！」[%p]"

msgid "4900:[name]ショップ店員[line]「ゲロカエルんだって言ってんじゃん、マジで」[%p]"
msgstr "4900:[name]店员[line]「就说我是呱呱蛙啦，真的。」[%p]"

msgid "5000:[name]ショップ店員[line]「っていうか、新作とかそういう言い方、マジやめてよ。“新ガエル”だから」[%p]"
msgstr "5000:[name]店员[line]「话说，真的别用『新作品』这种说法啦。是『新呱呱蛙』才对。」[%p]"

msgid "5100:[name]ショップ店員[line]「金、土、日と超並んでぇ、バンバン売れるんでぇ。マジヤバいっすよ。マジでバンバン売れるっすから」[%p]"
msgstr "5100:[name]店员[line]「星期五六日排在一起，卖得有够好。真的太猛了啦。真的是大热销呢。」[%p]"

msgid "5200:[name]ショップ店員[line]「バンバンっすよマジで」[%p]"
msgstr "5200:[name]店员[line]店员[%p]"

msgid "5300:優愛が、ふと眉根をひそめる。[%p]"
msgstr "5300:优爱突然皱起眉头。[%p]"

msgid "5400:この店員の口癖なのか、“バンバン”という言葉に違和感を覚えた。[%p]"
msgstr "5400:不知道是不是这个店员的习惯，「砰砰」这个词让我感到很奇怪。[%p]"

msgid "5500:なぜこれほど強調するのだろう。意図的ではないのだろうが、判がこの若い店員に名前を呼び捨てにされているように錯覚してしまう。[%p]"
msgstr "5500:为什么要这么强调呢？虽然不是故意的，但听起来就像这个年轻店员在直呼我的名字一样。[%p]"

msgid "5600:[name]判[line]「これ、メイドインチャイナってラベルに書いてあるだけなんだけど、メーカーとか分かる？」[%p]"
msgstr "5600:[name]判[line]「这上面只写着Made in China，你知道制造商是谁吗？」[%p]"

msgid "5700:[name]ショップ店員[line]「は？　メーカー？」[%p]"
msgstr "5700:[name]店员[line]「啊？制造商？」[%p]"

msgid "5800:[name]ショップ店員[line]「っつーか、ゲロカエルんにブランドとかねーし。ゲロカエルんはゲロカエルんじゃね？　それで完結してね？」[%p]"
msgstr "5800:[name]店员[line]「是说，吐得要死根本没品牌可言吧。吐得要死就是吐得要死啊？这样就结束了吧？」[%p]"

msgid "5900:[name]ショップ店員[line]「おっさん、マジウケんだけど」[%p]"
msgstr "5900:[name]店员[line]「大叔，你真的超好笑耶。」[%p]"

msgid "6000:[name]ショップ店員[line]「教えとくとぉ、ゲロカエルんって作ってんじゃねぇから。マジ、バンバン産まれてっから」[%p]"
msgstr "6000:[name]店员[line]「先跟你说，我可没在做呕吐物。真的会生出一堆小鸡鸡来啦。」[%p]"

msgid "6100:[name]判[line]「…………」[%p]"
msgstr "6100:[name]判[line]「…………」[%p]"

msgid "6200:さすがの判も、これ以上話していると爆発しそうだったので、そこで切り上げて代金を払い、店を出た。[%p]"
msgstr "6200:再让判继续说下去的话，他可能会爆炸，所以我决定结束话题付完钱后离开店里。[%p]"

msgid "6300:[name]判[line]「ちっ、店員の教育がなってねえな」[%p]"
msgstr "6300:[name]判[line]「啧，看来你们没有好好教员工待客之道呢。」[%p]"

msgid "6400:[name]判[line]「今時の若い連中ときたら、ロクに会話もできやしねー」[%p]"
msgstr "6400:[name]判[line]「现在的年轻人啊，根本没办法好好对话呢。」[%p]"

msgid "6500:[name]判[line]「だいたいなんだあの格好は」[%p]"
msgstr "6500:[name]「说到现在的小鬼们，连个像样的对话都谈不来——」[line]「说起来那是什么打扮啊？」[%p]"

msgid "6600:[name]優愛[line]「……そ、そうですね」[%p]"
msgstr "6600:[name]优爱[line]「……是、是这样没错。」[%p]"

msgid "6700:[name]優愛[line]「あ……」[%p]"
msgstr "6700:[name]优爱[line]「啊……」[%p]"

msgid "6800:優愛が不意に足を止めた。[linebreak]判のスーツの裾をつかみ、引き留める。[%p]"
msgstr "6800:优爱突然停下脚步。[linebreak]她抓住判的西装衣摆，留住他。[%p]"

msgid "6900:[name]優愛[line]「み、見てください、判さん」[%p]"
msgstr "6900:[name]优爱[line]「判……判先生，请您看这个！」[%p]"

msgid "7000:[name]判[line]「どしたぁ？」[%p]"
msgstr "7000:[name]判[line]「怎么啦～？」[%p]"

msgid "7100:優愛が指さす方向には、白い２トントラックが停まっていた。その荷台から、今まさにいくつものダンボールが下ろされている。[%p]"
msgstr "7100:优爱手指的方向，有一辆白色双层卡车停在那里。现在正从货架上搬出几个纸箱。[%p]"

msgid "7200:そしてそれは、先ほどのゲロカエルんショップの裏へと続々と運び込まれていた。[%p]"
msgstr "7200:那些箱子，陆续被搬到刚才那个吐到翻掉的商店后面。[%p]"

msgid "7300:[name]判[line]「おっ！　明日発売になる、ゲジカエルんの新作か」[%p]"
msgstr "7300:[name]然后，优爱指的方向是——[line]「哦！这是明天要发售的蚰蜒怪的新刊吗？」[%p]"

msgid "7400:[name]優愛[line]「あ、あの、ゲロカエルんです――」[%p]"
msgstr "7400:[name]优爱[line]「那……那个，是呕吐怪——」[%p]"

msgid "7500:[name]判[line]「優愛ちゃん、よく見つけたな。偉いぞ」[%p]"
msgstr "7500:[name]判[line]「优爱，你居然能找到它啊？真了不起！」[%p]"

msgid "7600:[name]優愛[line]「え、いえそんな……」[%p]"
msgstr "7600:[name]判[line]「咦，不，请别这么说……」[%p]"

msgid "7700:トラックの荷台には、普通なら社名が入っていそうなものだったが、真っ白に塗られていてなにも書かれていない。[%p]"
msgstr "7700:卡车车斗上一般都会印有公司名称，但这个却涂得一片雪白，什么都没写。[%p]"

msgid "7800:ただ、運び込まれているダンボールには、ゲロカエルんのイラストとともに一応社名が入っていた。[%p]"
msgstr "7800:不过搬进来的纸箱上姑且还是有印着公司名称，只是旁边还画了呕吐物的图案就是了。[%p]"

msgid "7900:[name]判[line]「有限会社シンコー……聞いたことあるかい？」[%p]"
msgstr "7900:[name]不过搬进来的纸箱上姑且还是有印着公司名称，只是旁边还画了呕吐物的图案就是了。[line]「『振兴有限公司』……你听过吗？」[%p]"

msgid "8000:[name]優愛[line]「えと、ありません……」[%p]"
msgstr "8000:[name]优爱[line]「呃，没有耶……」[%p]"

msgid "8100:[name]判[line]「となれば、困ったときのモモちゃん頼みだな」[%p]"
msgstr "8100:[name]判[line]「既然如此，遇到困难时就只能拜托小桃了。」[%p]"

