# THIS FILE IS AUTOMATICALLY GENERATED BY generate_po.py.
                     
msgid "0:蒼井セナは、驚愕の思いで目の前の少女の顔を見つめていた。[%p]"
msgstr "0:苍井青生惊愕地凝视着眼前这名少女的脸庞。[%p]"

msgid "100:セナのディソードによる攻撃を難なく受け止めた、その翼のような剣は、明らかにディソードだ。[%p]"
msgstr "100:那把轻松挡下迪索德攻击，有如翅膀一般的剑，明显是迪索德没错。[%p]"

msgid "200:扱いに慣れていなければ、こうもタイミングよくセナの攻撃を受け止めることなどできない。[%p]"
msgstr "200:如果她不习惯使用那把剑的话，不可能这么刚好挡下瑟娜的攻击。[%p]"

msgid "300:[name]梢[line]「ふひゃー」[%p]"
msgstr "300:[name]梢[line]「呼呀——」[%p]"

msgid "400:[name]梢[line]「咲畑しゃんもギガロマニアーックスだったのら〜」[%p]"
msgstr "400:[name]「呼呀——」[line]「咲畑也是Giga浪漫迷克斯～」[%p]"

msgid "500:梢が、コンテナハウスの上から高見の見物をするかのように、セナたちを眺めていた。投げ出した足をブランブランと揺らしている。[%p]"
msgstr "500:梢站在货柜屋上方，仿佛在隔岸观火般看着瑟娜等人，还摇晃着伸长的双腿。[%p]"

msgid "600:ついさっき奇襲攻撃の邪魔をしたのに呑気なものだ、とセナは心の中で舌打ちした。[%p]"
msgstr "600:明明刚刚才妨碍了奇袭，居然还这么悠哉——瑟娜在心中咂嘴道。[%p]"

msgid "700:だが視線は、目の前にいる梨深の瞳からそらさない。[linebreak]対する梨深は、思い詰めたような表情をしていた。[%p]"
msgstr "700:但是视线却无法从眼前的梨深深处移开。[linebreak]相对地，梨深则是一副想不开的表情。[%p]"

msgid "800:[name]拓巳[line]「あ……あ……」[%p]"
msgstr "800:[name]拓巳[line]「啊……啊……」[%p]"

msgid "900:梨深の背後で、情けなくへたり込んでいる西條拓巳がうめき声を上げている。[%p]"
msgstr "900:在梨深背后，狼狈地瘫坐在地上的西条拓巳发出呻吟声。[%p]"

msgid "1000:セナの狙いはあくまで彼であり、梨深と戦うつもりなどなかった。ましてやディソードでの斬り合いなど、マンガやアニメじゃないんだし、およそ現実的ではない。[%p]"
msgstr "1000:瑟娜的目标只有他，根本没打算和梨深战斗。更何况用断罪者互砍这种事又不是漫画或动画，完全不现实。[%p]"

msgid "1100:[name]セナ[line]「その男がなにをしたか、知っているのか」[%p]"
msgstr "1100:[name]瑟娜[line]「你知道那个男人做了什么吗？」[%p]"

msgid "1200:[name]セナ[line]「Ｉｒ２という公式を――」[%p]"
msgstr "1200:[name]「你有听说那名男子干了什么事吗？」[line]「I2R方程式——」[%p]"

msgid "1300:[name]梨深[line]「知ってるよ」[%p]"
msgstr "1300:[name]梨深[line]「我知道。」[%p]"

msgid "1400:セナの言葉は遮られた。[linebreak]静かな、悲しそうな、だがはっきりとした答え。[%p]"
msgstr "1400:瑟娜的话被盖掉了。[linebreak]平静，悲伤，但又明确的回答。[%p]"

msgid "1500:それを聞き、セナはディソードを握る手に力をこめる。[%p]"
msgstr "1500:听到这句话后，瑟娜更加用力地握住迪索德。[%p]"

msgid "1600:[name]セナ[line]「それなら――！」[%p]"
msgstr "1600:[name]瑟娜[line]「既然如此——」[%p]"

msgid "1700:[name]梨深[line]「タクを覚醒させれば」[%p]"
msgstr "1700:[name]梨深[line]「只要让阿拓觉醒的话……」[%p]"

msgid "1800:[name]梨深[line]「もっと、大変なことになる」[%p]"
msgstr "1800:[name]梨深[line]「事情会变得更严重。」[%p]"

msgid "1900:[name]セナ[line]「その前に殺す」[%p]"
msgstr "1900:[name]瑟娜[line]「我会先杀了你。」[%p]"

msgid "2000:[name]梨深[line]「あなたには」[%p]"
msgstr "2000:[name]「在那之前，我就会杀了你。」[line]「你——」[%p]"

msgid "2100:[name]梨深[line]「無理だよ」[%p]"
msgstr "2100:[name]「你——」[line]「不可能。」[%p]"

msgid "2200:話にならない、とセナは判断した。[linebreak]居合いの構えを取る。[%p]"
msgstr "2200:瑟娜认为根本没得谈。[linebreak]她摆出拔刀术的架式。[%p]"

msgid "2300:一方の梨深は、無造作に立っているだけ。[%p]"
msgstr "2300:另一方面，梨深只是随意地站着而已。[%p]"

msgid "2400:へたり込んだままの西條拓巳までの距離は、大股でおよそ５歩。３秒あれば決着を付けられる距離だ。[%p]"
msgstr "2400:距离瘫坐在地上的西条拓已，大约是五步的距离。只要三秒钟就能分出胜负了。[%p]"

msgid "2500:[name]セナ[line]「…………」[%p]"
msgstr "2500:[name]瑟娜[line]「…………」[%p]"

msgid "2600:セナは躊躇せず、[%p]"
msgstr "2600:瑟娜毫不犹豫地——[%p]"

msgid "2700:梨深に正面から突進した。[%p]"
msgstr "2700:从正面冲向梨深。[%p]"

msgid "2800:彼女目がけて、ディソードを横凪ぎに払う。[%p]"
msgstr "2800:朝着她，我横向劈出迪索德。[%p]"

msgid "2900:梨深はそれを、先ほどと同じように受け止めようとする。[%p]"
msgstr "2900:梨深就像方才一样接下这一剑。[%p]"

msgid "3000:だが――[%p]"
msgstr "3000:然而——[%p]"

msgid "3100:[name]梨深[line]「あっ！？」[%p]"
msgstr "3100:[name]梨深[line]「啊！？」[%p]"

msgid "3200:セナの一閃はフェイク。[%p]"
msgstr "3200:瑟娜的一闪是假动作。[%p]"

msgid "3300:剣は梨深の剣と身体をすり抜けてしまう。[%p]"
msgstr "3300:剑穿过梨深的身体与她的刀刃之间。[%p]"

msgid "3400:リアルブートを解いていた。[%p]"
msgstr "3400:她解除了现实启动。[%p]"

msgid "3500:今のセナのディソードは、妄想でしかなく。[%p]"
msgstr "3500:瑟娜现在的迪索尔巴德，只是她的幻想。[%p]"

msgid "3600:なににも触れられず。[linebreak]なにも斬れず。[linebreak]なにも殺せない。[%p]"
msgstr "3600:无法触及任何事物。[linebreak]什么都砍不到。[linebreak]什么人都杀不了。[%p]"

msgid "3700:けれどそれは狙い通り。[linebreak]攻撃を受け止めるべく身構えていた梨深は、反応が遅れる。[%p]"
msgstr "3700:不过，这也在我的预料之中。[linebreak]为了挡下攻击而摆出架式的梨深反应慢了一拍。[%p]"

msgid "3800:その間に、セナはすでに梨深の横をすり抜け、拓巳まであと２歩と迫っていた。[%p]"
msgstr "3800:在这段期间，瑟娜已经穿过梨深身边，距离拓巳只剩两步之遥了。[%p]"

msgid "3900:一瞬のうちに、再度リアルブート。[%p]"
msgstr "3900:一瞬间再次进行真实跳转。[%p]"

msgid "4000:これで――！[%p]"
msgstr "4000:这样就——！[%p]"

msgid "4100:拓巳の目は恐怖の色をたたえてセナに向けられており、[linebreak]労せずしてリアルブートが完了する。[%p]"
msgstr "4100:拓巳用充满恐惧的眼神看着瑟娜。[linebreak]不费吹灰之力就完成了真实跳转。[%p]"

msgid "4200:再び現実のものとなったディソードを、[linebreak]拓巳の心臓へと穿とうとして――[%p]"
msgstr "4200:再次化为现实的迪索尔巴德——[linebreak]准备贯穿拓巳的心脏时——[%p]"

msgid "4300:[name]？？？[line]「……セナ」[%p]"
msgstr "4300:[name]？？？[line]「……瑟娜。」[%p]"

msgid "4400:[name]セナ[line]「……！？」[%p]"
msgstr "4400:[name]瑟娜[line]「……！？」[%p]"

msgid "4500:視界の隅に、何者かの影がよぎる。[linebreak]その声を聞いた瞬間、激しく動揺したセナは狙いを外し、[%p]"
msgstr "4500:视野角落闪过某人的身影。[linebreak]听到这个声音，瑟娜瞬间动摇，射偏了目标。[%p]"

msgid "4600:剣の切っ先は拓巳の足をかすめて、コンクリートに突き刺さった。[%p]"
msgstr "4600:剑尖掠过拓巳的脚，插进水泥地里。[%p]"

msgid "4700:セナはゆっくりと顔を上げ、[%p]"
msgstr "4700:瑟娜缓缓抬起头来——[%p]"

msgid "4800:[name]セナ[line]「お、まえ……」[%p]"
msgstr "4800:[name]瑟娜[line]「你这混账东西……」[%p]"

msgid "4900:うなるような声を発した。[%p]"
msgstr "4900:发出低吼般的声音。[%p]"

msgid "5000:屋上の縁に。[linebreak]沈みかけた夕陽を背景にして。[linebreak]ひとりの男が幽鬼のように立っていた。[%p]"
msgstr "5000:在屋顶边缘。[linebreak]以即将西沉的夕阳为背景——[linebreak]有个男人如幽魂般站在那里。[%p]"

msgid "5100:顔色が悪く、土気色をしていて。[%p]"
msgstr "5100:脸色苍白，毫无生气。[%p]"

msgid "5200:落ちくぼんだ目の中に、充血した瞳。[%p]"
msgstr "5200:凹陷的眼睛里布满血丝。[%p]"

msgid "5300:無表情のまま、うなだれている。[%p]"
msgstr "5300:面无表情地垂着头。[%p]"

msgid "5400:その姿を見た途端、セナは拓巳のことなどどうでもよくなった。[%p]"
msgstr "5400:一看到拓巳，瑟娜就不再在意他了。[%p]"

msgid "5500:セナがずっと追い続けていた男。[linebreak]憎しみの対象。[%p]"
msgstr "5500:她一直在追查的男人。[linebreak]憎恨的对象。[%p]"

msgid "5600:セナを捨て、[linebreak]家族を裏切った男。[%p]"
msgstr "5600:抛弃瑟娜——[linebreak]背叛了家人的男人。[%p]"

msgid "5700:その男が、目の前にいる。[%p]"
msgstr "5700:那个男人，就在眼前。[%p]"

msgid "5800:[name]セナ[line]「ようやく、見つけた……っ」[%p]"
msgstr "5800:[name]瑟娜——[line]「终于找到了……！」[%p]"

msgid "5900:セナは男の方へと足を踏み出し、[linebreak]歯噛みし、こみ上げる憎悪をこらえる。[linebreak]ディソードを構え直す。[%p]"
msgstr "5900:瑟娜朝男人的方向跨出一步。[linebreak]她咬紧牙关，忍住涌上心头的恨意。[linebreak]重新举起迪索德。[%p]"

msgid "6000:さらに一歩、前へ。[%p]"
msgstr "6000:再往前踏出一步。[%p]"

msgid "6100:[name]男[line]「どうしようもなかった……」[%p]"
msgstr "6100:[name]男人[line]「我无能为力啊……」[%p]"

msgid "6200:[name]男[line]「私は、お前の母さんを生け贄に捧げるしかなかったんだ」[%p]"
msgstr "6200:[name]「我根本束手无策啊……」[line]「因为我只能把你妈当成祭品献出去。」[%p]"

msgid "6300:[name]セナ[line]「黙れ……」[%p]"
msgstr "6300:[name]瑟娜[line]「闭嘴……」[%p]"

msgid "6400:[name]男[line]「母さんの最期は、とても安らかなものだったよ。[linebreak]だからセナ、安心してほしい」[%p]"
msgstr "6400:[name]男人[line]「妈妈最后走得非常安详哦。[linebreak]所以瑟娜，你大可放心。」[%p]"

msgid "6500:[name]男[line]「母さんのおかげで、素晴らしい実験結果が出た。[linebreak]母さんは、世界のために役立ったんだ」[%p]"
msgstr "6500:[name]「所以瑟娜，请你放心吧。」[line]「多亏了妈妈，我得到了非常棒的实验结果哦。[linebreak]妈妈为这个世界做出了贡献呢。」[%p]"

msgid "6600:[name]男[line]「お前も、母さんの功績を誇っていい」[%p]"
msgstr "6600:[name]「多亏了妈妈，我得到了非常棒的实验结果哦。[line]「你也可以为妈妈的功劳感到骄傲哦。」[%p]"

msgid "6700:[name]セナ[line]「黙れよぉぉっ！！」[%p]"
msgstr "6700:[name]瑟娜[line]「给我闭嘴——！」[%p]"

msgid "6800:頭に血が上ったセナには、もはや男の姿しか目に入らない。[%p]"
msgstr "6800:瑟娜已经气到失去理智，眼中只有那个男人的身影了。[%p]"

msgid "6900:駆ける。[%p]"
msgstr "6900:奔驰。[%p]"

msgid "7000:一瞬だった。[%p]"
msgstr "7000:就只有一瞬间而已。[%p]"

msgid "7100:セナはあっという間に距離を詰めると、[linebreak]男の懐に飛び込み、[linebreak]深々とディソードを突き刺していた。[%p]"
msgstr "7100:瑟娜在转眼间拉近了距离——[linebreak]冲进男人怀里。[linebreak]深深地把迪索德插了进去。[%p]"

msgid "7200:[name]セナ[line]「はあっ、はあっ、はあっ」[%p]"
msgstr "7200:[name]瑟娜——[line]「呼……呼……呼……」[%p]"

msgid "7300:それが、セナの念願だった。[%p]"
msgstr "7300:这就是瑟娜的心愿。[%p]"

msgid "7400:この男を殺すためだけに、毎日のように渋谷を徘徊していた。[linebreak]この男を殺す日のことを、ずっと夢見ていた。[%p]"
msgstr "7400:为了杀死这个男人，她几乎每天都在涩谷游荡。[linebreak]一直梦想着杀了这个男人的日子到来。[%p]"

msgid "7500:[name]セナ[line]「やっと……母さんの、仇を……」[%p]"
msgstr "7500:[name]瑟娜[line]「终于……替妈妈，报仇了……」[%p]"

msgid "7600:剣を抜こうとした。[linebreak]男から離れようとした。[%p]"
msgstr "7600:我准备拔剑。[linebreak]我想离开那个男人身边。[%p]"

msgid "7700:だが、剣は抜けず。[linebreak]男が、セナを包み込むようにして、抱きしめてくる。[%p]"
msgstr "7700:但是，我无法拔出剑。[linebreak]男人像是要包裹住瑟娜似的紧紧抱住她。[%p]"

msgid "7800:[name]セナ[line]「よ、よせっ……気安く触るな……っ！」[%p]"
msgstr "7800:[name]瑟娜[line]「别、别这样……不要随便碰我……！」[%p]"

msgid "7900:全身が総毛立つ。[linebreak]今さらそんなことをされても、おぞましいだけだ。[%p]"
msgstr "7900:全身寒毛直竖。[linebreak]事到如今，就算被她这么做也只会感到恶心而已。[%p]"

msgid "8000:もうとっくの昔に、[linebreak]セナと男の間にあった家族の絆は、断ち切れている。[%p]"
msgstr "8000:早在很久以前——[linebreak]瑟娜和那个男人之间的家人羁绊，早就已经切断了。[%p]"

msgid "8100:それなのに、セナは、なぜか男を振り払えない自分に愕然となった。[%p]"
msgstr "8100:明明是这样才对，但不知为何瑟娜却无法甩开那个男人，这让她感到十分错愕。[%p]"

msgid "8200:温もり。[linebreak]大きな胸。[%p]"
msgstr "8200:温暖。[linebreak]丰满的胸部。[%p]"

msgid "8300:父というリアルな存在感。[linebreak]赤く染まっていくセナの手。[%p]"
msgstr "8300:父亲这种真实的存在感。[linebreak]瑟娜的手逐渐被鲜血染红。[%p]"

msgid "8400:[name]セナ[line]「わ、私は……」[%p]"
msgstr "8400:[name]瑟娜[line]「我、我是……」[%p]"

msgid "8500:[name]セナ[line]「こんなの……っ」[%p]"
msgstr "8500:[name]「我、我是……」[line]「这种事……！」[%p]"

msgid "8600:[name]梨深[line]「あなたが、望んだ妄想だよ」[%e]"
msgstr "8600:[name]梨深[line]「这是你所期望的幻想。」[%e]"

msgid "8700:[name]男[line][margin top=\"266\"][margin left=\"350\"]「お前が、望んだ妄想だ」[%e]"
msgstr "8700:[name]男人[line][margin top=\"266\"][margin left=\"350\"]「这是你所期望的幻想。」[%e]"

msgid "8800:[name]セナ[line]「――――っ」[%p]"
msgstr "8800:[name]瑟娜[line]「————！」[%p]"

msgid "8900:ささやかれて。[linebreak]セナは悟った。[%p]"
msgstr "8900:轻声细语。[linebreak]瑟娜领悟了。[%p]"

msgid "9000:――デッドスポットに紛れ込んだエラー――[%p]"
msgstr "9000:——混入死亡斑纹中的错误——[%p]"

msgid "9100:――精神攻撃――[%p]"
msgstr "9100:——精神攻击——[%p]"

msgid "9200:――幻惑――[%p]"
msgstr "9200:——幻觉——[%p]"

msgid "9300:――妄想を読まれた――[%p]"
msgstr "9300:——读取了对方的幻想——[%p]"

msgid "9400:しまった、と思ったときには遅かった。[%p]"
msgstr "9400:当她心想「糟糕」时，已经太迟了。[%p]"

msgid "9500:バランスを崩し、男もろとも、屋上の縁から、身体が投げ出される――[%p]"
msgstr "9500:失去平衡的身体连同男人一起被抛到屋顶边缘——[%p]"

msgid "9600:８階下にある地面が、セナにはやけに遠くに見えた。[%p]"
msgstr "9600:距离八层楼下方的地表，对瑟娜来说看起来格外遥远。[%p]"

