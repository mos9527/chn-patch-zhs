# THIS FILE IS AUTOMATICALLY GENERATED BY generate_po.py.
                     
msgid "0:[name]男Ａ[line]「視界には『デッドスポット』と呼ばれる、通常は使用される事のない、いくつかの神経チャネルが存在することをご存じですか？」[%p]"
msgstr "0:[name]男子A[line]「你知道在你的视野里，存在着一些被称为『死点』，平常不会使用的神经通道吗？」[%p]"

msgid "100:[name]男Ｂ[line]「それが第２世代となにか関係があるのかね？」[%p]"
msgstr "100:[name]男子B[line]「这跟第二代有什么关系呢？」[%p]"

msgid "200:[name]男Ａ[line]「そういうことになります」[%p]"
msgstr "200:[name]男子A[line]「就是这么回事。」[%p]"

msgid "300:[name]男Ａ[line]「簡単な実験をしてみましょう」[%p]"
msgstr "300:[name]男子A[line]「我们来做个简单的实验吧。」[%p]"

msgid "400:[name]男Ａ[line]「これをご覧ください」[%p]"
msgstr "400:[name]男子A[line]「请看这个。」[%p]"

msgid "500:[name]男Ｃ[line]「子供騙しを聞いているヒマはないぞ」[%p]"
msgstr "500:[name]男子C[line]「我可没空听你胡说八道。」[%p]"

msgid "600:[name]男Ａ[line]「重要なことですよ」[%p]"
msgstr "600:[name]男子A[line]「这很重要哦。」[%p]"

msgid "700:[name]男Ａ[line]「この図には左側にドット、右側にリングがあります」[%p]"
msgstr "700:[name]男子A[line]「这张图左边是点，右边则是环。」[%p]"

msgid "800:[name]男Ａ[line]「では、左目を手で覆って見えないようにして下さい」[%p]"
msgstr "800:[name]男子A[line]「那么，请用手遮住左眼，不要看。」[%p]"

msgid "900:[name]男Ａ[line]「そのまま右目だけで、図の左側にあるドットを見つめて下さい」[%p]"
msgstr "900:[name]男子A[line]「请用右眼注视图左边的小点。」[%p]"

msgid "1000:[name]男Ａ[line]「いかがですか？　ドットを見つめる目の端の方に、リングも存在しますか？」[%p]"
msgstr "1000:[name]男子A[line]「怎么样呢？注视小点时，视野角落有圆圈吗？」[%p]"

msgid "1100:[name]男Ｃ[line]「いくらドットだけを見つめていても、隣にあるリングがついでに見えているのは当然だと思うがな」[%p]"
msgstr "1100:[name]男子C[line]「就算只注视小点，会顺便看到旁边的圆圈也是理所当然吧。」[%p]"

msgid "1200:[name]男Ａ[line]「では先生、ドットだけを見つめたまま、モニタに顔を近づけていってみて下さい。隣のリングはどうなりますか？」[%p]"
msgstr "1200:[name]男子A[line]「那么老师，请你只注视小点，然后把脸靠近屏幕看看。旁边的圆圈会怎么样呢？」[%p]"

msgid "1300:[name]男Ｃ[line]「ほう……これは」[%p]"
msgstr "1300:[name]男子C[line]「哦……这是——」[%p]"

msgid "1400:[name]男Ｂ[line]「興味深いね」[%p]"
msgstr "1400:[name]男子B[line]「真有意思。」[%p]"

msgid "1500:[name]男Ａ[line]「右側のリングが消えた……そうですね？」[%p]"
msgstr "1500:[name]男子A[line]「右侧的圆环消失了……是这样没错吧？」[%p]"

msgid "1600:[name]男Ａ[line]「これは視界が狭まったからではありません」[%p]"
msgstr "1600:[name]男子A[line]「这并不是因为视野变窄了。」[%p]"

msgid "1700:[name]男Ａ[line]「その証拠に、さらにモニタに近づくことで、リングはまた視界に現れます」[%p]"
msgstr "1700:[name]男子A[line]「证据就是，只要再靠近屏幕一点，圆环就会出现在视野里。」[%p]"

msgid "1800:[name]男Ｃ[line]「ふん……これはいったいどんなトリックだ？」[%p]"
msgstr "1800:[name]男子C[line]「哼……这到底是怎样的机关啊？」[%p]"

msgid "1900:[name]男Ａ[line]「いいえ、トリックの類ではありません。普段は使用されていないですが、人間が元から持っている眼の機能のひとつですよ」[%p]"
msgstr "1900:[name]男子A[line]「不，这不是什么机关。虽然平常不会用到，但这是人类眼睛原本就有的机能之一哦。」[%p]"

msgid "2000:[name]男Ａ[line]「これは『デッドスポット』と呼ばれ――」[%p]"
msgstr "2000:[name]男子A[line]「这叫做『Dead End』——」[%p]"

msgid "2100:[name]男Ａ[line]「ノアによって、人間の視界の中に“無”から“有”を作り出す際に使用する、チャネルなのです」[%p]"
msgstr "2100:[name]男子A[line]「这是诺亚在人类视野中，从『无』创造出『有』时使用的通道。」[%p]"

