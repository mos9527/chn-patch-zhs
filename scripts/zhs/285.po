# THIS FILE IS AUTOMATICALLY GENERATED BY generate_po.py.
                     
msgid "0:梨深に引っ張られるようにして、渋々、階下へ。[linebreak]でも昇降口の前で、僕は立ち止まった。[%p]"
msgstr "0:我被梨深拉着，心不甘情不愿地走下了楼梯。[linebreak]不过在鞋柜区前面时，我不禁停了下来。[%p]"

msgid "100:[name]梨深[line]「タク？」[%p]"
msgstr "100:[name]梨深[line]「阿拓？」[%p]"

msgid "200:少し強めに引っ張ってくる梨深の手を、そっと振り払う。[%p]"
msgstr "200:我轻轻挥开梨深稍微用力拉扯的手。[%p]"

msgid "300:ここで廊下を左に曲がれば職員室、さらにその先が保健室だ。[linebreak]昇降口からそのまま外へ出ればグラウンド。そして校門。[%p]"
msgstr "300:从这里往走廊左边转过去就是教职员办公室，再往前走则是保健室。[linebreak]如果直接走出玄关就会来到操场，然后是校门。[%p]"

msgid "400:できれば今日は誰とも会いたくない。[linebreak]校内を歩き回りたくない。[%p]"
msgstr "400:可以的话，今天我不想见到任何人。[linebreak]不想在校内四处走动。[%p]"

msgid "500:屋上のあの惨状はすぐに発見されるだろう。[linebreak]そうなれば大パニックになる。[linebreak]僕も服に血をこびりつかせているから犯人扱いされるかも。[%p]"
msgstr "500:屋顶上的那副惨状应该很快就会被发现吧。[linebreak]那样一来，肯定会引起一场大恐慌。[linebreak]我衣服上也沾了血迹，说不定会被当成凶手。[%p]"

msgid "600:だから今すぐにでも学校から逃げ出したかった。[linebreak]保健室には行かずに、このまま昇降口から出て行くべきだ――[%p]"
msgstr "600:所以我现在就想逃离这间学校。[linebreak]我应该直接从校舍出入口离开，不要去保健室——[%p]"

msgid "700:あの屋上のこと、こずぴぃのことを梨深に伝えようと、僕は背後を振り返った――[%p]"
msgstr "700:正当我想把屋顶上的事和小九P的事告诉梨深，而转过身的时候——[%p]"

msgid "800:[name]拓巳[line]「……え？」[%p]"
msgstr "800:[name]拓巳[line]「……咦？」[%p]"

msgid "900:[name]梨深[line]「あ……」[%p]"
msgstr "900:[name]梨深[line]梨深[%p]"

msgid "1000:梨深が、手になにかを持っていた。[linebreak]鋭い切っ先。鋼鉄の翼のようにも見える。[linebreak]これってまさか、ディソード……？[%p]"
msgstr "1000:梨深的手上拿着某样东西。[linebreak]锐利的刀锋，看起来就像钢铁之翼。[linebreak]这该不会是断罪者……？[%p]"

msgid "1100:梨深はその先端を――[%p]"
msgstr "1100:梨深把前端——[%p]"

msgid "1200:今にも、僕の背中に突き立てようとしていて――[%p]"
msgstr "1200:正准备刺进我的背部——[%p]"

msgid "1300:見つめ合う。[%p]"
msgstr "1300:我们凝视着彼此。[%p]"

msgid "1400:梨深の瞳は悲しさをたたえていた。[linebreak]先に目をそらしたのは梨深だった。[%p]"
msgstr "1400:梨深的眼中充满了悲伤。[linebreak]率先移开视线的是梨深。[%p]"

msgid "1500:[name]梨深[line]「ごめんね」[%p]"
msgstr "1500:[name]梨深[line]「对不起哦。」[%p]"

msgid "1600:ぽつりとそうつぶやくと同時に、剣を振りかぶった――[%p]"
msgstr "1600:在她喃喃自语的同时，挥下了剑——[%p]"

msgid "1700:殺そうとしている。[linebreak]梨深が、僕を、殺そうとしている……！[%p]"
msgstr "1700:打算杀了我。[linebreak]梨深，想要杀掉我……！[%p]"

msgid "1800:無我夢中だった。[linebreak]とっさに、梨深の手首をつかみ、その剣が振り下ろされるのを防ぐ。[%p]"
msgstr "1800:浑然忘我。[linebreak]情急之下，抓住了梨深的手腕，防止那把剑挥下来。[%p]"

msgid "1900:僕はヘタレだし運動もからしきだけど、力は梨深よりはまだマシだった。[%p]"
msgstr "1900:我是个胆小鬼又不擅长运动，但力气还是比梨深大一点。[%p]"

msgid "2000:[name]梨深[line]「く……っ」[%p]"
msgstr "2000:[name]梨深[line]「唔……！」[%p]"

msgid "2100:[name]拓巳[line]「な、なんで……」[%p]"
msgstr "2100:[name]拓巳[line]「为、为什么……」[%p]"

msgid "2200:[name]拓巳[line]「なんでだよぉっ！」[%p]"
msgstr "2200:[name]拓巳[line]「为什么啊——！」[%p]"

msgid "2300:こいつも……梨深も結局は敵だったの？[linebreak]ずっと僕を騙してたの？　優愛のときみたいに、僕を欺いて、僕をハメようとしてたの？[%p]"
msgstr "2300:这家伙也是吗……结果连梨深都是敌人？[linebreak]她一直都在骗我吗？就像优爱一样，欺骗了我，想陷害我吗？[%p]"

msgid "2400:梨深も『将軍』の手下？[linebreak]梨深がニュージェネの真犯人だっていう、最初の僕の推理は、正しかったの？[%p]"
msgstr "2400:梨深也是「将军」的手下吗？[linebreak]我一开始推测梨深是新世代的真正凶手，这个推论是对的吗？[%p]"

msgid "2500:[name]拓巳[line]「な、なんでこんな……！」[%p]"
msgstr "2500:[name]拓巳[line]「为、为什么会这样……」[%p]"

msgid "2600:[name]梨深[line]「ごめん……。でも、タクには消えてもらうしかないの」[%p]"
msgstr "2600:[name]梨深[line]「抱歉……不过，我只能让阿拓消失。」[%p]"

msgid "2700:決定的だった。[linebreak]僕の勘違いじゃなくて、梨深が敵なのは事実。[linebreak]本性を現しただけだ。僕を守るなんてウソだったんだ。[%p]"
msgstr "2700:这是决定性的证据。[linebreak]不是我误会，梨深是敌人这件事是事实。[linebreak]她只是露出真面目而已，说要保护我是骗人的。[%p]"

msgid "2800:[name]拓巳[line]「ウソだ……」[%p]"
msgstr "2800:[name]拓巳[line]「不可能……」[%p]"

msgid "2900:[name]拓巳[line]「こ、こんなのウソだぁっ！」[%p]"
msgstr "2900:[name]拓巳[line]「这、这是骗人的吧！」[%p]"

msgid "3000:[name]梨深[line]「ウソついてて、ごめん」[%p]"
msgstr "3000:[name]梨深[line]「对不起，我一直在说谎。」[%p]"

msgid "3100:[name]梨深[line]「でもお願い、タク……」[%p]"
msgstr "3100:[name]「对不起，我一直在说谎。」[line]「不过拜托你了，阿拓……」[%p]"

msgid "3200:[name]梨深[line]「消えて……」[%p]"
msgstr "3200:[name]「可是求求你，阿拓……」[line]「消失吧……」[%p]"

msgid "3300:[name]拓巳[line]「ふ、ふざ、ふざけるな、ふざけるなよっ！」[%p]"
msgstr "3300:[name]「给我消失……」[line]「开…开什么玩笑，别开玩笑了！」[%p]"

msgid "3400:[name]拓巳[line]「ぼ、僕がっ、どれだけ梨深のことを信用してたと思ってるんだ！」[%p]"
msgstr "3400:[name]「开……开什么玩笑！你少胡说八道了！」[line]「我…你以为，我是多么信任梨深啊！」[%p]"

msgid "3500:[name]拓巳[line]「ど、ど、どれだけ、梨深を頼りにしてたと思ってるんだ！」[%p]"
msgstr "3500:[name]「你……你以为，我是多么信任梨深啊！」[line]「我…以为你是多么依赖梨深啊！」[%p]"

msgid "3600:[name]拓巳[line]「なんでそんな簡単に、ひ、人を、裏切れるんだよっ！」[%p]"
msgstr "3600:[name]「你……你以为我是多么依赖梨深啊！」[line]「为什么，能这么简单地…背叛别人呢？」[%p]"

msgid "3700:[name]拓巳[line]「こ、こ、この、悪魔女め！」[%p]"
msgstr "3700:[name]「为什么你能那么轻易地……背叛他人啊！」[line]「你…你这个恶魔女人！」[%p]"

msgid "3800:[name]梨深[line]「タク……ごめん……」[%p]"
msgstr "3800:[name]「你…你这…个，恶魔女人！」[line]「阿拓……对不起……」[%p]"

msgid "3900:[name]拓巳[line]「ごめんで済むかよ！　な、何様だよ！」[%p]"
msgstr "3900:[name]「拓……抱歉……」[line]「道歉有用的话就不需要警察了啦！你算老几啊？」[%p]"

msgid "4000:気が付けば僕は泣いていた。[linebreak]こんな裏切りは、ないよ……。[%p]"
msgstr "4000:回过神来，我已经哭了出来。[linebreak]怎么会有这种背叛啊……[%p]"

msgid "4100:優愛のときとは比べものにならない怒りと悔しさ。[linebreak]梨深は、僕の気持ちを踏みにじったんだ！[%p]"
msgstr "4100:这股愤怒与懊恼，是优爱那时无法比拟的。[linebreak]梨深践踏了我的感情啊！[%p]"

msgid "4200:許せない……！　許せない……！　許せない……！　許せない……！[linebreak]許せない……！　許せない……！　殺したい……！　許せない……！[linebreak]殺したい……！　許せない……！　許せない……！　殺したい……！[%p]"
msgstr "4200:不可原谅……！不可原谅……！不可原谅……！不可原谅……！[linebreak]不可原谅……！不可原谅……！好想杀了她啊……！不可原谅……！[linebreak]好想杀了她啊……！不可原谅……！不可原谅……！好想杀了她啊……！[%p]"

msgid "4300:手首をきつく握りしめてやると、梨深は苦痛に顔を歪めた。[linebreak]でも僕の方が苦痛なんだ。僕はお前に心をズタズタにされたんだ。[%p]"
msgstr "4300:我用力地握住她的手腕，梨深的脸因痛苦而扭曲起来。[linebreak]但是，感到痛苦的人是我啊！我的心被你撕裂了啊！[%p]"

msgid "4400:許せない、許せるもんか、殺してやりたい、殺してやりたい、殺してやりたい、殺してやり――[%p]"
msgstr "4400:不可原谅……我怎么可能原谅你？好想杀了她，好想杀了她，好想杀了她，好想——[%p]"

msgid "4500:[name]梢[line]「殺してやる……！」[%p]"
msgstr "4500:[name]梢[line]「我要杀了你……！」[%p]"

msgid "4600:[name]梨深[line]「がはっ――」[%p]"
msgstr "4600:[name]梨深[line]「嘎哈——」[%p]"

msgid "4700:いきなり。[%p]"
msgstr "4700:突然间……[%p]"

msgid "4800:梨深の身体が目の前から消えた。[%p]"
msgstr "4800:梨深的身体从我眼前消失了。[%p]"

msgid "4900:すぐ横の下駄箱へと、彼女の身体は吹っ飛び。[linebreak]叩き付けられ。下駄箱を破壊しながら倒し。[linebreak]残骸の中にめり込んで、見えなくなった。[%p]"
msgstr "4900:她的身体飞向旁边的鞋柜。[linebreak]撞了上去，破坏并压垮了鞋柜。[linebreak]陷进残骸之中，看不见了。[%p]"

msgid "5000:もうもうとした粉塵が立ちこめる。[%p]"
msgstr "5000:漫天粉尘遮蔽视线。[%p]"

msgid "5100:なにが起きたのか、最初、僕にはよく分からなかった。[%p]"
msgstr "5100:一开始，我还不明白发生了什么事。[%p]"

msgid "5200:[name]梢[line]「……拓巳しゃんの心の叫びね、聞〜こえたよ」[%p]"
msgstr "5200:[name]梢[line]「……我听到拓先生内心的呐喊了～」[%p]"

msgid "5300:ディソードを構えたこずぴぃが、立っていて。[linebreak]その表情は苦しげ。額には汗もにじんでいる。[linebreak]にもかかわらず、無理に笑って。[%p]"
msgstr "5300:拿着双刃剑的歌姬，站在那里。[linebreak]她的表情很痛苦，额头上也渗出汗水。[linebreak]尽管如此，她还是勉强挤出了笑容。[%p]"

msgid "5400:[name]梢[line]「殺したい、って。許せない、って。だから」[%p]"
msgstr "5400:[name]梢[line]「她说想杀了我，说不能原谅我，所以……」[%p]"

msgid "5500:[name]梢[line]「――殺してあげたのら」[%p]"
msgstr "5500:[name]「她要杀掉我，说不能原谅我，所以……」[line]「——就杀了她的啦。」[%p]"

msgid "5600:そんな心の声を飛ばしてきた。[%p]"
msgstr "5600:她的心之声传了过来。[%p]"

msgid "5700:[name]拓巳[line]「こ……ろした……ホントに……ころ……し……」[%p]"
msgstr "5700:[name]拓巳[line]「杀……了她……真的……杀了……她……」[%p]"

msgid "5800:信じられない思いで、破壊された下駄箱の方へ歩み寄った。[%p]"
msgstr "5800:我怀着难以置信的心情，走向被破坏的鞋柜。[%p]"

msgid "5900:その僕のつま先に、梨深が持っていたディソードが触れる。[linebreak]持ち主の手から離れ、床に転がっていた。[%p]"
msgstr "5900:梨洁儿手中的中剑碰到了我的脚尖。[linebreak]它离开了主人的手，掉落在地板上。[%p]"

msgid "6000:剣の下に、ゆっくりと広がっていく液体。[%p]"
msgstr "6000:剑下方缓缓扩散出一片液体。[%p]"

msgid "6100:それは、血。[%p]"
msgstr "6100:那是——鲜血。[%p]"

msgid "6200:誰の血なのか。[linebreak]そんなの、考えなくたって分かった。[%p]"
msgstr "6200:是谁的血？[linebreak]不用想也知道，那会是谁的血呢？[%p]"

msgid "6300:やがてディソードが、無数の黒い粒子に包まれて消滅していく。[%p]"
msgstr "6300:不久后，迪索德被无数黑色粒子包围消灭了。[%p]"

msgid "6400:梨深が……死んだ……？[linebreak]こんなにも、あっさりと？[%p]"
msgstr "6400:梨深她……死了……？[linebreak]就这么简单地，毫无道理地死去吗？[%p]"

msgid "6500:きちんと確認する気は起きなかった。[%p]"
msgstr "6500:我完全不想去确认真相了。[%p]"

msgid "6600:悲しくて、同時に、やり場のない悔しさがまだ残っていて。[%p]"
msgstr "6600:悲伤，同时还有无处发泄的不甘心残留着。[%p]"

msgid "6700:そんな自分自身の感情を、僕は殺した。[linebreak]感覚を麻痺させたのか、感覚が麻痺したのか。どっちでもいい。[linebreak]涙は止まっていた。[%p]"
msgstr "6700:我扼杀了自己这样的感情。[linebreak]是让感觉麻痹了呢，还是感觉麻痹了？怎样都好啦。[linebreak]眼泪停住了。[%p]"

msgid "6800:[name]拓巳[line]「こずぴぃ……僕を、助けて……」[%p]"
msgstr "6800:[name]拓巳[line]「小九……救我……」[%p]"

msgid "6900:もう、頼れるのはこずぴぃだけになってしまった。[%p]"
msgstr "6900:现在能依靠的人只有小九了。[%p]"

msgid "7000:梨深に裏切られたのに。[linebreak]いまだに僕は、誰かを信じたいって思ってる。[linebreak]いや、誰かに一緒にいてもらいたいだけかもしれない。[%p]"
msgstr "7000:明明被梨深背叛了。[linebreak]我到现在还是想相信别人。[linebreak]不，或许我只是希望有人能陪在我身边而已。[%p]"

msgid "7100:守ってほしいなんていうのは口実で。[linebreak]僕はただ、１人になるのがイヤなだけ。[%p]"
msgstr "7100:说想要有个人来保护我，只是借口罢了。[linebreak]我只是不想一个人而已。[%p]"

msgid "7200:裏切られるかもしれないって思っていても、そして実際に裏切られても、それでも、１人はイヤだった。１人じゃ、このくそったれな状況にとても耐えられないんだ。[%p]"
msgstr "7200:即使觉得可能会遭到背叛，而且实际上也真的被背叛了，我依然不想要一个人。如果只有自己一人的话，实在无法忍受这种烂到极点的状况。[%p]"

msgid "7300:今１人にされたらきっと、僕は僕でなくなるだろう。[linebreak]自分が消えてしまうだろう。[%p]"
msgstr "7300:要是现在变成孤零零一个人的话，我一定会变得不再是我吧。[linebreak]自己会消失不见吧。[%p]"

msgid "7400:僕はすがるようにこずぴぃに歩み寄った。[linebreak]こずぴぃはとても悲しそうで、とても苦しそうに見える。[linebreak]もう笑顔はなかった。[%p]"
msgstr "7400:我像要抓住救命稻草般走向小九彼方。[linebreak]小九彼方面露哀伤，看起来非常痛苦。[linebreak]脸上已经没有笑容了。[%p]"

msgid "7500:その細い肩を、思い切って抱きしめてみた。[linebreak]こずぴぃは抵抗しなくて。[%p]"
msgstr "7500:我试着用力抱住他纤细的肩膀。[linebreak]小九彼方没有反抗。[%p]"

msgid "7600:[name]梢[line]「さっきは、こずぴぃから逃げたのに」[%p]"
msgstr "7600:[name]梢[line]梢[%p]"

msgid "7700:[name]拓巳[line]「ご、ごめん、ごめんなさい……」[%p]"
msgstr "7700:[name]拓巳[line]「对、对不起，真的很抱歉……」[%p]"

msgid "7800:[name]拓巳[line]「そ、それでも、助けてくれ……」[%p]"
msgstr "7800:[name]拓巳[line]「就、就算这样还是请你救我一命吧……」[%p]"

msgid "7900:[name]梢[line]「拓巳しゃんは、こずぴぃの味方？」[%p]"
msgstr "7900:[name]梢[line]「拓巳先生是小俊皮的同伴吗？」[%p]"

msgid "8000:こずぴぃはすでに何人殺したんだろう。[linebreak]警察に捕まったらどうなるんだろう。[%p]"
msgstr "8000:不知道小俊皮已经杀了多少人了？[linebreak]要是被警方逮捕，会有什么下场呢？[%p]"

msgid "8100:そのこずぴぃと一緒に行動してもいいんだろうか。[linebreak]そんなことを自問したけど、すぐに思い直した。[%p]"
msgstr "8100:我跟小俊皮一起行动真的好吗？[linebreak]虽然我如此自问，但立刻改变了想法。[%p]"

msgid "8200:僕は、こずぴぃと一緒にいる。こずぴぃと一緒がいい。[linebreak]警察よりも厄介な敵がいるんだ。そいつを相手に１人でいるのは危険すぎる。[%p]"
msgstr "8200:我要跟小俊皮在一起。我想跟小俊皮待在一起。[linebreak]我有比警察更棘手的敌人要对付，独自面对那家伙实在太危险了。[%p]"

msgid "8300:[name]拓巳[line]「ぼ、僕には、こずぴぃが必要だ」[%p]"
msgstr "8300:[name]拓巳[line]「我……我需要小俊皮啊！」[%p]"

msgid "8400:[name]拓巳[line]「一緒に……いたい……」[%p]"
msgstr "8400:[name]拓巳[line]「我想和你在一起。」[%p]"

msgid "8500:[name]梢[line]「……拓巳しゃんは、身勝手な人なの」[%p]"
msgstr "8500:[name]梢[line]「……拓先生是个自私的人呢。」[%p]"

msgid "8600:そう答えて、こずぴぃの手が僕の背に回された。[linebreak]かすかな力で、こずぴぃの方からも抱きしめてくる。[%p]"
msgstr "8600:小九回答后，便伸手环住我的背部。[linebreak]他用微弱的力量主动抱住我。[%p]"

msgid "8700:[name]梢[line]「でもね、こずぴぃのこと必要って言ってくれたから、こずぴぃも拓巳しゃん、一生懸命守る」[%p]"
msgstr "8700:[name]小九也以细微的力气回抱着我。[line]「不过，既然你说需要小九，那小九也会拼命保护拓巳先生。」[%p]"

msgid "8800:[name]梢[line]「あのね、前に汚いおじしゃんが言ってたのら」[%p]"
msgstr "8800:[name]「可是呢，因为你说你需要小九，所以小九也会努力保护拓巳先生。」[line]「那个啊，之前脏脏的大叔有说过啦。」[%p]"

msgid "8900:[name]梢[line]「我慢することないって。負の感情を否定しちゃダメーって」[%p]"
msgstr "8900:[name]「就是呢，以前有个脏脏的大叔曾经这么说过啦。」[line]「不用忍耐也没关系啊。不可以否认负面情感哦～」[%p]"

msgid "9000:[name]梢[line]「だから、こずぴぃは、こずぴぃをいじめる人みんな、殺すね」[%p]"
msgstr "9000:[name]「不要压抑自己，不可以否认自己的负面情绪哦～」[line]「所以呢，我会把欺负小九的那些人都杀掉。」[%p]"

msgid "9100:[name]梢[line]「それと、拓巳しゃんをいじめる人も！」[%p]"
msgstr "9100:[name]「所以啊，我要把欺负小九的人全都杀了。」[line]「还有，欺负拓巳先生的人也是！」[%p]"

msgid "9200:氷のように冷たくて、同時に、太陽のように温かい感情が、こずぴぃから送られてくる。彼女に宿る、矛盾する感情。狂気と、優しさと。[%p]"
msgstr "9200:小九从拓巳先生身上感受到一股如冰般寒冷，同时又像太阳一样温暖的感情。寄宿在她身上的感情很矛盾，有疯狂也有温柔。[%p]"

msgid "9300:心の声を飛ばすのと同じ仕組みなのか、それとも僕のただの錯覚なのかは分からないけど。[%p]"
msgstr "9300:不知道这是和传送内心话相同的原理，还是单纯我的错觉就是了。[%p]"

msgid "9400:そのとき、校内に女子の悲鳴が響き渡った。[linebreak]校舎の上の方からだった。[%p]"
msgstr "9400:就在这时，女生们的惨叫声传遍校园。[linebreak]声音是从校舍上方传来。[%p]"

msgid "9500:屋上の惨状が見つかったんだろうか。[%p]"
msgstr "9500:是发现屋顶上的惨况了吗？[%p]"

msgid "9600:逃げなきゃ。[%p]"
msgstr "9600:得快点逃跑才行。[%p]"

msgid "9700:こずぴぃ、ここにいたら警察に捕まっちゃうよ……。[%p]"
msgstr "9700:小九，你待在这里会被警察抓走哦……[%p]"

msgid "9800:彼女の身体を離すと、なぜかこずぴぃはすとんとその場にへたり込んだ。[%p]"
msgstr "9800:我放开她的身体后，不知为何小九当场瘫坐在地。[%p]"

msgid "9900:[name]梢[line]「はぁ……ぁ……ぅ……」[%p]"
msgstr "9900:[name]梢[line]「呼啊……啊……呜……」[%p]"

msgid "10000:ど、どうしたの？[%p]"
msgstr "10000:怎，怎么了？[%p]"

msgid "10100:[name]梢[line]「すごく……疲れてるのら……。身体、だるいの……」[%p]"
msgstr "10100:[name]梢[line]「我好累……全身无力……」[%p]"

msgid "10200:[name]梢[line]「ねえ、拓巳しゃん……。こずぴぃね、心の中に、まっくろくろな……妄想がどんどん生まれてくるよ……」[%p]"
msgstr "10200:[name]梢[line]「欸，拓巳先生……小九玖的心里啊，不断产生出漆黑的……妄想哦……」[%p]"

msgid "10300:[name]梢[line]「押し潰されちゃいそうなの……」[%p]"
msgstr "10300:[name]梢[line]「感觉要被压扁了啦……」[%p]"

msgid "10400:まっくろくろな妄想って、なんだ？[%p]"
msgstr "10400:漆黑的妄想是什么啊？[%p]"

msgid "10500:いずれにしてもここにとどまっているのはまずい。[linebreak]梨深の死体が転がってるんだ、もし誰かに見られたら、確実に僕らの仕業だってバレてしまう。[%p]"
msgstr "10500:不管怎样，继续待在这里很不妙。[linebreak]梨深的尸体就倒在那里，要是被谁看见了的话，我们干的好事肯定会被发现。[%p]"

msgid "10600:とにかくこずぴぃに肩を貸して、その場を離れた。[%p]"
msgstr "10600:总之我让小九陪在梨深身边，并且离开了现场。[%p]"

