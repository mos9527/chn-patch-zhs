# THIS FILE IS AUTOMATICALLY GENERATED BY generate_po.py.
                     
msgid "0:井乃頭線の神泉駅は、渋谷から１駅。電車でわずか１分ほどで到着する駅だ。[%p]"
msgstr "0:井乃头线的神泉站距离涩谷只有一站，搭电车只要短短一分钟就能抵达。[%p]"

msgid "100:駅自体がトンネルによってすっぽりと覆われ、ホームの端にある開口部からわずかに外光が入ってくる程度。[%p]"
msgstr "100:车站本身完全被隧道所覆盖，只有月台边缘的开口处能稍微透进一点阳光而已。[%p]"

msgid "200:今は地震の影響で照明がすべて消えており、普段と比べてひどく薄暗い。[%p]"
msgstr "200:现在因为地震的影响，所有灯光都熄灭了，比平常还要昏暗许多。[%p]"

msgid "300:そのホームに、折原梢の身体を支えながら、蒼井セナは足を踏み入れた。[%p]"
msgstr "300:苍井青绪扶着折原的身影，踏入了月台中。[%p]"

msgid "400:切符を買う必要はなかった。[%p]"
msgstr "400:不需要买票。[%p]"

msgid "500:駅員がどこにも見当たらなかったし、券売機も使い物にならない。[%p]"
msgstr "500:车站里看不到任何站务人员，售票机也无法使用。[%p]"

msgid "600:線路を見ると、レールは一目で分かるほどに歪んでしまっていた。当然、電車は昨日から運行停止になっている。[%p]"
msgstr "600:往铁轨一看，轨道扭曲得相当明显。电车理所当然地从昨天就停驶了。[%p]"

msgid "700:[name]梢[line]「ふにゅぅ……ずっきんずっきんするの……」[%p]"
msgstr "700:[name]梢[line]「呼喵……我剪我剪……」[%p]"

msgid "800:[name]セナ[line]「…………」[%p]"
msgstr "800:[name]瑟娜[line]「…………」[%p]"

msgid "900:梢の左足は、血だらけだった。[%p]"
msgstr "900:梢的左脚满是鲜血。[%p]"

msgid "1000:地震のとき、割れたガラスでざっくりと切ってしまったのだ。[linebreak]セナがタオルを巻いて血を止めようとしたが、傷は予想以上に深く、応急処置ではどうにもならなかった。[%p]"
msgstr "1000:在发生地震时，被碎裂的玻璃划伤了。[linebreak]瑟娜用毛巾包住伤口试图止血，但伤势比想象中还要严重，光靠应急处理根本无济于事。[%p]"

msgid "1100:ギガロマニアックスはその力で怪我を治すことはできない。[%p]"
msgstr "1100:基加罗马尼亚克斯无法用它的力量治疗伤口。[%p]"

msgid "1200:痛みが、否応なく怪我は現実のものだと脳に思い知らせるから。[%p]"
msgstr "1200:因为疼痛会强制让大脑认知到受伤是现实。[%p]"

msgid "1300:“怪我をしていない”という周囲共通認識を作れない。[linebreak]なかったことにはできない。[linebreak]人間の本能が、それをさせてくれないのだ。[%p]"
msgstr "1300:无法制造出「没有受伤」这种周围人之间的共识。[linebreak]不能当作没发生过。[linebreak]人类的本能不会允许自己这么做。[%p]"

msgid "1400:[name]梢[line]「もう歩けないのら〜」[%p]"
msgstr "1400:[name]梢[line]「偶已经走不动了啦～」[%p]"

msgid "1500:梢が泣きそうな顔で弱音を吐いても、セナは無言だった。[linebreak]ただひたすら、一心不乱という様子で、セナはこの神泉駅を目指して歩いてきたのだ。[%p]"
msgstr "1500:即使看到梢哭丧着脸说出泄气话，瑟娜依然不发一语。[linebreak]她只是一心一意地朝着这个神泉车站走来而已。[%p]"

msgid "1600:無人と化したホームにたどり着いたところで、セナは不意に梢を支えていた手を離した。[%p]"
msgstr "1600:抵达空无一人的月台后，瑟娜突然放开支撑梢的手。[%p]"

msgid "1700:片足に力が入らない梢は、たまらずその場にへたり込んでしまう。[%p]"
msgstr "1700:单脚使不上力气的梢当场瘫坐在地。[%p]"

msgid "1800:[name]梢[line]「……？」[%p]"
msgstr "1800:[name]梢[line]「……？」[%p]"

msgid "1900:梢の困惑気味に見上げてくる視線は一顧だにせず。[linebreak]セナは、ある一点をじっとにらみつけたまま、ギリリと歯ぎしりした。[%p]"
msgstr "1900:面对梢带着困惑抬起来的目光，工兵完全不予理会。[linebreak]瑟娜一直瞪着某一个点，并咬牙切齿地说道：[%p]"

msgid "2000:[name]セナ[line]「…………」[%p]"
msgstr "2000:[name]瑟娜[line]「…………」[%p]"

msgid "2100:[name]セナ[line]「やっと、見つけた……！」[%p]"
msgstr "2100:[name]「…………」[line]「终于找到了……！」[%p]"

msgid "2200:その視線の先。[%p]"
msgstr "2200:在她的视线前方——[%p]"

msgid "2300:薄暗いホームの奥に、座り込んでいるひとつの人影があった。[%p]"
msgstr "2300:有个坐在月台深处的人影。[%p]"

msgid "2400:[name]梢[line]「あ……セナしゃんのパッパーなのら……」[%p]"
msgstr "2400:[name]梢[line]「啊……是瑟娜酱的帕～帕～呢……」[%p]"

msgid "2500:汚らしい服をまとったホームレス。[linebreak]その手には“人の心は死滅する”と書かれたボード。[%p]"
msgstr "2500:穿着肮脏衣服的流浪汉。[linebreak]手上拿着一块写着「人心已灭」的板子。[%p]"

msgid "2600:セナは男にゆっくりと歩み寄っていった。[%p]"
msgstr "2600:瑟娜缓缓走向那个男人。[%p]"

msgid "2700:[name]セナ[line]「誰に向けたメッセージだ？」[%p]"
msgstr "2700:[name]瑟娜[line]「这是要给谁的信息啊？」[%p]"

msgid "2800:その声は、凍り付きそうなほどに冷たく。[%p]"
msgstr "2800:她的声音冰冷得几乎令人冻结。[%p]"

msgid "2900:[name]男[line]「負を正へと変換できる者たちへの、遺言だよ――」[%p]"
msgstr "2900:[name]男人[line]「这是给那些能够把负数转换为正数的人们的遗言——」[%p]"

msgid "3000:[name]男[line]「セナ」[%p]"
msgstr "3000:[name]男人[line]「瑟娜。」[%p]"

msgid "3100:ホームレスの男――波多野はうなだれたまま、娘の顔を見ずに淡々とその名を口にした。[%p]"
msgstr "3100:流浪汉——波多野低着头，没有看向女儿的脸，只是淡淡地说出这个名字。[%p]"

msgid "3200:[name]セナ[line]「お前に馴れ馴れしく名前を呼ばれたくない」[%p]"
msgstr "3200:[name]瑟娜[line]「我不想被你用那种亲昵的态度叫名字。」[%p]"

msgid "3300:[name]セナ[line]「……この半年、ずっと考えてきた」[%p]"
msgstr "3300:[name]瑟娜[line]「……这半年来，我一直在思考。」[%p]"

msgid "3400:[name]セナ[line]「お前を、どうやって殺すか」[%p]"
msgstr "3400:[name]「……这半年来，我一直都在思考。」[line]「要怎么杀你才好呢？」[%p]"

msgid "3500:[name]波多野[line]「ありがとう」[%p]"
msgstr "3500:[name]波多野[line]「谢谢。」[%p]"

msgid "3600:[name]セナ[line]「どうして礼を言う」[%p]"
msgstr "3600:[name]瑟娜[line]「为什么要道谢？」[%p]"

msgid "3700:[name]波多野[line]「利己的な理由だよ」[%p]"
msgstr "3700:[name]瑟娜[line]「很自私的理由。」[%p]"

msgid "3800:[name]波多野[line]「お前に殺されることが、私にとって最善の贖罪かもしれない。そう思ったからだ」[%p]"
msgstr "3800:[name]「很自私的理由。」[line]「因为我认为，被你杀死对我来说或许是最适当的赎罪方式。」[%p]"

msgid "3900:[name]セナ[line]「……それなら話は早い」[%p]"
msgstr "3900:[name]瑟娜[line]「……既然如此，事情就好说了。」[%p]"

msgid "4000:セナはディソードをリアルブートする。[%p]"
msgstr "4000:瑟娜启动了迪索德。[%p]"

msgid "4100:彼女が選択した“父を殺す方法”は、ディソードによる斬殺。[%p]"
msgstr "4100:她选择的是用迪索德斩杀父亲的方法。[%p]"

msgid "4200:シンプルであることは、真理に近いということ。[%p]"
msgstr "4200:简单就是接近真理的意思。[%p]"

msgid "4300:殺し方に意味はない。[linebreak]殺すことに、意味がある。[%p]"
msgstr "4300:杀人方式没有意义。[linebreak]杀人的行为才有意义。[%p]"

msgid "4400:[name]波多野[line]「済まない」[%p]"
msgstr "4400:[name]波多野[line]「抱歉了。」[%p]"

msgid "4500:波多野はいまだ、顔を上げず。[%p]"
msgstr "4500:波多野依然低着头。[%p]"

msgid "4600:[name]セナ[line]「どうして謝る」[%p]"
msgstr "4600:[name]瑟娜[line]「为什么要道歉？」[%p]"

msgid "4700:[name]波多野[line]「お前に、父殺しの汚名を着せてしまうからだ」[%p]"
msgstr "4700:[name]波多野[line]「因为我会害你背上弑父的污名。」[%p]"

msgid "4800:[name]セナ[line]「殺せさえすれば、どんな辱めだって受けてやる」[%p]"
msgstr "4800:[name]瑟娜[line]「只要能杀了你，不管受到什么样的侮辱我都愿意。」[%p]"

msgid "4900:[name]セナ[line]「お前は、母さんと妹を殺した」[%p]"
msgstr "4900:[name]瑟娜[line]「是你杀了我母亲和妹妹。」[%p]"

msgid "5000:[name]波多野[line]「事実だ」[%p]"
msgstr "5000:[name]波多野[line]「这是事实。」[%p]"

msgid "5100:[name]セナ[line]「しかも、あんな、むごい最期を……」[%p]"
msgstr "5100:[name]瑟娜[line]「而且，还死得那么惨……」[%p]"

msgid "5200:それまで、感情を押し殺し冷徹さを保っていたセナの声が、かすかに揺らいだ。[%p]"
msgstr "5200:瑟娜压抑情感保持冷静的声音中，微微地动摇了。[%p]"

msgid "5300:[name]波多野[line]「お前には、見せたくはなかった」[%p]"
msgstr "5300:[name]波多野[line]「我本来不想让你看到这一幕……」[%p]"

msgid "5400:[name]波多野[line]「いや、今のは聞き流してくれ」[%p]"
msgstr "5400:[name]「原本我不想让你看到这副模样。」[line]「不，刚才那句话你就当没听到吧。」[%p]"

msgid "5500:[name]セナ[line]「なぜあんなことを！？」[%p]"
msgstr "5500:[name]瑟娜[line]「你为什么要那么做！」[%p]"

msgid "5600:[name]波多野[line]「過程を説明することは無意味だよ」[%p]"
msgstr "5600:[name]波多野[line]「说明过程没有意义。」[%p]"

msgid "5700:[name]波多野[line]「お前は納得しないし、結果が改変されるわけでもない」[%p]"
msgstr "5700:[name]波多野[line]「你不会接受，结果也不会改变。」[%p]"

msgid "5800:セナはディソードの切っ先を波多野の頭へと向ける。[%p]"
msgstr "5800:瑟娜用迪索德的剑尖指向波多野的头。[%p]"

msgid "5900:[name]セナ[line]「そうやってまた逃げるのか」[%p]"
msgstr "5900:[name]瑟娜[line]「你又要逃避了吗？」[%p]"

msgid "6000:[name]波多野[line]「間違っていたのは私だ。それがすべてなんだ」[%p]"
msgstr "6000:[name]波多野[line]「错的是我，这就是一切。」[%p]"

msgid "6100:[name]セナ[line]「お前には……失望した……」[%p]"
msgstr "6100:[name]瑟娜[line]「对你……我很失望……」[%p]"

msgid "6200:[name]波多野[line]「まだ、期待していたのか？　なにを？」[%p]"
msgstr "6200:[name]「我对你……很失望……」[line]「你还在期待吗？期待什么？」[%p]"

msgid "6300:[name]セナ[line]「…………」[%p]"
msgstr "6300:[name]瑟娜[line]「…………」[%p]"

msgid "6400:[name]波多野[line]「人を含めて、世界はすべて電気仕掛け」[%p]"
msgstr "6400:[name]波多野[line]「包含人类在内，整个世界都是由电子所构成。」[%p]"

msgid "6500:[name]波多野[line]「そう言ったのは、お前だ、セナ」[%p]"
msgstr "6500:[name]波多野[line]「说这种话的人是你，瑟娜。」[%p]"

msgid "6600:[name]セナ[line]「……！」[%p]"
msgstr "6600:[name]瑟娜[line]「……！」[%p]"

msgid "6700:セナの肩が震える。[%p]"
msgstr "6700:瑟娜肩膀颤抖着。[%p]"

msgid "6800:ここ３年近く――波多野がマナ計画を始めて以来、２人はほとんど会っていない。にもかかわらず、いともたやすく見透かされたことにセナは動揺した。[%p]"
msgstr "6800:这三年来——自从波多野开始推动魔力计划后，两人几乎不曾见面。尽管如此，自己却轻易地被看穿了心思，这让瑟娜感到动摇。[%p]"

msgid "6900:違和感を覚えつつも、心の揺れは激情へと変換され、彼女の理性を吹き飛ばそうとする。[%p]"
msgstr "6900:尽管觉得不对劲，内心的动摇却转换为激动的情绪，试图摧毁她的理智。[%p]"

msgid "7000:[name]波多野[line]「セナ、惑わされるな」[%p]"
msgstr "7000:[name]波多野[line]「瑟娜，别被他迷惑了！」[%p]"

msgid "7100:[name]波多野[line]「電気仕掛けならば、お前のその私を恨む感情もまた、ただの電気信号でしかない」[%p]"
msgstr "7100:[name]波多野[line]「如果只是电力驱动，你对我的憎恨也不过是单纯的电子讯号罢了。」[%p]"

msgid "7200:波多野がたたみかけてくる。[linebreak]セナは自らに言い聞かせた。[%p]"
msgstr "7200:波多野继续说道：[linebreak]瑟娜如此说服自己。[%p]"

msgid "7300:――落ち着け。[%p]"
msgstr "7300:——冷静点。[%p]"

msgid "7400:[name]セナ[line]「お前の言葉なんか、無効だ」[%p]"
msgstr "7400:[name]瑟娜[line]「你说的话，无效。」[%p]"

msgid "7500:[name]波多野[line]「あまり感情的な行動を取るな。でなければ、お前は矛盾する」[%p]"
msgstr "7500:[name]波多野[line]「不要做出太情绪化的行动，否则你会自相矛盾。」[%p]"

msgid "7600:[name]波多野[line]「その言葉を受け取ってはいけない。精神攻撃だ」[%p]"
msgstr "7600:[name]「别做那么情绪化的事，不然会很矛盾。」[line]「不要接受他的说法，那是精神攻击。」[%p]"

msgid "7700:[name]波多野[line]「判断を中止するんだ」[%p]"
msgstr "7700:[name]「不可以听信他的话，那是在进行精神攻击。」[line]「不要做出判断。」[%p]"

msgid "7800:[name]セナ[line]「話は、終わりだ……！」[%p]"
msgstr "7800:[name]瑟娜[line]「话就说到这里了……！」[%p]"

msgid "7900:セナはディソードを構えようとする。[%p]"
msgstr "7900:瑟娜准备举起断罪剑。[%p]"

msgid "8000:それよりわずかに先んじて、波多野がぬっと立ち上がっていた。[%p]"
msgstr "8000:在她行动之前，波多野已经迅速起身了。[%p]"

msgid "8100:ハラハラと２人のやり取りを見ていた梢は、いざこざを止めるべきかと迷ったが、足が痛かったからあきらめた。[%p]"
msgstr "8100:忐忑不安地关注两人互动，梢犹豫着是否该上前阻止这场纠纷，但因为脚很痛所以放弃了。[%p]"

msgid "8200:悲しくはなかったが、胸の奥がシクシクと泣いていた。[%p]"
msgstr "8200:虽然不觉得悲伤，但是内心深处却隐隐作痛。[%p]"

msgid "8300:波多野は、大股でセナへと足を踏み出す。[%p]"
msgstr "8300:波多野朝赛娜跨出一大步。[%p]"

msgid "8400:[name]セナ[line]「……っ」[%p]"
msgstr "8400:[name]赛娜[line]「……！」[%p]"

msgid "8500:反応が遅れたセナは、迎え撃つ動作へと切り替える。[linebreak]けれど踏ん張ろうとして、砂利に足を取られた。[%p]"
msgstr "8500:反应慢半拍的赛娜，切换为迎击动作。[linebreak]然而她想站稳脚步时，脚却被沙砾绊了一下。[%p]"

msgid "8600:姿勢を崩す。動揺が身体の動きに出た。[linebreak]セナから鋭さが消えていた。[%p]"
msgstr "8600:她的姿势失去平衡，动摇的情绪也表现在动作上。[linebreak]瑟娜不再那么锐利了。[%p]"

msgid "8700:彼女は怯えていた。[linebreak]父を恨むことで自分を保ってきた。[%p]"
msgstr "8700:她感到害怕。[linebreak]借由憎恨父亲来维持自我。[%p]"

msgid "8800:だがいざ対峙してみて、向き合うことが怖くなった。[linebreak]それがなぜかは自分でもよく分からず。[linebreak]少なくとも直前の父の言葉が原因なのは確かで。[%p]"
msgstr "8800:然而一旦对峙，她便害怕面对现实。[linebreak]连自己也不明白原因何在。[linebreak]至少可以确定，原因是父亲先前那番话。[%p]"

msgid "8900:波多野が、ものすごい勢いで飛びかかってくる。[%p]"
msgstr "8900:波多野以惊人的气势飞扑过来。[%p]"

msgid "9000:[name]セナ[line]「いや……っ！」[%p]"
msgstr "9000:[name]瑟娜[line]「不要啊……！」[%p]"

msgid "9100:後ろへ倒れ込みながら。[%p]"
msgstr "9100:一边往后仰身，一边——[%p]"

msgid "9200:その巨大なディソードを下から振り上げる。[%p]"
msgstr "9200:从下方挥起那把巨大的迪索德。[%p]"

msgid "9300:重力も、遠心力も無視する剣。[%p]"
msgstr "9300:无视重力与离心力的一剑。[%p]"

msgid "9400:その刃が、まずホームのタイルを抉り、次に父の肩を切り裂いた。[linebreak]熱したナイフでバターを切るように、流麗に。[%p]"
msgstr "9400:那把剑，首先剜起月台上的磁砖，接着切开父亲的肩膀。[linebreak]就像用加热过的刀子切割奶油一样，流畅无比。[%p]"

msgid "9500:セナは、父の手によって突き飛ばされる。[%p]"
msgstr "9500:瑟娜被父亲的手推了出去。[%p]"

msgid "9600:父の傷から鮮血が舞い。[%p]"
msgstr "9600:鲜血从父亲的伤口飞溅而出。[%p]"

msgid "9700:安堵したような父の目。[%p]"
msgstr "9700:父亲的眼神像是松了一口气似的。[%p]"

msgid "9800:セナは唇を噛み。[%p]"
msgstr "9800:瑟娜咬紧嘴唇。[%p]"

msgid "9900:直後に父の胸を、高速でなにかが貫いた。[%p]"
msgstr "9900:紧接着，有某种东西以高速贯穿了父亲的胸口。[%p]"

msgid "10000:父の背中側から飛来したそれは、セナの頬をかすめて消えていく。[%p]"
msgstr "10000:从父亲背后飞来的那个物体掠过瑟娜的脸颊，消失在远方。[%p]"

msgid "10100:父は、肩だけでなく、胸からも血が噴き出し。[linebreak]傷口から溢れた真紅の液体が、セナの頬にかかる。[%p]"
msgstr "10100:不只肩膀，连胸口都喷出了鲜血。[linebreak]从伤口涌出的鲜红血液沾到了瑟娜的脸颊上。[%p]"

msgid "10200:――銃弾！？[%p]"
msgstr "10200:——子弹！？[%p]"

msgid "10300:突き飛ばされ、仰向けに倒れたセナは、とっさに顔を上げ、銃を撃った何者かの姿を探した。[%p]"
msgstr "10300:被撞飞出去，仰面朝天倒在地上的瑟娜立刻抬起头来寻找开枪的人影。[%p]"

msgid "10400:[name]男[line]「おっとぉ！　ミスったッス！」[%p]"
msgstr "10400:[name]男人[line]「哎呀～！我失误了哩！」[%p]"

msgid "10500:その男は、渋谷駅へと続くトンネルと、神泉駅のトンネルの境、そこだけぽっかりと空が見える踏切に立っていた。[linebreak]手には、拳銃。[%p]"
msgstr "10500:那名男子站在通往涩谷车站与神泉车站的两条隧道交界处，唯一能看见天空的铁路道口上。[linebreak]手上拿着一把枪。[%p]"

msgid "10600:[name]男[line]「でも、ま、この距離にしてはいい腕だと思わないッスか？」[%p]"
msgstr "10600:[name]是男人。[line]「不过，哎呀～以这个距离来说，你不觉得他的技术很好吗？」[%p]"

msgid "10700:セナの横に、父が倒れ込む。[linebreak]すでにその半身は血で染まり。[%p]"
msgstr "10700:父亲倒在瑟娜身旁。[linebreak]他的身体已经有一半被鲜血染红了。[%p]"

msgid "10800:――まさか、かばった？　私を？[%p]"
msgstr "10800:——难道说，他保护我了吗？[%p]"

msgid "10900:信じられない思いで、セナはタイルの上に広がっていく血だまりを見つめた。[%p]"
msgstr "10900:瑟娜难以置信地凝视着在磁砖地板上扩散开来的鲜血。[%p]"

msgid "11000:[name]波多野[line]「言葉は、混沌だ……」[%p]"
msgstr "11000:[name]波多野[line]「语言是，混沌的……」[%p]"

msgid "11100:息も絶え絶えに、父がつぶやく。[%p]"
msgstr "11100:父亲奄奄一息地低喃道。[%p]"

msgid "11200:[name]波多野[line]「お前は、素直すぎるんだ、セナ……」[%p]"
msgstr "11200:[name]波多野[line]「你太老实了，瑟娜……」[%p]"

msgid "11300:[name]波多野[line]「そんなお前が……とても悲しく……そして、愛おしいよ……」[%p]"
msgstr "11300:[name]波多野[line]「这样的你……让我感到非常悲伤……同时也觉得十分惹人怜爱啊……」[%p]"

msgid "11400:銃弾はセナを狙っていた。[linebreak]それを、波多野はかばった。[%p]"
msgstr "11400:子弹瞄准了瑟娜。[linebreak]而波多野挺身保护她。[%p]"

msgid "11500:父を殺そうとしていたはずが、その父に、命を救われた。[%p]"
msgstr "11500:原本打算杀害父亲，却反过来被他所救。[%p]"

msgid "11600:[name]セナ[line]「ふざ、けるな……」[%p]"
msgstr "11600:[name]瑟娜[line]「别开玩笑了……」[%p]"

msgid "11700:セナはディソードを握る手に力を込める。[%p]"
msgstr "11700:瑟娜用力握住迪索尔巴德的手。[%p]"

msgid "11800:[name]男[line]「まあ、別に構わないッスよね。波多野親子にはどっちも死んでもらうつもりだったんで」[%p]"
msgstr "11800:[name]男人[line]「算了，无所谓啦。反正我本来就打算让波多野父子都去死。」[%p]"

msgid "11900:ホームに、奇妙な音が響いていた。[linebreak]セナは、その音がノアⅡの端末によるものだと瞬時に悟る。[%p]"
msgstr "11900:月台上响起奇怪的声音。[linebreak]瑟娜瞬间就明白那是诺亚Ⅱ的电脑发出的声音。[%p]"

msgid "12000:――この音はいつから？[%p]"
msgstr "12000:——这声音是从什么时候开始传来的？[%p]"

msgid "12100:許せなかった。[%p]"
msgstr "12100:无法原谅。[%p]"

msgid "12200:父と自分との決着を邪魔した、銃を持ったあの男の存在が、セナにはどうしても許せなかった。[%p]"
msgstr "12200:妨碍父亲与自己一决胜负，那个持枪的男人的存在，瑟娜无论如何都无法原谅。[%p]"

msgid "12300:[name]セナ[line]「よくも、邪魔を――」[%p]"
msgstr "12300:[name]瑟娜[line]「竟敢妨碍我——」[%p]"

msgid "12400:立ち上がり、正面へと目を向けたとき。[%p]"
msgstr "12400:当我起身，看向正前方时……[%p]"

msgid "12500:[name]セナ[line]「……！」[%p]"
msgstr "12500:[name]瑟娜[line]「……！」[%p]"

msgid "12600:赤ん坊の泣き声が、薄暗いホームに響いた。[linebreak]セナは、自らの手に違和感を覚える。[linebreak]なぜか、剣の代わりに――[%p]"
msgstr "12600:婴儿的哭声响彻了幽暗月台。[linebreak]瑟娜感觉到自己的手不太对劲。[linebreak]不知为何，她手上拿的是——[%p]"

msgid "12700:腐り、干からびた、赤ん坊を抱いていた。[%p]"
msgstr "12700:抱着一个腐烂且干瘪的小婴儿。[%p]"

msgid "12800:[name]セナ[line]「ひっ……」[%p]"
msgstr "12800:[name]瑟娜——[line]「咿……！」[%p]"

msgid "12900:反射的に、放り捨ててしまう。[%p]"
msgstr "12900:我下意识地把东西扔了出去。[%p]"

msgid "13000:けれどすぐに、その赤ん坊が、[linebreak]"
msgstr "13000:然而，那个婴儿立刻——[linebreak]"

msgid "13100:かつて一度だけ見た、自分の妹であることに気付く。[%p]"
msgstr "13100:我察觉到那是过去曾经见过一次面的妹妹。[%p]"

msgid "13200:けれどすぐに、その赤ん坊が、かつて一度だけ見た、自分の妹であることに気付く。"
msgstr "13200:然而，那个婴儿立刻——"

msgid "13300:[name]セナ[line]「う……」[%p]"
msgstr "13300:[name]瑟娜[line]「呜……」[%p]"

msgid "13400:セナは――"
msgstr "13400:瑟娜——"

msgid "13500:[linebreak]また動揺する。[linebreak]目が離せなくなる。[%p]"
msgstr "13500:[linebreak]她又动摇了。[linebreak]我无法移开目光。[%p]"

msgid "13600:セナは――また動揺する。目が離せなくなる。"
msgstr "13600:瑟娜——又动摇了，她无法移开目光。"

msgid "13700:妹を投げ捨て、気持ち悪いと思ってしまった自分が許せなくなる。[%p]"
msgstr "13700:我无法原谅抛弃妹妹，还觉得她恶心的自己。[%p]"

msgid "13800:あのときもそうだった。[linebreak]母の最期を見せられたとき。[linebreak]母の、狂気に支配され死んでいく姿。[%p]"
msgstr "13800:那个时候也是这样。[linebreak]被迫目睹母亲临终的模样时——[linebreak]被疯狂所支配，逐渐死去的母亲身影。[%p]"

msgid "13900:それを見せられて。[linebreak]悲しさや、恐ろしさ以上に。[linebreak]気持ち悪さを、セナは感じていた。[%p]"
msgstr "13900:被迫目睹那副模样——[linebreak]比起悲伤或恐惧，更令她感到的是——[linebreak]一股恶心感油然而生。[%p]"

msgid "14000:そのことが、彼女自身の胸の中で罪悪感として残り。[linebreak]父を恨むことでその感情から目をそらしていた。[%p]"
msgstr "14000:这件事在她心中留下了罪恶感。[linebreak]所以她才会借由憎恨父亲来逃避自己的情感。[%p]"

msgid "14100:[name]セナ[line]「私は……」[%p]"
msgstr "14100:[name]瑟娜[line]「我……」[%p]"

msgid "14200:[name]セナ[line]「私、は……っ」[%p]"
msgstr "14200:[name]「我……」[line]「我……」[%p]"

msgid "14300:これが、妄想による攻撃だと頭の片隅で理解はしていても。[%p]"
msgstr "14300:尽管脑中某个角落明白这是源自于妄想的攻击——[%p]"

msgid "14400:心の奥に封印していた感情を呼び覚まされ、金縛りに遭ったように身動きが取れなくなっていた――[%p]"
msgstr "14400:被唤醒了封印在内心深处的感情，就像鬼压床一样动弹不得——[%p]"

msgid "14500:それは、セナだけでなく梢も同様だった。[%p]"
msgstr "14500:不只是赛莉亚，就连梢也是一样。[%p]"

msgid "14600:へたり込んでいた梢の目の前には、大きな鏡が現れ、そこに映る、足を血で汚した自分が、問いかけてきていた。[%p]"
msgstr "14600:瘫坐在地上的梢眼前出现一面大镜子，镜子里映照出双脚沾满鲜血的自己，开口问道：[%p]"

msgid "14700:[name]梢[line]「お前は誰？」[%p]"
msgstr "14700:[name]梢[line]「你是谁？」[%p]"

msgid "14800:梢は恐ろしさで凍り付く。[linebreak]目から大粒の涙を溢れさせた。[%p]"
msgstr "14800:恐惧让梢全身冻结。[linebreak]她的眼中涌出豆大的泪珠。[%p]"

msgid "14900:忘れたかった傷。思い出したくなかった過去。[%p]"
msgstr "14900:想遗忘的伤痕，不愿想起的记忆。[%p]"

msgid "15000:心からも、腐敗した涙が漏れ出てくる。[linebreak]逃げ出したくなる。[linebreak]鏡を見たくなくて、顔をそむけたくなる。[%p]"
msgstr "15000:发自内心的腐败泪水流了出来。[linebreak]她想拔腿就跑。[linebreak]不想照镜子，想要把脸别开。[%p]"

msgid "15100:それなのに、意思に反して身体は動かず。[%p]"
msgstr "15100:然而，身体却违背她的意志动弹不得。[%p]"

msgid "15200:そして鏡の向こうの自分は、泣いていなかった。[%p]"
msgstr "15200:镜子里头的自己没有哭泣。[%p]"

msgid "15300:[name]梢[line]「お前は誰？」[%p]"
msgstr "15300:[name]梢[line]「你是谁？」[%p]"

msgid "15400:[name]梢[line]「お前は誰？　お前は誰？　お前は誰？　お前は誰？　お前は誰？[linebreak]お前は誰？　お前は誰？　お前は誰？　お前は誰？　お前は誰？[linebreak]お前は誰？　お前は誰？」[%p]"
msgstr "15400:[name]「你是什么人？」[line]「你是谁？你是谁？你是谁？你是谁？你是谁？[linebreak]你是谁？你是谁？你是谁？你是谁？你是谁？[linebreak]你是谁？你是谁？你是谁？你是谁？你是谁？[%p]"

msgid "15500:[name]梢[line]「私は誰？」[%p]"
msgstr "15500:[name]你是谁？你是谁？你是谁？你是谁？你是谁？[line]你是谁？你是谁？你是谁？你是谁？你是谁？[%p]"

msgid "15600:[name]梢[line]「私は誰？　私は誰？　私は誰？　私は誰？　私は誰？　私は誰？[linebreak]私は誰？　私は誰？　私は誰？　私は誰？　私は誰？　私は誰？[linebreak]私は誰？　私は誰？　私は誰？」[%p]"
msgstr "15600:[name]你是谁？你是谁？你是谁？你是谁？你是谁？[line]你是谁？你是谁？你是谁？你是谁？你是谁？[linebreak]你是谁？你是谁？你是谁？你是谁？你是谁？[linebreak]你是谁？你是谁？你是谁？你是谁？你是谁？[%p]"

msgid "15700:[color index=\"A0FF0000\"]ゲシュタルト崩壊[color index=\"800000\"]に陥る。[%p]"
msgstr "15700:[color index=\"A0FF0000\"]你是谁？你是谁？你是谁？你是谁？你是谁？[color index=\"800000\"]陷入混乱。[%p]"

msgid "15800:かつて言葉を失ったときと同じように。[linebreak]かつて心が壊れたときと同じように。[%p]"
msgstr "15800:就像过去失去语言能力时一样。[linebreak]就像过去心灵崩坏的时候一样。[%p]"

msgid "15900:鏡の奥で、自分に問いかけてくる自分が、自分に見えなくなる。[%p]"
msgstr "15900:看不见在镜子深处，那个正在询问自己的自己了。[%p]"

msgid "16000:自分の声に、確信が持てない。[%p]"
msgstr "16000:无法对自己的声音抱有确信。[%p]"

msgid "16100:自分の髪型に、確信が持てない。[%p]"
msgstr "16100:无法对自己的发型抱有确信。[%p]"

msgid "16200:自分の鼻の形に、確信が持てない。[%p]"
msgstr "16200:无法对自己的鼻子形状抱有确信。[%p]"

msgid "16300:自分の瞳の色に、確信が持てない。[%p]"
msgstr "16300:无法对自己的眼睛颜色抱有确信。[%p]"

msgid "16400:自分の唇の厚さに、確信が持てない。[%p]"
msgstr "16400:无法对自己的嘴唇厚度抱有确信。[%p]"

msgid "16500:自分の形に、確信が持てない。[%p]"
msgstr "16500:无法对自己的形状抱有确信。[%p]"

msgid "16600:自分の顔に、確信が持てない。[%p]"
msgstr "16600:无法对自己的脸抱有确信。[%p]"

msgid "16700:[name]梢[line]「私は……誰？」[%p]"
msgstr "16700:[name]梢[line]「我是……谁？」[%p]"

msgid "16800:[name]拓巳[line]「君は、こずぴぃだよ！」[%p]"
msgstr "16800:[name]拓巳[line]「你是小九P！」[%p]"

msgid "16900:[name]梢[line]「……！」[%p]"
msgstr "16900:[name]拓巳[line]「……！」[%p]"

msgid "17000:無数に分裂しかかっていた、鏡の中の自分の姿が。[linebreak]突然飛び込んできたその声によって、増殖を停止した。[%p]"
msgstr "17000:镜中无数分身的身影，开始分裂得更细了。[linebreak]因为这道突如其来的声音，让它们停止增殖。[%p]"

msgid "17100:[name]梢[line]「私は……こずぴぃ……」[%p]"
msgstr "17100:[name]梢[line]「我是……小九……」[%p]"

msgid "17200:[name]梢[line]「私は、こずぴぃ！」[%p]"
msgstr "17200:[name]梢[line]梢[%p]"

msgid "17300:アイデンティティが再確定される。[linebreak]梢は我に返り。[%p]"
msgstr "17300:我再次确认了自己的身份。[linebreak]梢回过神来。[%p]"

msgid "17400:目の前にそびえ立っていた巨大な鏡は砕け散った。[%p]"
msgstr "17400:耸立在眼前的巨镜碎裂四散了。[%p]"

msgid "17500:梢は、自分を救った声の主を探す。[%p]"
msgstr "17500:梢寻找着拯救自己的声音主人。[%p]"

msgid "17600:改札へと続く通路。[linebreak]そこに“彼”の姿を認めて。[%p]"
msgstr "17600:通往验票闸门的道路。[linebreak]在那里，她看到了「他」的身影。[%p]"

msgid "17700:失ったはずの――現実の――声で、[linebreak]梢は“彼”の名を叫んでいた。[%p]"
msgstr "17700:用理应已经失去——现实中的声音说：[linebreak]她喊出了「他」的名字。[%p]"

