# THIS FILE IS AUTOMATICALLY GENERATED BY generate_po.py.
                     
msgid "0:[name]梨深[line]「……あれは、空？」[%p]"
msgstr "0:[name]梨深[line]「……那是天空吗？」[%p]"

msgid "100:[name]梨深[line]「あの空は――」[%p]"
msgstr "100:[name]「……那是天空吗？」[line]「那片天空是——」[%p]"

msgid "200:[name]梨深[line]「どうして、あんなに泣きたくなるくらい青いのかな？」[%p]"
msgstr "200:[name]「那片天空是——」[line]「为什么，会蓝得让人想哭呢？」[%p]"

msgid "300:[name]タクミ[line]「それを望んだから」[%p]"
msgstr "300:[name]拓美[line]「因为我希望如此。」[%p]"

msgid "400:[name]梨深[line]「あたしは、望んでない……」[%p]"
msgstr "400:[name]梨深[line]「我并不想这样……」[%p]"

msgid "500:[name]タクミ[line]「僕が、望んだんだ」[%p]"
msgstr "500:[name]拓美[line]「是我自己想要这样的。」[%p]"

msgid "600:[name]タクミ[line]「ここは、君の心象世界であると同時に、僕の心象世界でもあるから」[%p]"
msgstr "600:[name]「是我自己想要这样的。」[line]「这里既是你的内心世界，同时也是我的内心世界。」[%p]"

msgid "700:梨深がこっちを振り向く。[linebreak]怪訝な顔で、見つめてくる。[%p]"
msgstr "700:梨深转头看向我这边。[linebreak]她一脸狐疑地盯着我看。[%p]"

msgid "800:[name]梨深[line]「あなた、誰？　ここには、あたし以外はいないはずなのに」[%p]"
msgstr "800:[name]梨深[line]梨深[%p]"

msgid "900:[name]梨深[line]「あなたは、別のあたし？」[%p]"
msgstr "900:[name]梨深[line]梨深[%p]"

msgid "1000:[name]タクミ[line]「違うよ」[%p]"
msgstr "1000:[name]拓美[line]「不是哦。」[%p]"

msgid "1100:[name]梨深[line]「だったら、誰？　どうやってここに来たの？」[%p]"
msgstr "1100:[name]梨深[line]「那到底是谁呢？又是怎么来到这里的？」[%p]"

msgid "1200:[name]梨深[line]「あたしの心の中だよ？　ずっと、暗闇しかなかった。光は、あたしに当たるスポットライトだけ」[%p]"
msgstr "1200:[name]梨深[line]「在我的心中哦！我一直都只有黑暗，光芒就只有照在我身上的聚光灯而已。」[%p]"

msgid "1300:[name]梨深[line]「それなのに、あなたが来た途端に、暗闇がなくなって、空ができた」[%p]"
msgstr "1300:[name]梨深[line]「明明是这样，但你一来之后，黑暗就消失了，天空出现了。」[%p]"

msgid "1400:[name]タクミ[line]「妄想が、シンクロしたんだ。君の力が、とても強かったから」[%p]"
msgstr "1400:[name]拓美[line]「因为你的力量非常强大，所以我们的妄想同步了。」[%p]"

msgid "1500:[name]梨深[line]「そう……」[%p]"
msgstr "1500:[name]梨深[line]「这样啊……」[%p]"

msgid "1600:梨深は、改めて空を見上げる。[linebreak]透き通るような、紺碧の、空。[%p]"
msgstr "1600:梨深再次抬头仰望天空。[linebreak]那是澄澈透明，蔚蓝的天空。[%p]"

msgid "1700:[name]梨深[line]「あたし、本物の空、見たことないんだ」[%p]"
msgstr "1700:[name]梨深[line]「我从没看过真正的天空。」[%p]"

msgid "1800:[name]梨深[line]「あたしっていう人格が生まれたときにはね、もう、暗い部屋に閉じこめられてた」[%p]"
msgstr "1800:[name]梨深[line]「在我这个人格诞生的时候，就已经被关在阴暗的房间里了。」[%p]"

msgid "1900:[name]梨深[line]「心の中も、外も、闇しかなかった」[%p]"
msgstr "1900:[name]梨深[line]梨深[%p]"

msgid "2000:[name]梨深[line]「そこからは、絶対に出してもらえないの」[%p]"
msgstr "2000:[name]梨深[line]梨深[%p]"

msgid "2100:[name]梨深[line]「…………」[%p]"
msgstr "2100:[name]梨深[line]「…………」[%p]"

msgid "2200:[name]梨深[line]「あたし、今、死のうとしてたんだ」[%p]"
msgstr "2200:[name]梨深[line]「我刚才，打算寻死。」[%p]"

msgid "2300:[name]梨深[line]「心を、自殺させるの」[%p]"
msgstr "2300:[name]梨深[line]梨深[%p]"

msgid "2400:[name]梨深[line]「きっと、これまでのあたしも、そうしてきたと思うし」[%p]"
msgstr "2400:[name]梨深[line]「我想，过去的我一定也是这么做的。」[%p]"

msgid "2500:[name]梨深[line]「でも、せっかくあなたが空を持ってきてくれたから」[%p]"
msgstr "2500:[name]梨深[line]「不过，难得你把天空带给我。」[%p]"

msgid "2600:[name]梨深[line]「この光景を、記憶に刻んでおいて、いいかな？　次のあたしが、またこうして想像できるように」[%p]"
msgstr "2600:[name]梨深[line]「可以让我把这幅景象烙印在记忆里吗？为了让下一个我，也能像这样想象。」[%p]"

msgid "2700:[name]タクミ[line]「死にたいの？」[%p]"
msgstr "2700:[name]拓美[line]「你想死吗？」[%p]"

msgid "2800:[name]梨深[line]「うん……」[%p]"
msgstr "2800:[name]「你想死吗？」[line]「嗯……」[%p]"

msgid "2900:[name]タクミ[line]「本当の空を、見てみたいとは思わないの？」[%p]"
msgstr "2900:[name]拓美[line]「你不想看看真正的天空吗？」[%p]"

msgid "3000:[name]梨深[line]「…………」[%p]"
msgstr "3000:[name]梨深[line]「…………」[%p]"

msgid "3100:[name]梨深[line]「……ねえ」[%p]"
msgstr "3100:[name]「…………」[line]「……呐。」[%p]"

msgid "3200:[name]梨深[line]「空って、本当にあるのかな？」[%p]"
msgstr "3200:[name]「……我问你哦。」[line]「真的有天空吗？」[%p]"

msgid "3300:[name]タクミ[line]「あるよ」[%p]"
msgstr "3300:[name]拓美[line]「有的。」[%p]"

msgid "3400:[name]タクミ[line]「でも、現実の空は、この空よりも、汚れてる」[%p]"
msgstr "3400:[name]「有啊。」[line]「可是，现实中的天空比这片天空还要污浊。」[%p]"

msgid "3500:[name]梨深[line]「どこにある？」[%p]"
msgstr "3500:[name]梨深[line]「在哪里呢？」[%p]"

msgid "3600:[name]梨深[line]「あの空は、どこにあるの？」[%p]"
msgstr "3600:[name]梨深[line]梨深[%p]"

msgid "3700:[name]タクミ[line]「君が、望めば、どこにでも」[%p]"
msgstr "3700:[name]拓美[line]「只要你希望，无论何处我都会去。」[%p]"

msgid "3800:[name]梨深[line]「望んで、いいの……？」[%p]"
msgstr "3800:[name]梨深[line]梨深[%p]"

msgid "3900:[name]タクミ[line]「もちろんだよ」[%p]"
msgstr "3900:[name]拓美[line]「当然可以。」[%p]"

msgid "4000:[name]梨深[line]「どうやって、望むの？」[%p]"
msgstr "4000:[name]梨深[line]「要怎么实现呢？」[%p]"

msgid "4100:[name]タクミ[line]「光は電磁場の波。その振動現象を人の視覚は色として捉えるんだ」[%p]"
msgstr "4100:[name]拓美[line]「光是电磁场的波动，而人类视觉会捕捉到这种振动现象并将其视为颜色。」[%p]"

msgid "4200:[name]タクミ[line]「自分のデッドスポットに、望む色を当てはめてごらん。そうすれば――」[%p]"
msgstr "4200:[name]拓美[line]「你试着把自己的死亡斑纹套用在你喜欢的颜色上，如此一来——」[%p]"

msgid "4300:[name]タクミ[line]「君の望む世界の色を、作り出せる」[%p]"
msgstr "4300:[name]拓美[line]「就能创造出你所期望的世界颜色。」[%p]"

msgid "4400:[name]タクミ[line]「世界は、君の望みに応えてくれるよ」[%p]"
msgstr "4400:[name]拓美[line]拓美[%p]"

msgid "4500:その出会いから、何日か、あるいは何ヶ月か経って。僕の病室に、梨深がやってきた。[%p]"
msgstr "4500:从那次相遇后，过了几天或几个月，梨深深到我的病房来。[%p]"

msgid "4600:僕と彼女は、同じ病院に入院していた者同士。[%p]"
msgstr "4600:我和她都是住同一间医院的人。[%p]"

msgid "4700:ギガロマニアックスという、誇るべきなのか、忌むべきなのか分からない、人智を超越した力の持ち主同士。[%p]"
msgstr "4700:我们两个都拥有超越人类智慧的力量，不知道该引以为傲还是感到可恨，叫做基加米罗马尼亚克斯的东西。[%p]"

msgid "4800:[name]七海[line]「おにぃ、お客さんだよ」[%p]"
msgstr "4800:[name]七海[line]「哥～有客人来了哦。」[%p]"

msgid "4900:[name]七海[line]「ねえねえ、誰？　カノジョさん？　いつの間に？」[%p]"
msgstr "4900:[name]七海[line]「欸，那是谁啊？女朋友吗？什么时候冒出来的？」[%p]"

msgid "5000:[name]七海[line]「やるねえ、おにぃってば」[%p]"
msgstr "5000:[name]七海[line]「哥，你真有一套。」[%p]"

msgid "5100:[name]タクミ[line]「…………」[%p]"
msgstr "5100:[name]拓美[line]「…………」[%p]"

msgid "5200:[name]梨深[line]「……また、会えたね」[%p]"
msgstr "5200:[name]梨深[line]「……我们又见面了呢。」[%p]"

msgid "5300:彼女は、とてもやつれていた。[%p]"
msgstr "5300:她看起来非常憔悴。[%p]"

msgid "5400:肌も病的なくらいに青白かったし。[linebreak]髪はパサパサで、伸び放題で。[linebreak]手の爪は割れていて。[%p]"
msgstr "5400:肌肤也苍白到不健康的程度。[linebreak]头发干枯毛躁，任其生长。[linebreak]指甲也裂开了。[%p]"

msgid "5500:常に、眩しそうに目を細めていた。[%p]"
msgstr "5500:眼睛总是像感到刺眼般眯细着。[%p]"

msgid "5600:[name]タクミ[line]「本物の空は、見られた？」[%p]"
msgstr "5600:[name]拓美——[line]「你有看到真正的天空吗？」[%p]"

msgid "5700:[name]梨深[line]「うん」[%p]"
msgstr "5700:[name]梨深——[line]「嗯。」[%p]"

msgid "5800:[name]七海[line]「あ、そだ」[%p]"
msgstr "5800:[name]七海——[line]「啊，对了。」[%p]"

msgid "5900:僕たちの様子を窺っていた七海が、なにかを思い出したかのように手を叩いた。[%p]"
msgstr "5900:一直看着我们的七海像是想起什么般拍了一下手。[%p]"

msgid "6000:[name]七海[line]「ナナ、お洗濯してこなくちゃ。[linebreak]梨深さん、でしたっけ？　どうぞごゆっくり〜」[%p]"
msgstr "6000:[name]七海——[line]七海——[linebreak]是叫……梨深同学吗？请慢用～」[%p]"

msgid "6100:七海が出ていく。[linebreak]それを僕と梨深は２人してぼんやりと見送った。[%p]"
msgstr "6100:七海离开了房间。[linebreak]我和梨深两人呆呆地目送她离开。[%p]"

msgid "6200:梨深が、僕に向き直る。[%p]"
msgstr "6200:梨深转向我这边说道：[%p]"

msgid "6300:[name]梨深[line]「タクミは、いつから入院してるの？」[%p]"
msgstr "6300:[name]梨深[line]「拓美，你是从什么时候开始住院的呢？」[%p]"

msgid "6400:[name]タクミ[line]「５年ぐらい」[%p]"
msgstr "6400:[name]拓美[line]「五年左右。」[%p]"

msgid "6500:[name]梨深[line]「退院の予定は？」[%p]"
msgstr "6500:[name]「五年左右。」[line]「预定什么时候出院呢？」[%p]"

msgid "6600:[name]タクミ[line]「ないよ。僕の余命は……あと、２年ぐらいだと思うし」[%p]"
msgstr "6600:[name]拓美[line]「没有哦，我的寿命大概只剩两年左右而已了。」[%p]"

msgid "6700:[name]梨深[line]「…………」[%p]"
msgstr "6700:[name]梨深[line]「…………」[%p]"

msgid "6800:[name]梨深[line]「あたしは、あなたのおかげで……頑張ることができた」[%p]"
msgstr "6800:[name]「…………」[line]「我啊，多亏有你……才能努力下去。」[%p]"

msgid "6900:[name]梨深[line]「終わりなんて、絶対にないと思ってた。でもあなたが教えてくれた空を見るまで、頑張ろうと思った」[%p]"
msgstr "6900:[name]「我啊，多亏有你……才能努力下去。」[line]「我一直觉得，绝对不会有结束的一天。但是在你教会我仰望天空之前，我都想努力下去。」[%p]"

msgid "7000:[name]梨深[line]「そしたら、急に、外に出してもらえた。用済みになった、って言った方が正しいかもしれないけど」[%p]"
msgstr "7000:[name]「我觉得，绝对没有结束的一天。但是，在你教会我仰望天空之前，我都想努力下去。」[line]「然后突然间就让我出来了。虽然说『已经没用了』可能比较正确吧。」[%p]"

msgid "7100:[name]タクミ[line]「そう」[%p]"
msgstr "7100:[name]拓美[line]「是吗？」[%p]"

msgid "7200:[name]梨深[line]「あたしは、あなたに救われたの」[%p]"
msgstr "7200:[name]梨深[line]「我被你拯救了。」[%p]"

msgid "7300:[name]梨深[line]「だから、今度はあたしが、あなたの力になりたい」[%p]"
msgstr "7300:[name]「我被你拯救了。」[line]「所以，这次我想成为你的力量。」[%p]"

msgid "7400:[name]タクミ[line]「そんなの、気にしなくていい」[%p]"
msgstr "7400:[name]拓美[line]「你不用在意那种事啦。」[%p]"

msgid "7500:[name]タクミ[line]「わざわざ、僕に会いに来なくてもいいよ」[%p]"
msgstr "7500:[name]拓美[line]「你不用特地跑来找我啦。」[%p]"

msgid "7600:[name]タクミ[line]「君には、これから普通に生きてほしい」[%p]"
msgstr "7600:[name]拓美[line]「我希望你能过着正常的生活。」[%p]"

msgid "7700:[name]梨深[line]「普通の生き方なんて……分からないから」[%p]"
msgstr "7700:[name]梨深[line]「我根本不知道……什么叫做正常生活啊！」[%p]"

