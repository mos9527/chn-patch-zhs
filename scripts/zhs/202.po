msgid "6000:[name]あやせ[line]「逃がさないわよ」[%p]"
msgstr "6000:[name]あやせ[line]「逃がさないわよ」[%p]"

msgid "6100:[name]三住[line]「はあ？　なになに、俺の相手してくれんの？」[%p]"
msgstr "6100:[name]三住[line]「はあ？　なになに、俺の相手してくれんの？」[%p]"

msgid "6200:[name]あやせ[line]「ええ。そうね」[%p]"
msgstr "6200:[name]あやせ[line]「ええ。そうね」[%p]"

