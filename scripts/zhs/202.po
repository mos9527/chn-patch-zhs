msgid "7100:[name]あやせ[line]「そいつを、殺すのよ」[%p]"
msgstr "7100:[name]あやせ[line]「そいつを、殺すのよ」[%p]"

msgid "7200:――そいつ――[%p]"
msgstr "7200:――そいつ――[%p]"

